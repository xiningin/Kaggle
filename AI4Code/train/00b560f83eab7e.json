 print(os.path.join(dirname, filename))\n\n","6363ef31":"matches = pd.read_csv(\"..\/input\/ipl-complete-dataset-20082020\/IPL Matches 2008-2020.csv\")\ndeliveries = pd.read_csv(\"..\/input\/ipl-complete-dataset-20082020\/IPL Ball-by-Ball 2008-2020.csv\")","6249ebc0":"team_names = matches.team1.unique()\nteam_names","479364d5":"team_names_abv = [\"RCB\",\"KXIP\",\"DD\",\"MI\",\"KKR\",\"RR\",\"DeCh\",\"CSK\",\"KTK\",\"PWI\",\"SH\",\"GL\",\"RPS\",\"RPS\",\"DC\"]\nmatches.replace(team_names,team_names_abv,inplace = True)","1dd7e4e4":"matches","9be62223":"matches.isnull().sum().plot(kind='bar')","7ca23b33":"matches = matches.fillna(value={'method':0})\n","302665a5":"matches[matches.result.isnull()]","065ba82a":"matches = matches[matches['result'].notnull()]\n","db25c3fd":"matches.loc[matches.result=='tie', 'result_margin']=0","a1fa407b":"matches.venue[matches.city.isnull()]","5123a945":"matches.city[matches.venue =='Dubai International Cricket Stadium'].unique()","1ec3e8be":"matches.city[matches.venue == 'Sharjah Cricket Stadium'].unique()","5b2ee354":"def venue_func(x):\n    if x == 'Dubai International Cricket Stadium':\n        return 'Dubai'\n    if x == 'Sharjah Cricket Stadium':\n        return 'Sharjah'\n    else :\n        return x\n    \nmatches.city = matches.venue.apply(venue_func)","97c6185a":"matches.isnull().sum()","a6aa7b3f":"matches.venue.value_counts()[:15].plot(kind = 'barh')\n","99b49942":"matches.loc[matches.method == 'D\/L',['venue']].value_counts()","92a3d507":"matches.toss_decision.value_counts().plot(kind = 'pie')","33694d3f":"\nplt.subplot(1,2,1)\nmatches.toss_winner.value_counts().plot(kind = 'bar')\nplt.subplot(1,2,2)\nmatches.toss_winner.value_counts().plot(kind = 'pie')","c4a2d9fe":"matches['season'] = matches['date'].str[:4].astype(int)\nmatches.season.value_counts().plot(kind = 'bar')","6279601f":"total_win = matches.winner.value_counts()\ntotal_win.plot(kind = 'bar')","64589195":"total_matches = (matches.team1.value_counts()+ matches.team2.value_counts())\ntotal_matches.plot(kind = 'bar')","fac162c5":"win_percentage = total_win\/total_matches*100\nwin_percentage.plot(kind = 'bar')","cd0d7d37":"matches.winner[matches.eliminator =='Y'].value_counts().plot(kind = 'pie')","1f444921":"matches.player_of_match.value_counts()[:10].plot(kind = 'bar')","a5b9745c":"deliveries.head()","64e62a5e":"deliveries.replace(team_names,team_names_abv,inplace = True)","67a060fa":"sns.countplot(x=deliveries.inning)","86ece216":"deliveries.info()","dd41637f":"deliveries.isnull().sum().plot(kind = 'barh')","5d7c5293":"deliveries['dismissal_kind'].value_counts()","5f57e6fc":"most_match_bats = deliveries.groupby(['batsman']).agg({'id': lambda x: len(set(x))}).sort_values(ascending = False,by = 'id')\nmost_match_bats[:15]","4f017a31":"bowl_match=deliveries.groupby(['bowler']).agg({'id': lambda x : len(set(x))}).sort_values(ascending = False,by = 'id')\nbowl_match[:15]","cb7cd469":"deliveries.bowler.value_counts()[:10].plot(kind = 'bar')","2d9fc709":"bats = deliveries.batsman.value_counts()\nbats[:15]","d95e5c3b":"runs = deliveries.groupby(['batsman']).batsman_runs.sum().sort_values(ascending=False)\nruns[:15]","16aeab79":"strike_rate = (runs\/bats[:100]*100).sort_values(ascending = False)\nstrike_rate[:15].plot(kind = 'barh')","fb4c5094":"out = deliveries.groupby(['batsman']).is_wicket.sum().sort_values(ascending = False)\nout[:15]","fc8529e6":"avg = (runs\/out[:240]).sort_values(ascending = False)\navg[:15]","d52ec378":"deliveries.batsman[deliveries.batsman_runs ==6].value_counts()[:15]","069dc03b":"deliveries.batsman[deliveries.batsman_runs ==4].value_counts()[:10]","c856ba73":"deliveries.batsman[deliveries.total_runs==7].value_counts()[:10]","c2c31b3d":"most_wickets = deliveries.groupby(['bowler']).is_wicket.sum().sort_values(ascending = False)\nmost_wickets[:15]","aadd5867":"(deliveries.groupby(['bowler']).extra_runs.sum()\/bowl_match['id']).sort_values(ascending = False)[:15]","da71d4a0":"deliveries.groupby(['id','batting_