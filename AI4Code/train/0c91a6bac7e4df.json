st which will contain the polarity of the comments\n\nfor i in comments['comment_text']:\n    polarity.append(TextBlob(i).sentiment.polarity)","108b6002":"comments['polarity']=polarity","49fc8a69":"comments.head(20)","454393ab":"comments_positive=comments[comments['polarity']==1]","b565608f":"comments_positive.shape","8e5f8eb9":"comments_positive.head()","2200ec3f":"#!pip install wordcloud","dbfd55d7":"from wordcloud import WordCloud,STOPWORDS","976dbd70":"stopwords=set(STOPWORDS)","90f1b360":"total_comments=' '.join(comments_positive['comment_text'])","418ecb05":"wordcloud=WordCloud(width=1000,height=500,stopwords=stopwords).generate(total_comments)","3e6d694d":"plt.figure(figsize=(15,5))\nplt.imshow(wordcloud)\nplt.axis('off')","7f0866fe":"comments_negative=comments[comments['polarity']==-1]","48a63288":"total_comments=' '.join(comments_negative['comment_text'])","b5cb63a1":"wordcloud=WordCloud(width=1000,height=500,stopwords=stopwords).generate(total_comments)","fb6bc183":"plt.figure(figsize=(15,5))\nplt.imshow(wordcloud)\nplt.axis('off')","fdf4d3b4":"videos=pd.read_csv('..\/input\/youtube\/USvideos.csv',error_bad_lines=False)","f22ffd4e":"videos.head()","0bd77661":"tags_complete=' '.join(videos['tags'])","447951fc":"tags_complete","8d5d7a05":"import re","73a7d3f5":"tags=re.sub('[^a-zA-Z]',' ',tags_complete)","b5a9808c":"tags","f6ca381d":"tags=re.sub(' +',' ',tags)","1b08db6b":"wordcloud=WordCloud(width=1000,height=500,stopwords=set(STOPWORDS)).generate(tags)","281d4a3f":"plt.figure(figsize=(15,5))\nplt.imshow(wordcloud)\nplt.axis('off')","701378ec":"sns.regplot(data=videos,x='views',y='likes')\nplt.title('Regression plot for views & likes')","344fe3f4":"sns.regplot(data=videos,x='views',y='dislikes')\nplt.title('Regression plot for views & dislikes')","a7d36e42":"df_corr=videos[['views','likes','dislikes']]","34117524":"df_corr.corr()","e18e4ff4":"sns.heatmap(df_corr.corr(),annot=True)","07b37179":"comments.head()","145e2383":"comments['comment_text'][1]","85c5d