{"cell_type":{"f6a76cde":"code","a904b4e4":"code","c3493961":"code","c199ac64":"code","acf59b18":"code","925f8e8e":"code","9011888c":"code","e8fa8e1b":"code","ef5c2346":"code","db190c57":"code","538f522e":"code","0e99ba4b":"code","ae2924c0":"code","8bea8128":"code","babf004f":"markdown","86a510fd":"markdown","56661a76":"markdown","ea9dd159":"markdown","fec27deb":"markdown","9be5b7b2":"markdown","64c349ad":"markdown","fc94c7a7":"markdown","66862d7c":"markdown","f5e8bd1e":"markdown","97e598e8":"markdown","43146dad":"markdown","784261ed":"markdown","12e7610b":"markdown","3b788ae4":"markdown","332611b1":"markdown","3965bd53":"markdown","46a49061":"markdown","c9d13934":"markdown","770efbee":"markdown"},"source":{"f6a76cde":"import pandas as pd\nimport numpy as np\n\nimport matplotlib.pyplot as plt\nimport seaborn as sns\nplt.style.use('ggplot')\n\nimport warnings\nwarnings.filterwarnings('ignore')","a904b4e4":"data = pd.read_csv('..\/input\/mrr-by-alimbekov\/mrr.csv',\n                   parse_dates=['first_order', 'EndDate'])","c3493961":"data.head()","c199ac64":"data.isna().sum()","acf59b18":"print(f'\u041e\u0431\u0449\u0435\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0437\u0430\u043f\u0438\u0441\u0435\u0439 - {len(data)}; \u041a\u043e\u043b-\u0432\u043e \u0443\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u044b\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 - {data[\"customer_id\"].nunique()}')","925f8e8e":"plt.figure(figsize = (12, 6))\n\ndata['first_order'].dt.day.value_counts()\\\n                          .sort_index()\\\n                          .plot()\n\nplt.title('Purchases distribution')\nplt.xlabel('Number of purchases')\nplt.ylabel('Days');","9011888c":"data['duration'] = ((data['EndDate'] - data['first_order']).dt.days).astype(int)\n\ndata['duration'].value_counts()","e8fa8e1b":"data['rate'].value_counts()","ef5c2346":"rate_grouped = data.groupby('rate')\\\n                   .agg(ClientsCount = ('customer_id', 'count'),\n                        DurationDays = ('duration', 'mean'),\n                        Revenue = ('Amount', 'sum'))\n\nrate_grouped['RevenueNormed'] = (rate_grouped['Revenue']\/rate_grouped['ClientsCount'])\/(rate_grouped['DurationDays']\/30)\nrate_grouped","db190c57":"data['commission'].value_counts()","538f522e":"start_days = data['first_order'].dt.to_period('D').dt.to_timestamp()\nstart_months = data['first_order'].dt.to_period('M').dt.to_timestamp()\n\nend_days = data['EndDate'].dt.to_period('D').dt.to_timestamp()\nend_months = data['EndDate'].dt.to_period('M').dt.to_timestamp()\n\n\nresults_days = []\nresults_months = []\n\nfor i in range(len(start_days)):\n    results_days += pd.date_range(start_days[i], end_days[i]-pd.to_timedelta(1, unit='d'), freq='d').astype(str).tolist()\n    results_months += pd.date_range(start_months[i], end_months[i], freq='M').to_period('M').astype(str).tolist()","0e99ba4b":"from collections import Counter\n\ncounts_days = Counter(results_days)\ncounts_months = Counter(results_months)\n\nmrr_table = pd.DataFrame(zip(counts_days.keys(), counts_days.values()),\n                         columns = ['Dates', 'SubscriptionCounts'])\nmrr_table['Dates'] = mrr_table['Dates'].apply(lambda x: x[:7])\nmrr_table['MRR_Daily'] = mrr_table['SubscriptionCounts'] * (75\/30) * 0.95\nmrr_table = mrr_table.groupby('Dates')['MRR_Daily']\\\n                     .sum()\\\n                     .reset_index()\n\ntemp = pd.DataFrame(zip(counts_months.keys(), counts_months.values()),\n                    columns = ['Dates', 'Counts'])\ntemp['MRR_Monthly'] = temp['Counts'] * 75 * 0.95\n\nmrr_table = mrr_table.set_index('Dates')\\\n                     .join(temp.set_index('Dates'))\n\nmrr_table.head()","ae2924c0":"mrr_table['MRR_Daily'].sum() == mrr_table['MRR_Monthly'].sum() == data['Amount'].sum()*0.95","8bea8128":"plt.rcParams[\"figure.figsize\"] = (16, 10)\n\nmrr_table[['MRR_Daily', 'MRR_Monthly']].plot()\nplt.axvline(22, c='darkorange', label='Last Sale date', linestyle='-.')\nplt.title('MRR - 2 approaches')\nplt.ylabel('Revenue')\nplt.legend();","babf004f":"\u0422\u043e \u0435\u0441\u0442\u044c \u0432 \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0443\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u044b\u0435 \u043a\u043b\u0438\u0435\u043d\u0442\u044b. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043d\u0435 \u043f\u0440\u0438\u0434\u0435\u0442\u0441\u044f \u0430\u043d\u0430\u043b\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043a\u043e\u043d\u0432\u0435\u0440\u0441\u0438\u044e \u043c\u0435\u0436\u0434\u0443 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f\u043c\u0438 \u043f\u043e\u0434\u043f\u0438\u0441\u043e\u043a. \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u0435\u0441\u043b\u0438 \u0431\u044b \u0443 \u043d\u0430\u0441 \u0431\u044b\u043b\u0438 \u0431\u044b \u043f\u043e\u0432\u0442\u043e\u0440\u0435\u043d\u0438\u044f $\\text{customer_id}$, \u0442\u043e \u043c\u043e\u0436\u043d\u043e \u0431\u044b\u043b\u043e \u0431\u044b \u0430\u043d\u0430\u043b\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043a\u0430\u043a\u0430\u044f \u0447\u0430\u0441\u0442\u044c \u043c\u0435\u0441\u044f\u0447\u043d\u044b\u0445 \u043f\u043e\u0434\u043f\u0438\u0441\u043e\u043a \u043a\u043e\u043d\u0432\u0435\u0440\u0442\u0438\u0440\u0443\u044e\u0442\u0441\u044f \u043d\u0430 \u0431\u043e\u043b\u0435\u0435 \u0434\u043e\u043b\u0433\u043e\u0441\u0440\u043e\u0447\u043d\u044b\u0435 \u0432\u0438\u0434\u044b \u043f\u043e\u0434\u043f\u0438\u0441\u043e\u043a \u0438 \u043d\u0430\u043e\u0431\u043e\u0440\u043e\u0442","86a510fd":"\u041a\u043e\u043c\u043c\u0438\u0441\u0441\u0438\u044f \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 5% \u0434\u043b\u044f \u0432\u0441\u0435\u0445 \u043f\u0440\u043e\u0434\u0430\u0436. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043c\u043e\u0436\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u0434\u043e\u0445\u043e\u0434 \u0443\u043c\u043d\u043e\u0436\u0438\u0442\u044c \u043d\u0430 0.95 \u0438 \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u0447\u0438\u0441\u0442\u0443\u044e \u043f\u0440\u0438\u0431\u044b\u043b\u044c\u044d","56661a76":"## rate","ea9dd159":"\u0412 \u0446\u0435\u043b\u043e\u043c \u0433\u0440\u0443\u043f\u043f\u044b \u0441\u0431\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b \u0438 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u043f\u043e\u0434\u043f\u0438\u0441\u043e\u043a \u0438\u043c\u0435\u044e\u0442 \u043f\u043e\u0447\u0442\u0438 \u043e\u0434\u0438\u043d\u0430\u043a\u043e\u0432\u043e\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432.\n\n\u0422\u0435\u043f\u0435\u0440\u044c \u043f\u043e\u043f\u0440\u043e\u0431\u0443\u0435\u043c \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c \u043d\u0430 \u0433\u0440\u0443\u043f\u043f\u044b \u0432 \u0440\u0430\u0437\u0440\u0435\u0437\u0435 \u0434\u043e\u0445\u043e\u0434\u043e\u0432 ","fec27deb":"## customer_id","9be5b7b2":"\u041f\u043e\u0445\u043e\u0436\u0435, \u0447\u0442\u043e \u043f\u043e\u0434 \u043c\u0435\u0441\u044f\u0447\u043d\u044b\u043c\u0438 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0430\u043c\u0438 \u043f\u043e\u0434\u0440\u0430\u0437\u0443\u043c\u0435\u0432\u0430\u0435\u0442\u0441\u044f 2 \u043c\u0435\u0441\u044f\u0446\u0430 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0438\n\n\u0414\u0430\u043d\u043d\u0430\u044f \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u0430 \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u043e\u0432\u0435\u0440\u0435\u043d\u0430 \u043d\u0438\u0436\u0435","64c349ad":"# \u0412\u0438\u0437\u0443\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044f","fc94c7a7":"\u041f\u0440\u043e\u0432\u0435\u0440\u043a\u0430 \u0441\u0435\u0437\u043e\u043d\u043d\u043e\u0441\u0442\u0438 \u043f\u043e \u0434\u043d\u044f\u043c","66862d7c":"## dates","f5e8bd1e":"\u0417\u0430\u0434\u0430\u0447\u0430 - \u043f\u043e\u0441\u0447\u0438\u0442\u0430\u0442\u044c MRR. \n\n\u0417\u0430\u0434\u0430\u0447\u0430 by Renat Alimbekov (https:\/\/alimbekov.com\/)","97e598e8":"* \u0421\u0438\u043d\u044f\u044f \u043b\u0438\u043d\u0438\u044f - MRR \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u0430\u043d\u043d\u044b\u0439 \u043f\u043e \u043c\u0435\u0441\u044f\u0446\u0430\u043c\n* \u041a\u0440\u0430\u0441\u043d\u0430\u044f \u043b\u0438\u043d\u0438\u044f - MRR \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u0430\u043d\u043d\u044b\u0439 \u043f\u043e \u0434\u043d\u044f\u0438\n* \u041e\u0440\u0430\u043d\u0436\u0435\u0432\u0430\u044f \u043b\u0438\u043d\u0438\u044f - \u0434\u0430\u0442\u0430 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0435\u0439 \u043f\u0440\u043e\u0434\u0430\u0436\u0438\n\nMRR \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u0430\u043d\u043d\u044b\u0439 \u043f\u043e \u0434\u043d\u044f\u043c \u0433\u043e\u0440\u0430\u0437\u0434\u043e \u0442\u043e\u0447\u043d\u0435\u0435 \u0438 \u043c\u043e\u0436\u043d\u043e \u0434\u0430\u0436\u0435 \u0443\u0432\u0438\u0434\u0435\u0442\u044c \u0432 \u043a\u0430\u043a\u0438\u0445 \u043c\u0435\u0441\u044f\u0446\u0430\u0445 \u0431\u044b\u043b\u0438 \u043f\u0440\u043e\u0441\u0435\u0434\u0430\u043d\u0438\u0435 \u0433\u0440\u0430\u0444\u0438\u043a\u0430. \u0410 \u044d\u0442\u043e\u0442 \u0436\u0435 \u043f\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u0430\u043d\u043d\u044b\u0439 \u043f\u043e \u043c\u0435\u0441\u044f\u0446\u0430\u043c \u0431\u043e\u043b\u0435\u0435 \u0433\u043b\u0430\u0434\u043a\u0438\u0439, \u043d\u043e \u043f\u0435\u0440\u0435\u0434\u0430\u0435\u0442 \u0434\u0438\u043d\u0430\u043c\u0438\u043a\u0443 \u043f\u043e\u0447\u0442\u0438 \u0442\u0430\u043a \u0436\u0435 \u0442\u043e\u0447\u043d\u043e \u043a\u0430\u043a \u0438 \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u0430\u043d\u043d\u044b\u0439 \u043f\u043e \u0434\u043d\u044f\u043c. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u0435\u0441\u043b\u0438 \u0434\u0430\u043d\u043d\u044b\u0445 \u043d\u0435 \u043c\u043d\u043e\u0433\u043e, \u0442\u043e \u043c\u043e\u0436\u043d\u043e \u043f\u043e \u0434\u043d\u044f\u043c \u0441\u0447\u0438\u0442\u0430\u0442\u044c, \u0430 \u0432 \u043e\u0431\u0440\u0430\u0442\u043d\u043e\u043c \u0441\u043b\u0443\u0447\u0430\u0435 \u043f\u043e \u043c\u0435\u0441\u044f\u0446\u0430\u043c \u0433\u043e\u0440\u0430\u0437\u0434\u043e \u0432\u044b\u0433\u043e\u0434\u043d\u0435\u0435 \u0431\u0443\u0434\u0435\u0442\n\n\u0412 \u0446\u0435\u043b\u043e\u043c \u0434\u0438\u043d\u0430\u043c\u0438\u043a\u0430 \u043e\u0447\u0435\u043d\u044c \u0445\u043e\u0440\u043e\u0448\u0430\u044f \u0438 \u043c\u043e\u0436\u043d\u043e \u0443\u0432\u0438\u0434\u0435\u0442\u044c \u043a\u0430\u043a \u043f\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u043b \u0440\u043e\u0441\u0442 \u043a\u0430\u0436\u0434\u044b\u0439 \u043c\u0435\u0441\u044f\u0446. \u041d\u043e \u0441\u0442\u043e\u0438\u0442 \u043e\u0442\u043c\u0435\u0442\u0438\u0442\u044c, \u0447\u0442\u043e \u0432 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0435 \u043c\u0435\u0441\u044f\u0446\u044b \u0435\u0441\u0442\u044c \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0441\u0442\u0430\u0433\u043d\u0430\u0446\u0438\u044f. \u042d\u0442\u043e \u043c\u043e\u0436\u0435\u0442 \u043e\u0437\u043d\u0430\u0447\u0430\u0442\u044c, \u0447\u0442\u043e \u0440\u043e\u0441\u0442 \u043e\u0441\u0442\u0430\u043d\u043e\u0432\u0438\u043b\u0441\u044f \u0438 \u043d\u0443\u0436\u043d\u043e \u0440\u0435\u0448\u0430\u0442\u044c \u0447\u0442\u043e-\u0442\u043e \u0441 \u044d\u0442\u0438\u043c \u0432\u043e\u043f\u0440\u043e\u0441\u043e\u043c.","43146dad":"1-2 \u043f\u0443\u043d\u043a\u0442\u044b","784261ed":"# Importing and Data Loading","12e7610b":"# \u0412\u0430\u043b\u0438\u0434\u0430\u0446\u0438\u044f","3b788ae4":"## \u0421\u0442\u0440\u0430\u0442\u0435\u0433\u0438\u044f\n\n\u041f\u043e \u043c\u0435\u0441\u044f\u0446\u0430\u043c \u0431\u0443\u0434\u0435\u043c \u0441\u0447\u0438\u0442\u0430\u0442\u044c \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0430\u043a\u0442\u0438\u0432\u043d\u044b\u0445 \u043f\u043e\u0434\u043f\u0438\u0441\u043e\u043a. \u041d\u043e \u0442\u0443\u0442 \u0435\u0441\u0442\u044c 2 \u043f\u043e\u0434\u0445\u043e\u0434\u0430: (1) \u0442\u0430\u043a \u043a\u0430\u043a \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0438 \u043f\u043e\u043a\u0443\u043f\u0430\u043b\u0438\u0441\u044c, \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, 25-\u0433\u043e \u0447\u0438\u0441\u043b\u0430, \u0442\u043e \u043c\u043e\u0436\u043d\u043e \u0434\u043e\u0445\u043e\u0434 \u043d\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043f\u043e \u0434\u043d\u044f\u043c \u0438 5 \u0434\u043d\u0435\u0439 \u0434\u043e\u0445\u043e\u0434\u0430 \u043f\u0440\u0438\u043f\u043b\u044e\u0441\u043e\u0432\u0430\u0442\u044c \u0442\u0435\u043a\u0443\u0449\u0435\u043c\u0443 \u043c\u0435\u0441\u044f\u0446\u0443, \u0430 \u043e\u0441\u0442\u0430\u043b\u044c\u043d\u044b\u0435 25 \u0434\u043d\u0435\u0439 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0438 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u043c\u0443 \u043c\u0435\u0441\u044f\u0446\u0443; (2) \u043d\u0435 \u043f\u0430\u0440\u0438\u0442\u044c\u0441\u044f \u043d\u0430\u0441\u0447\u0435\u0442 \u0442\u0430\u043a\u0438\u0445 \u0441\u043b\u0443\u0447\u0430\u0435\u0432 \u0438 \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u043e \u043c\u0435\u0441\u044f\u0446\u0430\u043c \u0441\u0447\u0438\u0442\u0430\u0442\u044c\n\n\u042f \u0440\u0435\u0448\u0438\u043b \u043e\u0431\u0430 \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u0430 \u043f\u043e\u043f\u0440\u043e\u0431\u043e\u0432\u0430\u0442\u044c \u0438 \u0435\u0449\u0435 \u043c\u0435\u0436\u0434\u0443 \u0441\u043e\u0431\u043e\u0439 \u0441\u0440\u0430\u0432\u043d\u0438\u0442\u044c. \u0412 \u043b\u044e\u0431\u043e\u043c \u0441\u043b\u0443\u0447\u0430\u0435 \u043f\u043e\u0434\u0445\u043e\u0434 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0439:\n1. \u041f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u0434\u0430\u0442\u044b \u043c\u0435\u0436\u0434\u0443 \u0441\u0442\u0430\u0440\u0442\u043e\u043c \u0438 \u043e\u043a\u043e\u043d\u0447\u0430\u043d\u0438\u0435\u043c \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0438. \u0418\u043d\u0442\u0435\u0432\u0430\u043b\u044b \u0432 \u0434\u043d\u044f\u0445 \u0438\u043b\u0438 \u0432 \u043c\u0435\u0441\u044f\u0446\u0430\u0445\n2. \u041f\u043e\u0441\u0447\u0438\u0442\u0430\u0442\u044c \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0440\u0430\u0437 \u0432\u0441\u0442\u0440\u0435\u0447\u0430\u0435\u0442\u0441\u044f \u043a\u0430\u0436\u0434\u0430\u044f \u0434\u0430\u0442\u0430 \u0432 \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0435 - \u0442\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u043e\u043b\u0443\u0447\u0438\u043c \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043f\u043e\u0434\u043f\u0438\u0441\u043e\u043a \u043e\u0431\u0441\u043b\u0443\u0436\u0438\u0432\u0430\u043b\u043e\u0441\u044c \u0432 \u043b\u044e\u0431\u043e\u0439 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u044b\u0439 \u0434\u0435\u043d\u044c\/\u043c\u0435\u0441\u044f\u0446\n3. \u0418\u0437 EDA \u043c\u044b \u0437\u043d\u0430\u0435\u043c, \u0447\u0442\u043e \u043c\u0435\u0441\u044f\u0447\u043d\u0430\u044f \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0430 \u0441\u0442\u043e\u0438\u0442 __75 \u0443.\u0435.__ \u0438\u043b\u0438 __2.5 \u0443.\u0435.__ \u0432 \u0434\u0435\u043d\u044c. \u041c\u044b \u043c\u043e\u0436\u0435\u043c \u043f\u0440\u043e\u0441\u0442\u043e \u043d\u0430 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043e\u0431\u0441\u043b\u0443\u0436\u0438\u0432\u0430\u0435\u043c\u044b\u0445 \u043f\u043e\u0434\u043f\u0438\u0441\u043e\u043a \u0443\u043c\u043d\u043e\u0436\u0438\u0442\u044c \u0438 \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c MRR. \u0422\u0430\u043a\u0436\u0435 \u0443\u043c\u043d\u043e\u0436\u0430\u0435\u043c \u0432\u0441\u0435 \u0447\u0442\u043e \u0441\u0432\u044f\u0437\u0430\u043d\u043e \u0441 \u0434\u043e\u0445\u043e\u0434\u0430\u043c\u0438 \u043d\u0430 0.95, \u0447\u0442\u043e\u0431\u044b \u0443\u0447\u0438\u0442\u044b\u0432\u0430\u0442\u044c \u043a\u043e\u043c\u043c\u0438\u0441\u0441\u0438\u0438 \u043f\u043b\u0430\u0442\u0435\u0436\u043d\u044b\u0445 \u0441\u0438\u0441\u0442\u0435\u043c\n4. \u0410 \u043f\u0440\u043e\u0432\u0430\u043b\u0438\u0434\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b \u043c\u043e\u0436\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u0441\u0440\u0430\u0432\u043d\u0438\u0432 \u043e\u0434\u0438\u043d\u0430\u043a\u043e\u0432\u043e\u0441\u0442\u044c \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0445 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0438: \u0441\u0443\u043c\u043c\u044b \u0432\u0441\u0435\u0445 MRR \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u0430\u043d\u043d\u044b\u0445 \u043f\u043e \u0434\u043d\u044f\u043c, \u0441\u0443\u043c\u043c\u044b \u0432\u0441\u0435\u0445 MRR \u0440\u0430\u0441\u0441\u0447\u0438\u0442\u0430\u043d\u043d\u044b\u0445 \u043f\u043e \u043c\u0435\u0441\u044f\u0446\u0430\u0445, \u0441\u0443\u043c\u043c\u044b \u0432\u0441\u0435\u0445 __Amount__ \u043f\u043e \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0443","332611b1":"# MRR","3965bd53":"\u041d\u0443 \u043a\u0430\u0436\u0435\u0442\u0441\u044f, \u0447\u0442\u043e \u0432 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u043c \u043f\u0440\u043e\u0434\u0430\u0436\u0438 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u044f\u0442 \u043f\u043e\u0441\u043b\u0435 \u043f\u0435\u0440\u0432\u043e\u0439 \u043d\u0435\u0434\u0435\u043b\u0438 \u043c\u0435\u0441\u044f\u0446\u0430. \u041c\u044b \u043c\u043e\u0436\u0435\u043c \u0437\u0430\u043c\u0435\u0442\u0438\u0442\u044c \u043a\u0430\u043a \u043a\u043e\u043b-\u0432\u043e \u043f\u0440\u043e\u0434\u0430\u0436 \u0440\u0435\u0437\u043a\u043e \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u043f\u043e\u0441\u043b\u0435 7 \u0434\u043d\u0435\u0439.","46a49061":"3 \u043f\u0443\u043d\u043a\u0442","c9d13934":"* __ClientsCount__ - \u043a\u043e\u043b-\u0432\u043e \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432\n* __Revenue__ - \u043e\u0431\u0449\u0438\u0439 \u0434\u043e\u0445\u043e\u0434\n* __RevenueNormed__ - \u043d\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439 \u0434\u043e\u0445\u043e\u0434 = \u0441\u0440\u0435\u0434\u043d\u0438\u0439 \u0434\u043e\u0445\u043e\u0434 \u0441 \u043a\u043b\u0438\u0435\u043d\u0442\u0430 \u043d\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439 \u043d\u0430 \u043a\u043e\u043b-\u0432\u043e \u043c\u0435\u0441\u044f\u0446\u0435\u0432 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0438\n\n\u0415\u0441\u043b\u0438 \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c \u043d\u0430 \u043d\u043e\u0440\u043c\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439 \u0434\u043e\u0445\u043e\u0434, \u0442\u043e \u043c\u043e\u0436\u043d\u043e \u0437\u0430\u043c\u0435\u0442\u0438\u0442\u044c, \u0447\u0442\u043e \u043e\u043d\u0430 \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 75 \u0443.\u0435. \u0432 \u043d\u0435\u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u0442\u0438\u043f\u0430 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0438. \u042d\u0442\u043e \u043e\u0437\u043d\u0430\u0447\u0430\u0435\u0442, \u0447\u0442\u043e \u043a\u043b\u0438\u0435\u043d\u0442\u044b \u043d\u0435 \u0432\u0438\u0434\u044f\u0442 \u0440\u0430\u0437\u043d\u0438\u0446\u044b \u043c\u0435\u0436\u0434\u0443 \u043c\u0435\u0441\u044f\u0447\u043d\u043e\u0439 \u0438 \u043f\u043e\u043b\u0443\u0433\u043e\u0434\u043e\u0432\u043e\u0439\/\u0433\u043e\u0434\u043e\u0432\u043e\u0439 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u043e\u0439. \u0410 \u044d\u0442\u043e \u0432 \u0441\u0432\u043e\u044e \u043e\u0447\u0435\u0440\u0435\u0434\u044c \u043c\u043e\u0436\u0435\u0442 \u0441\u043b\u0443\u0436\u0438\u0442 \u043f\u0440\u0438\u0447\u0438\u043d\u043e\u0439 \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u043a\u043b\u0438\u0435\u043d\u0442\u044b \u043d\u0435 \u0445\u043e\u0442\u044f\u0442 \u043f\u0435\u0440\u0435\u0445\u043e\u0434\u0438\u0442\u044c \u0441 \u043c\u0435\u0441\u044f\u0447\u043d\u043e\u0439 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0438 \u043d\u0430 \u043f\u043e\u043b\u0443\u0433\u043e\u0434\u043e\u0432\u044b\u0435\/\u0433\u043e\u0434\u043e\u0432\u044b\u0435. \u0415\u0441\u043b\u0438 \u043e\u043d\u0438 \u043f\u0435\u0440\u0435\u0439\u0434\u0443\u0442 \u043d\u0430 \u0434\u043e\u043b\u0433\u043e\u0441\u0440\u043e\u0447\u043d\u0443\u044e \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0443, \u0442\u043e \u043e\u043d\u0438 \u0431\u0443\u0434\u0443\u0442 \u0442\u043e\u043b\u044c\u043a\u043e \u0432 \u043f\u0440\u043e\u0438\u0433\u0440\u044b\u0448\u0435, \u0442\u0430\u043a \u043a\u0430\u043a \u0432\u044b\u0433\u043e\u0434\u044b \u0432 \u043f\u043b\u0430\u043d\u0435 \u0446\u0435\u043d\u044b \u043d\u0435\u0442, \u0430 \u0433\u0438\u0431\u043a\u043e\u0441\u0442\u044c \u0442\u0435\u0440\u044f\u0435\u0442\u0441\u044f (\u043e\u043d\u0438 \u043d\u0435 \u043c\u043e\u0433\u0443\u0442 \u043f\u0440\u0438 \u0436\u0435\u043b\u0430\u043d\u0438\u0438 \u0443\u0439\u0442\u0438 \u043d\u0430 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0439 \u043c\u0435\u0441\u044f\u0446 \u043f\u0440\u0438 \u043d\u0430\u043b\u0438\u0447\u0438\u0438 \u0434\u043e\u043b\u0433\u043e\u0441\u0440\u043e\u0447\u043d\u043e\u0439 \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0438). \u0422\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c, \u0431\u0438\u0437\u043d\u0435\u0441 \u0442\u0435\u0440\u044f\u0435\u0442 \u043d\u0430\u0434\u0435\u0436\u043d\u043e\u0441\u0442\u044c \u0432 \u043f\u043b\u0430\u043d\u0435 revenue generation, \u043f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e \u0432\u043e\u043b\u0430\u0442\u0438\u043b\u044c\u043d\u044b\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u043c\u043d\u043e\u0433\u043e. \n\n\u041a\u0430\u043a \u0430\u043d\u0430\u043b\u0438\u0442\u0438\u043a, \u044f \u0431 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0438\u043b \u0441\u043d\u0438\u0437\u0438\u0442\u044c \u0446\u0435\u043d\u0443 \u0438\u043b\u0438 \u0435\u0449\u0435 \u043a\u0430\u043a\u0438\u043c-\u0442\u043e \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u043c\u043e\u0442\u0438\u0432\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u043f\u0435\u0440\u0435\u0445\u043e\u0434\u0438\u0442\u044c \u043d\u0430 \u0434\u043e\u043b\u0433\u043e\u0441\u0440\u043e\u0447\u043d\u0443\u044e \u043f\u043e\u0434\u043f\u0438\u0441\u043a\u0443. \u041f\u043e \u043c\u043e\u0435\u043c\u0443 \u043c\u043d\u0435\u043d\u0438\u044e, \u0434\u0430\u043d\u043d\u043e\u0435 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u043f\u043e\u043c\u043e\u0436\u0435\u0442 \u043f\u043e\u043b\u0443\u0447\u0430\u0442\u044c \u0431\u043e\u043b\u0435\u0435 \u0441\u0442\u0430\u0431\u0438\u043b\u044c\u043d\u044b\u0439 \u0434\u043e\u0445\u043e\u0434 \u0438 \u0437\u0430\u0440\u0443\u0447\u0438\u0442\u044c\u0441\u044f \u0434\u043e\u0432\u0435\u0440\u0438\u0435\u043c \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432, \u0442\u0435\u043c \u0441\u0430\u043c\u044b\u043c \u0443\u0432\u0435\u043b\u0438\u0447\u0438\u0442\u044c \u043a\u043e\u043b-\u0432\u043e \u043b\u043e\u044f\u043b\u044c\u043d\u044b\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432","770efbee":"# EDA"}}