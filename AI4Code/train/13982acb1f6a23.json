{"cell_type":{"ef1f4088":"code","93ba2c30":"code","f2ed6b16":"code","5afb2515":"code","478e5262":"code","ba80d144":"code","af9cd3d7":"code","fc56d43d":"code","44473665":"code","e629be32":"code","7698f6a4":"code","516217fd":"code","7a7bbc11":"code","1d963322":"code","41ca09c7":"code","d712e2bc":"code","c4386374":"code","6e19884e":"code","18b3ae25":"code","284bb917":"code","96f5de1a":"code","1562dfcc":"code","a92a9477":"code","c1002f1a":"code","a7e6038b":"code","b9c4ca9d":"markdown","54c4e2f4":"markdown","c75460b7":"markdown","cfbac3a8":"markdown","dddced6c":"markdown","5cb2dfc3":"markdown","f3e9dbb0":"markdown","83c708d2":"markdown","d1388704":"markdown","ab717ea3":"markdown","c2336eb9":"markdown","87b05f17":"markdown","5dc0eb71":"markdown","b5e65ce1":"markdown"},"source":{"ef1f4088":"import pandas as pd\nimport numpy as np\n\n# \uc0ac\uc774\ud0b7\ub7ec\ub2dd \ub0b4\ubd80 \ub370\uc774\ud130\uc14b \ub85c\ub4dc\nfrom sklearn.datasets import load_iris    # load (\ub370\uc774\ud130\uc14b \ubd88\ub7ec\uc624\ub294 \ubaa8\ub4c8), iris (\ub2e4\uc911\ubd84\ub958\uc758 \ub300\ud45c\uc801\uc778 \ub370\uc774\ud130\uc14b)\n\n# \uc54c\uace0\ub9ac\uc998 \ubaa8\ub378 \ubd88\ub7ec\uc624\uae30(\uc758\uc0ac\uacb0\uc815\ub098\ubb34: DecisionTree, \ub2e4\uc911\ubd84\ub958 \ubaa8\ub4c8: Classifier)\nfrom sklearn.tree import DecisionTreeClassifier\n\n# Train set \uacfc Test set \uc744 \ubd84\ud560\ud574 \uc8fc\ub294 model_selection \uc548\uc758 train_test_split \uc77d\uc5b4\uc624\uae30 (\ud640\ub4dc\uc544\uc6c3 \ud14c\uc2a4\ud2b8 \uc704\ud574)\nfrom sklearn.model_selection import train_test_split","93ba2c30":"# \uc0ac\uc774\ud0b7\ub7f0 \uc790\uccb4\ub370\uc774\ud130\uc14b\uc758 iris \ub370\uc774\ud130\uc14b \ub85c\ub529\niris = load_iris()","f2ed6b16":"iris   # \ub515\uc154\ub108\ub9ac \ud615\ud0dc, Key \uc640 Value \ub294 Numpy Array \ub85c \ub418\uc5b4\uc788\uc74c","5afb2515":"iris['data']   # \uc2e4\uc81c \uc785\ub825 \ub370\uc774\ud130\uac00 \uc800\uc7a5\ub41c \uacf3","478e5262":"iris['target']   # \uacb0\uacfc\uac12\uc774 \ub4e4\uc5b4\uc788\ub294\uacf3","ba80d144":"iris['feature_names']   # \ub370\uc774\ud130 \uc5f4\uc758 \uc815\ubcf4\uac00 \ub4e4\uc5b4 \uc788\ub294\uacf3","af9cd3d7":"iris_df = pd.DataFrame(data=iris['data'], columns=iris['feature_names'])\niris_df","fc56d43d":"# target \ubcc0\uc218 \ucd94\uac00 (\uc5f4\uc774\ub984 'label' \ub85c \uc9c0\uc815)\niris_df['label'] = iris['target']\niris_df.head()","44473665":"# \uc0ac\uc774\ud0b7\ub7f0\uc740 \ub370\uc774\ud130\ud504\ub808\uc784\uc758 \ub370\uc774\ud130\uac00 \ubaa8\ub450 \uc218\uce58\ub85c \ub418\uc5b4 \uc788\uc5b4\uc57c\ub9cc\ud568\n# \uc785\ub825\ubcc0\uc218 - \ud0c0\uac9f\ubcc0\uc218 \ud615\ud0dc\ub85c \uc138\ud305","e629be32":"np.random.seed(4953)   # \uc6d0\ud558\ub294 \uc218 \uc785\ub825","7698f6a4":"x = iris_df.drop(columns='label')   # \ud0c0\uac9f\uac12 \uc5f4 \uc81c\uc678\ud558\uace0 \uc785\ub825\uac12 \uc5f4\ub9cc x \ubcc0\uc218\uc5d0 \uc800\uc7a5\ny = iris_df['label']   # \ud0c0\uac9f\uac12 \uc5f4 y \ubcc0\uc218\uc5d0 \uc800\uc7a5","516217fd":"x","7a7bbc11":"y","1d963322":"x_train, x_test, y_train, y_test = train_test_split(x, y, test_size=0.3)   # (x:\uc785\ub825\ub370\uc774\ud130, y:\ud0c0\uac9f\ub370\uc774\ud130, \ud14c\uc2a4\ud2b8\uc14b \ube44\uc728 \uc9c0\uc815:\uae30\ubcf8\uac12 0.25)","41ca09c7":"x_train   # \ud2b8\ub808\uc778 \ub370\uc774\ud130\uc14b\uc758 \uc785\ub825\ub370\uc774\ud130(\uc804\uccb4\uc911 70% \ub79c\ub364\uac12)","d712e2bc":"y_train   # \ud2b8\ub808\uc778 \ub370\uc774\ud130\uc14b\uc758 \ud0c0\uac9f\ub370\uc774\ud130(\uc804\uccb4\uc758 70% \ub79c\ub364\uac12)","c4386374":"x_test   # \ud14c\uc2a4\ud2b8 \ub370\uc774\ud130\uc14b\uc758 \uc785\ub825\ub370\uc774\ud130(\uc804\uccb4\uc758 30% \ub79c\ub364\uac12)","6e19884e":"y_test   # \ud14c\uc2a4\ud2b8 \ub370\uc774\ud130\uc14b\uc758 \ud0c0\uac9f\ub370\uc774\ud130(\uc804\uccb4\uc758 30% \ub79c\ub364\uac12)","18b3ae25":"dt = DecisionTreeClassifier()   # \uc5ec\ub7ec \uc635\uc158 \uc9c0\uc815 \uac00\ub2a5","284bb917":"dt.fit(x_train, y_train)","96f5de1a":"dt.predict(x_test)   # x_test \uc758 \uacb0\uacfc\uac12 \uc608\uce21\uce58 \ucd9c\ub825","1562dfcc":"pred = dt.predict(x_test)","a92a9477":"pred","c1002f1a":"y_test   # test \ub370\uc774\ud130\uc14b \uacb0\uacfc\uac12(\ud0c0\uac9f\uac12)","a7e6038b":"from sklearn.metrics import accuracy_score\naccuracy_score(y_test, pred)   # accuracy_score(\uc2e4\uc81c\uac12, \uc608\uce21\ud55c\uac12) : \uc608\uce21 \uc815\ud655\ub3c4\ub97c \ube44\uc728\ub85c \ucd9c\ub825","b9c4ca9d":"## - \ud559\uc2b5 \uacb0\uacfc \ube44\uad50","54c4e2f4":"### accuracy_score : 0.91","c75460b7":"## - train_test_split (Train set \uacfc Test set \ub79c\ub364\uc73c\ub85c \ubd84\ub9ac, 4\uac1c\uc758 \ub370\uc774\ud130\ub85c \ubd84\ub9ac)","cfbac3a8":"## - \uc785\ub825\ub370\uc774\ud130\uc640 \ud0c0\uac9f\ub370\uc774\ud130 x, y \ub85c \uad6c\ubd84","dddced6c":"## - \ud559\uc2b5 \uacb0\uacfc \ud655\uc778(predict)","5cb2dfc3":"#### 1. \ud544\uc694 \ud328\ud0a4\uc9c0 \ubc0f \ub370\uc774\ud130 \ub85c\ub4dc\n#### 2. \ub370\uc774\ud130\ud504\ub808\uc784 \uc0dd\uc131\n#### 3. \ub79c\ub364\uc2dc\ub4dc \uace0\uc815 (\ub09c\uc218\ud45c \uc9c0\uc815)\n#### 4. \ub370\uc774\ud130 \ubd84\ud560 (\uc785\ub825\ub370\uc774\ud130:x, \ud0c0\uac9f\ub370\uc774\ud130:y)\n#### 5. train_test_split (train \ub370\uc774\ud130\uc14b x, y and  test \ub370\uc774\ud130\uc14b x, y : \ucd1d 4\uac1c \ub370\uc774\ud130\uc14b \uc0dd\uc131)\n#### 6. \ubaa8\ub378 \uc815\uc758 (\uc758\uc0ac\uacb0\uc815\ub098\ubb34: DecisionTreeClassifier)\n#### 7. train set  \ubaa8\ub378\ub9c1 (\uae30\uacc4\ud559\uc2b5, fit )\n#### 8. test set  Evaluation (\ubaa8\ud615\ud3c9\uac00)","f3e9dbb0":"## - \ud559\uc2b5 \uacb0\uacfc \uc800\uc7a5","83c708d2":"## - \ud2b8\ub808\uc778\uc14b \uae30\uacc4\ud559\uc2b5(fit)","d1388704":"## - \ub370\uc774\ud130\ud504\ub808\uc784 \ub9cc\ub4e4\uae30","ab717ea3":"## - \ub79c\ub364 \uc2dc\ub4dc \uace0\uc815 (\ub79c\ub364\uac12\uc744 \ub9cc\ub4e4\ub54c \ub09c\uc218\ud45c \ubc88\ud638\ub97c \uc9c0\uc815\ud558\uc5ec \ubb34\uc791\uc704\uac12\uc744 \uac00\uc838\uc624\ub3c4\ub85d\ud568: \uc2e4\ud5d8 \uc7ac\ud604\uc744 \uc704\ud574 \ud544\uc218)","c2336eb9":"## [ ML \uae30\ubcf8 \uc791\uc5c5 \uc21c\uc11c ]","87b05f17":"# Scikit-learn ML Model (iris)","5dc0eb71":"## - \ubaa8\ud615 \ud3c9\uac00 (\uc0ac\uc774\ud0b7\ub7f0 metrics \uc911 accuracy_score \uc0ac\uc6a9, \uc608\uce21 \uc815\ud655\ub3c4\ub97c \ube44\uc728\ub85c \ucd9c\ub825)","b5e65ce1":"## - \ubaa8\ub378(\uc54c\uace0\ub9ac\uc998) \uc815\uc758 (\uc758\uc0ac\uacb0\uc815\ub098\ubb34: DecisionTreeClassifier)"}}