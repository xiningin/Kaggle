{"cell_type":{"66dc5db7":"code","c4cf46c6":"code","74a76e04":"code","8a961492":"code","d68f29a3":"code","c1ec7904":"code","12d91b30":"code","89d6da69":"code","06d81e74":"code","cff08b1d":"code","92ad6fe5":"code","e84f4645":"code","ae72081c":"code","884646c2":"code","bd24236d":"code","ca714325":"code","3927f955":"code","c6b10e11":"code","d7ea8903":"code","0f3fca92":"code","4eed589f":"code","84b8ccfd":"code","3a8930c8":"code","a9446707":"code","5e74e540":"code","ac54e6be":"code","f6196da4":"code","44c1821c":"code","4dd1f4fb":"code","a5f32c25":"code","0ef6f4d9":"code","6b45f8c6":"markdown","25f1607c":"markdown","596336b5":"markdown","3d9eb374":"markdown","e1e43b7c":"markdown","9cc262ee":"markdown","6284cbab":"markdown","822b4da2":"markdown","3273bc92":"markdown","5d48021b":"markdown","9d4db71a":"markdown","13ded0de":"markdown","a2f36b89":"markdown","f6920291":"markdown","4201821e":"markdown","50a9bbe9":"markdown","c3fbfbbe":"markdown","1346a1f4":"markdown","b0f1aeba":"markdown"},"source":{"66dc5db7":"import numpy as np\nimport pandas as pd\nimport seaborn as sns\nimport re\nimport datetime\n\nfrom sklearn.preprocessing import MinMaxScaler\n\n%pylab inline","c4cf46c6":"apps = pd.read_csv('\/kaggle\/input\/google-play-store-apps\/googleplaystore.csv').set_index('App')\nreviews = pd.read_csv('\/kaggle\/input\/google-play-store-apps\/googleplaystore_user_reviews.csv').set_index('App')","74a76e04":"df = apps.join(reviews, how='left')","8a961492":"df","d68f29a3":"sentiments = df[['Sentiment_Polarity', 'Sentiment_Subjectivity']].copy(deep=True)","c1ec7904":"sentiments.Sentiment_Polarity.fillna(0, inplace=True)\nsentiments.Sentiment_Subjectivity.fillna(0, inplace=True)","12d91b30":"sentiments.Sentiment_Polarity[sentiments.Sentiment_Polarity != 0].hist(bins=50, density=True)\npylab.xlabel('Polarity')\npylab.ylabel('Counts')\npylab.title('Sentiment Polarity histogram')\npylab.show()","89d6da69":"sentiments.Sentiment_Subjectivity[sentiments.Sentiment_Subjectivity != 0].hist(bins=50, density=True)\npylab.xlabel('Subjectivity')\npylab.ylabel('Counts')\npylab.title('Sentiment Subjectivity histogram')\npylab.show()","06d81e74":"sentiments.corr()\n# \u043d\u0435 \u043a\u043e\u0440\u0440\u0435\u043b\u0438\u0440\u0443\u044e\u0442","cff08b1d":"sns.scatterplot(sentiments.Sentiment_Polarity, sentiments.Sentiment_Subjectivity)\npylab.title('Scatterplot Polarity\/Subjectivity')\npylab.show()","92ad6fe5":"pylab.plot(np.arange(-1, 1.00001, 0.01), \\\n           [sentiments.Sentiment_Subjectivity[round(sentiments.Sentiment_Polarity, 2) == i \/ 100].median() for i in range(-100, 101, 1)])","e84f4645":"# \u0415\u0441\u043b\u0438 \u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0435\u0442\u0441\u044f \u0442\u0430\u043a\u0430\u044f \u0444\u043e\u0440\u043c\u0443\u043b\u0430, \u0442\u043e \nsentiments['Review_Rate'] = 10 * sentiments.Sentiment_Polarity \/ (sentiments.Sentiment_Subjectivity + 1)","ae72081c":"apps.Rating.fillna(2.5, inplace=True)\napps_reviews = apps.join(sentiments.groupby(by=sentiments.index).mean().Review_Rate, how='inner')","884646c2":"apps_reviews[['Rating']].corrwith(apps_reviews.Review_Rate)","bd24236d":"apps_reviews.head()","ca714325":"apps_reviews.dropna(inplace=True) # \u0434\u0440\u043e\u043f\u0430\u0435\u043c \u0432\u0441\u0435 \u0441\u0442\u0440\u043e\u0447\u043a\u0438 \u0441 nan. \u0418\u0445 \u041e\u0427\u0415\u041d\u042c \u043c\u0430\u043b\u043e \u0432 \u043f\u0440\u043e\u0446\u0435\u043d\u0442\u043d\u043e\u043c \u0441\u043e\u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0438, \u0430 \u0433\u043e\u043b\u043e\u0432\u043d\u043e\u0439 \u0431\u043e\u043b\u0438 \u0441 \u043d\u0438\u043c\u0438 \u043c\u043d\u043e\u0433\u043e\napps_reviews.Reviews = apps_reviews.Reviews.apply(int) # \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043e\u0442\u0437\u044b\u0432\u043e\u0432 - \u044f\u0432\u043d\u043e \u0446\u0435\u043b\u043e\u0435 \u0447\u0438\u0441\u043b\u043e, \u0430 \u043d\u0435 \u043a\u0430\u043a\u043e\u0439-\u0442\u043e \u0442\u0430\u043c object\n\n# Size. \u0422\u0430\u043c \u0435\u0441\u0442\u044c \u043e\u0434\u043d\u0430 \u0441\u0442\u0440\u043e\u043a\u0430 \"1000+\", \u043c\u043e\u0436\u0435\u0442 \u0435\u0441\u0442\u044c \u0434\u0440\u0443\u0433\u0438\u0435, \u0442\u043e\u0436\u0435 \u0445\u0440\u0435\u043d\u043e\u0432\u044b\u0435. \u0415\u0441\u043b\u0438 \u0441\u0442\u0440\u043e\u043a\u0430 \u043d\u0435 \"Varies with device\" \u0438 \u043d\u0435 \u0432\u0438\u0434\u0430 \"\u0447\u0438\u0441\u043b\u043e.\u0447\u0438\u0441\u043b\u043eM\" \u0438\u043b\u0438 \"\u0447\u0438\u0441\u043b\u043e.\u0447\u0438\u0441\u043b\u043ek\",\n# \u0442\u043e \u043c\u044b \u0435\u0451 \u0431\u043b\u0430\u0433\u043e\u043f\u043e\u043b\u0443\u0447\u043d\u043e \u0434\u0440\u043e\u043f\u0430\u0435\u043c. \u0412\u0441\u0435 \"Varies with device\" \u0437\u0430\u043c\u0435\u043d\u044f\u0435\u043c \u043d\u0430 -1, \u0432\u0441\u0435 \u043a\u0438\u043b\u043e\u0431\u0430\u0439\u0442\u044b \u043f\u0440\u0438\u0432\u043e\u0434\u0438\u043c \u043a \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u043c.\nfor i, s in apps_reviews.Size.iteritems():\n    if not re.match(r'^\\d+[\\.]?\\d+[Mk]$', s) and s != 'Varies with device':\n        apps_reviews.drop(i, inplace=True)\n\ndef size_to_Mb(s):\n    if s == 'Varies with device':\n        return -1\n    else:\n        return float(s[:-1]) if s.endswith('M') else float(s[:-1]) \/ (2 ** 10)\n\napps_reviews.Size = apps_reviews.Size.apply(size_to_Mb) # \u0440\u0430\u0437\u043c\u0435\u0440 \u0438\u043b\u0438 \u0446\u0435\u043b\u043e\u0435 \u0447\u0438\u0441\u043b\u043e, \u0438\u043b\u0438 -1, \u0435\u0441\u043b\u0438 \"Varies with device\"\napps_reviews.Installs = apps_reviews.Installs.apply(lambda x: int(re.sub(r'\\D', '', x))) # \u0443\u0431\u0438\u0440\u0430\u0435\u043c \u0438\u0437 \u0441\u0442\u0440\u043e\u043a\u0438 \u0432\u0441\u0451, \u0447\u0442\u043e \u043d\u0435 \u0447\u0438\u0441\u043b\u0430, \u0438 \u043f\u0440\u0438\u0432\u043e\u0434\u0438\u043c \u043a \u0446\u0435\u043b\u043e\u043c\u0443\napps_reviews.drop(columns=['Type'], inplace=True) # \u043d\u0430\u0444\u0438\u0433 \u043d\u0435 \u0441\u0434\u0430\u043b\u0430\u0441\u044c \u043b\u0438\u0448\u043d\u044f\u044f \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0430\u043b\u044c\u043d\u0430\u044f \u0444\u0438\u0447\u0430, \u0432\u0441\u0435 \u044d\u0442\u0438 \u0436\u0435 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u043e\u043b\u0443\u0447\u0430\u044e\u0442\u0441\u044f \u043c\u0430\u0441\u043a\u043e\u0439 Price == 0\napps_reviews.Price = apps_reviews.Price.apply(lambda x: float(re.sub(r'\\$', '', x))) # \u0443\u0431\u0438\u0440\u0430\u0435\u043c \u0437\u043d\u0430\u0447\u043a\u0438 \u0434\u043e\u043b\u043b\u0430\u0440\u0430, \u0433\u0434\u0435 \u0435\u0441\u0442\u044c, \u0438 \u043f\u0440\u0438\u0432\u043e\u0434\u0438\u043c \u043a float\napps_reviews.drop(columns=['Genres'], inplace=True) # \u0434\u0443\u0431\u043b\u0438\u043a\u0430\u0442 Category\n\n# \u0437\u0430\u043c\u0435\u043d\u044f\u0435\u043c \u0434\u0430\u0442\u0443 \u0430\u043f\u0434\u0435\u0439\u0442\u0430 \u043d\u0430 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043c\u0435\u0441\u044f\u0446\u0435\u0432 \u043e\u0442 \u0442\u0435\u043a\u0443\u0449\u0435\u0439 \u0434\u0430\u0442\u044b\napps_reviews['Last Updated'] = apps_reviews['Last Updated'].apply(lambda x: (datetime.datetime.today().year - datetime.datetime.strptime(x, '%B %d, %Y').year) * 12 +\n                                             (datetime.datetime.today().month - datetime.datetime.strptime(x, '%B %d, %Y').month))\n\n# Current Ver \u0443\u0434\u0430\u043b\u044f\u044e. \u0412 \u0443\u043f\u043e\u0440 \u043d\u0435 \u0432\u0438\u0436\u0443 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0430\u0446\u0438\u0439 \u043e\u0442 \u0432\u0435\u0440\u0441\u0438\u0438 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u044f. \u041c\u0430\u043a\u0441\u0438\u043c\u0443\u043c - \u0441\u0447\u0438\u0442\u0430\u0442\u044c, \u0447\u0442\u043e \u0432\u0441\u0451, \u0447\u0442\u043e \"0.\u0447\u0438\u0441\u043b\u043e\" - \u044d\u0442\u043e \u0431\u0435\u0442\u0430-\u0432\u0435\u0440\u0441\u0438\u044f.\n# \u041d\u043e \u044d\u0442\u043e \u043b\u0438\u0448\u043d\u0438\u0439 \u0433\u0435\u043c\u043e\u0440\u0440\u043e\u0439, \u0440\u0430\u0437\u0434\u0435\u043b\u044f\u0442\u044c \u0438\u0445 \u0435\u0449\u0451, \u043a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e, \u0431\u0435\u0442\u0430-\u0432\u0435\u0440\u0441\u0438\u044f - \u0442\u043e\u0436\u0435 \u0432\u0435\u0440\u0441\u0438\u044f.\napps_reviews.drop(columns=['Current Ver'], inplace=True)\n\n# Android Ver. \u0415\u0441\u0442\u044c \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0439 \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0440\u0430\u0431\u043e\u0442\u0430\u043b\u0438 \u0441 \u043a\u0430\u043a\u043e\u0439-\u0442\u043e \u0432\u0435\u0440\u0441\u0438\u0438 \u0430\u043d\u0434\u0440\u043e\u0438\u0434\u0430 \u043f\u043e \u043a\u0430\u043a\u0443\u044e-\u0442\u043e \u0441\u0442\u0430\u0440\u0443\u044e, \u0438 \u0443\u0436\u0435 \u043d\u0435 \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u044e\u0442\u0441\u044f. \u0423\u0434\u0430\u043b\u044f\u0435\u043c \u0438\u0445 \u043d\u0430\u0444\u0438\u0433, \n# \u0441\u043c\u044b\u0441\u043b\u0430 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u043e\u0432\u0430\u0442\u044c \u043d\u0435\u0442. \u0414\u043b\u044f \u043e\u0441\u0442\u0430\u043b\u044c\u043d\u044b\u0445: \u0432\u044b\u0442\u0430\u0441\u043a\u0438\u0432\u0430\u0435\u043c \u0432\u0435\u0440\u0441\u0438\u044e, \u0441 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u043f\u0440\u0438\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442, \u0432 \u0444\u043e\u0440\u043c\u0430\u0442\u0435 \"\u0447\u0438\u0441\u043b\u043e.\u0447\u0438\u0441\u043b\u043e\". \n# \u041d\u0435 \u0432\u0438\u0436\u0443 \u043e\u0441\u043e\u0431\u043e\u0439 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e\u0441\u0442\u0438 \u0440\u0430\u0437\u043b\u0438\u0447\u0430\u0442\u044c \u0432\u0435\u0440\u0441\u0438\u0438 4.0.3 \u0438 4.0.1. \u041e\u043f\u044f\u0442\u044c \u0436\u0435, \u0434\u043b\u044f \"Various with device\" \u0441\u0442\u0430\u0432\u0438\u043c -1.\nfor i, s in apps_reviews['Android Ver'].iteritems():\n    if '-' in s:\n        apps_reviews.drop(i, inplace=True)\n\napps_reviews['Android Ver'] = apps_reviews['Android Ver'].apply(lambda x: float(re.search('\\d+\\.\\d+', x.split()[0]).group(0)) if x != 'Varies with device' else -1)","3927f955":"apps_reviews","c6b10e11":"sns.heatmap(apps_reviews.corr())","d7ea8903":"apps_reviews.to_csv('apps_preprocessed.csv', index=True)","0f3fca92":"apps_reviews.Review_Rate.describe()","4eed589f":"apps_reviews.drop(apps_reviews[apps_reviews.Reviews == 0].index, inplace=True)","84b8ccfd":"def sigmoid(x): \n    return 1 \/ (1 + np.exp(-x))\n\napps_reviews['RR_index'] = (apps_reviews.Rating + apps_reviews.Review_Rate) * \\\n                            sigmoid(MinMaxScaler((-6, 6)).fit_transform(apps_reviews.Reviews.to_numpy().reshape(-1, 1))).reshape(-1)","3a8930c8":"TOP_N = 3000\ntop = apps_reviews.nlargest(TOP_N, 'RR_index')","a9446707":"top","5e74e540":"top.to_csv('top_3000_reviews.csv', index=True)","ac54e6be":"apps_reviews","f6196da4":"# \u043f\u043e \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f\u043c \u0434\u043e\u0431\u0443\u0434\u0435\u043c \u0441\u0440\u0435\u0434\u043d\u0438\u0435 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0438\ncat_inst_dict = apps_reviews[['Category', 'Installs']].groupby('Category').median().to_dict()['Installs']\nmean_cat = apps_reviews.Category.map(cat_inst_dict)","44c1821c":"from scipy.stats import norm\n\n\ncats = MinMaxScaler().fit_transform((apps_reviews.Installs \/ mean_cat).to_numpy().reshape(-1, 1))\n\ndef rational_sigmoid(a):\n    return a \/ (1 + np.abs(a))\nsize = MinMaxScaler().fit_transform((-rational_sigmoid(MinMaxScaler((-4, 4)).\\\n                                                       fit_transform(apps_reviews.Size.to_numpy().reshape(-1, 1)))).reshape(-1, 1))\ninstalls = MinMaxScaler().fit_transform((norm.pdf(np.log10(apps_reviews.Installs), loc=6, scale=0.3)).reshape(-1, 1))\nprice = MinMaxScaler().fit_transform(np.exp(-0.01 * apps_reviews.Price).to_numpy().reshape(-1, 1))\nlu = MinMaxScaler().fit_transform((1 \/ apps_reviews['Last Updated']).to_numpy().reshape(-1, 1))\nav = MinMaxScaler().fit_transform(apps_reviews['Android Ver'].apply(lambda x: x if x != -1 else apps_reviews['Android Ver'].max()).to_numpy().reshape(-1, 1))\nrr = MinMaxScaler().fit_transform((apps_reviews.RR_index).to_numpy().reshape(-1, 1))\n\napps_reviews['Promote_index'] = MinMaxScaler().fit_transform(cats * size * installs * price * lu * av * rr)","4dd1f4fb":"promote_by_all = apps_reviews.sort_values(by='Promote_index', ascending=False)","a5f32c25":"promote_by_all.to_csv('promote_by_all.csv', index=True)","0ef6f4d9":"promote_by_all","6b45f8c6":"\u041f\u043e\u0441\u0442\u0440\u043e\u0438\u043c \u0433\u0438\u0441\u0442\u043e\u0433\u0440\u0430\u043c\u043c\u044b. \u0421\u0442\u0440\u043e\u0438\u043c \u0434\u043b\u044f \u0432\u0441\u0435\u0445, \u043a\u0440\u043e\u043c\u0435 \u043d\u0443\u043b\u0435\u0439, \u0430 \u0442\u043e \u0438\u0445 \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043c\u043d\u043e\u0433\u043e. \u041d\u0438\u0447\u0435\u0433\u043e \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0433\u043e \u043d\u0435 \u0432\u0438\u0434\u043d\u043e, \u0447\u0442\u043e-\u0442\u043e \u043f\u043e\u0445\u043e\u0436\u0435\u0435 \u043d\u0430 \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e\u0435 \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u0435. \u041e\u0442\u0437\u044b\u0432\u044b \u0447\u0443\u0442\u044c \u0441\u0434\u0432\u0438\u043d\u0443\u0442\u044b \u0432 \u044