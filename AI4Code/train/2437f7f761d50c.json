escription'][0]\nprint(first_description)","cf496d73":"#Exercise-3\nfirst_row = reviews.iloc[0]\nprint(first_row)","e64884e1":"#Exercise-4\nfirst_descriptions =pd.Series(reviews.iloc[0:10, 1])\nprint(first_descriptions)","9727f45c":"#Exercise-5\nsample_reviews = reviews.iloc[[1, 2, 3, 5, 8], :]\nsample_reviews","454fdda5":"#Exercise-6\ndf = reviews.loc[[0,1,10,100],['country','province','region_1','region_2']]\ndf","d877a645":"#Exercise-7\ndf = reviews.iloc[0:100][['country','variety']]\ndf","c9dedfe7":"#Exercise-8\nitalian_wines = reviews[reviews['country']=='Italy']\nitalian_wines","2469b076":"#Exercise-9\ntop_oceania_wines = reviews[(reviews['points']>= 95) & ((reviews['country']=='Australia')|(reviews['country']=='New Zealand'))]\ntop_oceania_wines","f09837f5":"#Exercise-1\nmedian_points = reviews['points'].median()\nprint(median_points)","89215018":"#Exercise-2\ncountries = reviews.country.unique()\nprint(countries)","899575f9":"#Exercise-3\nreviews_per_country = reviews.country.value_counts()\nreviews_per_country","8ad2a55e":"#Exercise-4\ncentered_price = reviews.loc[:, 'price']-reviews.loc[:, 'price'].mean()\ncentered_price","554ab42c":"#Exercise-5\nbargain_wine = reviews.loc[(reviews.points\/reviews.price).idxmax(), 'title']\nprint(bargain_wine)","dd8d3e17":"#Exercise-6\nfruity = reviews['description'].map(lambda x: 'fruity' in x)\ntropical=reviews['description'].map(lambda x: 'tropical' in x)\ndescriptor_counts = pd.Series([tropical.sum(), fruity.sum()],index=['tropical','fruity'])\ndescriptor_counts","e5e032a3":"#Exercise-7\ndef ratings(row):\n    if(row['country']=='Canada'):\n        return 3\n    elif(row['points']>=95):\n        return 3\n    elif(row['points']>=85 and row['points']<95):\n        return 2\n    else:\n        return 1\nstar_ratings = reviews.apply(ratings,axis='columns')\nstar_ratings","af81658e":"# Exercise-1\nreviews_written = reviews.groupby('taster_twitter_handle').taster_twitter_handle.count()\nreviews_written","2153166c":"# Exerci