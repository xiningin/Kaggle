6":"sns.countplot('Embarked',hue='Survived',data=titanic_data,palette='Spectral');","f3de9631":"X = titanic_data.drop(columns = ['PassengerId','Name','Ticket','Survived'],axis=1)\nY = titanic_data['Survived']","e3c1e437":"model=LogisticRegression()","5f7e6343":"model.fit(X,Y)","f0c709c6":"pred_on_train_data=model.predict(X)","00b14310":"accuracy_on_train_data = accuracy_score(Y,pred_on_train_data)\nprint(\"The accuracy score is:{}\".format(accuracy_on_train_data))","36740608":"#loading the data for testing\ntest_data=pd.read_csv('..\/input\/titanic\/test.csv')\ntest_data","f67aaeae":"test_data.isnull().sum()","8395de41":"test_data = test_data.drop(columns='Cabin', axis=1)\ntest_data['Age'].fillna(test_data['Age'].mean(), inplace=True)\ntest_data['Fare'].fillna(test_data['Fare'].mean(), inplace=True)\ntest_data.replace({\"Sex\":{\"male\":0,\"female\":1},\"Embarked\":{\"C\":0,\"Q\":1,\"S\":2}},inplace=True)\ntest_data.head()","2120896a":"test_data.isnull().sum()","3fba8772":"X_test=test_data.drop(columns = ['PassengerId','Name','Ticket'],axis=1)","cbee91f1":"test_data['survived']=model.predict(X_test)\ntest_data","a71fdc75":"sub=test_data.iloc[:,[0,10]]\nsub","73073c72":"sub.to_csv('submission.csv',index=False)","ceefac3a":"#### Splitting the datset into training and testing data","1214c9b7":"# Titanic Survival predictions","50ba6c3c":"## EDA"}}