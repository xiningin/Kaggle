s.layers import LSTM\nfrom tensorflow.keras.layers import Bidirectional\nfrom tensorflow.keras.layers import Dense\nfrom tensorflow.keras.preprocessing.text import one_hot","01dc5327":"rep=[one_hot(i,voc_size) for i in corpus]","4ff1fd1b":"rep","d1ab7c5b":"sent_length=50","3a03e6a5":"embedded_docs=pad_sequences(rep,padding='pre',maxlen=sent_length)","e2f24cdd":"embedded_docs","21145206":"dim=50","e00ef8a1":"model=Sequential()\nmodel.add(Embedding(voc_size,dim,input_length=sent_length))\nmodel.add(Dropout(0.3))\nmodel.add(Bidirectional(LSTM(100)))\nmodel.add(Dropout(0.3))\nmodel.add(Dense(1,activation='sigmoid'))\nmodel.compile(loss='binary_crossentropy',optimizer='adam',metrics=['accuracy'])","89ed010e":"model.summary()","c2f905a6":"x_data=np.array(embedded_docs)\ny_data=np.array(y)","d764ebaf":"x_data.shape\n","8345b1d8":"y_data.shape","996e6d80":"from sklearn.model_selection import train_test_split\nx_train,x_test,y_train,y_test=train_test_split(x_data,y_data,test_size=0.30,random_state=42)","f125879b":"model.fit(x_train,y_train,validation_data=(x_test,y_test),epochs=30,batch_size=64)","7e55eb69":"y_pred=model.predict_classes(x_test)","90a3675a":"y_pred","ce10dcfd":"from sklearn.metrics import confusion_matrix,classification_report,accuracy_score","35ab1237":"score=accuracy_score(y_test,y_pred)","6e8e6afe":"score","163e84bb":"confusion_matrix(y_test,y_pred)","6aed5580":"classification_report(y_test,y_pred)","5ad3e97c":"SPLITTING OF DATA","77553a05":"DATA PREPROCESSING","401f75cf":"FAKE NEWS DETECTOR | A SYSTEM TO IDENTIFY UNRELIABLE NEWS ARTICLES","6f2f645f":"WORD EMBEDDING REPRESENTATION","07f41173":"LSTM | LONG SHORT TERM MEMORY MODEL","13459a9a":"PERFORMANCE METRICS AND ACCURACY","52ace2a4":"MODEL TRAINING"}}