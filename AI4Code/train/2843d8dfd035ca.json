mmies(x_test['Embarked'],drop_first=True)","78e76974":"x_test.drop(['Sex','Embarked'],axis=1,inplace=True)","33ad1654":"test= pd.concat([x_test,sex_test,embarked_test],axis=1)","b76e9d30":"test.head()","445ae802":"prediction=classifier.predict(test)","aa78cc96":"lll=pd.read_csv('..\/input\/titanic\/gender_submission.csv')","08dfb159":"from sklearn.metrics import confusion_matrix,accuracy_score,classification_report","e3bec0f7":"accuracy_score(lll['Survived'],prediction)","bb762579":"confusion_matrix(lll['Survived'],prediction)","bebe40da":"submission=pd.DataFrame({'PassengerId':lll['PassengerId'],'Survived':prediction})","23616926":"submission.to_csv('submission3.csv',index=False)","02727c6f":"# FUNCTION TO FILL NaN VALUES IN COLUMN 'AGE'","977af4e0":"# IMPORTING SOME USEFUL LIBRARIES","20cc0427":"# VISUALIZING DATA","9f29a03c":"# THANKS A LOT !!!!!  (plz upvote)","da3d6381":"# TAKING ONLY USEFUL FEATURES","c884ac0e":"# CHANGING CATEGORICAL FEATURIES INTO DUMMY VALUES","ed7e1b93":"# SAME APPROACH FOR 'TEST' DATASET"}}