{"cell_type":{"e2551a58":"code","82702730":"code","8c022221":"code","ca45a4cb":"code","73e48fa9":"code","a9e86156":"code","c7d5ff8e":"code","7db86b4a":"code","5d433981":"code","627c4488":"code","62b92586":"code","0ad574ce":"code","f616bed3":"code","6dd2e434":"code","ae8f0822":"code","025e1e5b":"code","a996b404":"code","ccc883e7":"code","d80e8cd7":"code","a7b4e3e4":"code","dc555b82":"code","789c8a19":"code","8f264ed4":"code","8a9a124f":"code","3f4dae49":"code","e457eb9c":"code","082e6360":"code","e3d47f36":"code","fd6bfae1":"code","2dba276f":"code","0ead0c2c":"code","c151316b":"code","98afde5a":"code","78491cde":"code","c9089892":"markdown","475d9669":"markdown","fe729354":"markdown","8413ef30":"markdown","ee99c6b1":"markdown","ef28cc1c":"markdown","f71edee6":"markdown","6b2d034e":"markdown","c13b8fd5":"markdown","df6bfd6d":"markdown","31de1a92":"markdown","38665337":"markdown","a1e48823":"markdown","37c453dd":"markdown","dce519a9":"markdown","7c090f84":"markdown","2c158415":"markdown","cde8fd70":"markdown","b86697fc":"markdown","8fcbe501":"markdown","c8860c9f":"markdown","6b056338":"markdown","56ed69e1":"markdown","07053a4e":"markdown","9a2649af":"markdown","f7844317":"markdown","4a18acd5":"markdown","4474d364":"markdown","bccf4055":"markdown","d7ab78ed":"markdown"},"source":{"e2551a58":"import numpy as np\nimport pandas as pd\nimport matplotlib.pyplot as plt\nimport seaborn as sns\nimport datetime\nfrom sklearn.ensemble import GradientBoostingRegressor\n\n%matplotlib inline","82702730":"DATA_TRAIN_PATH='\/kaggle\/input\/realestatepriceprediction\/train.csv'\nDATA_TEST_PATH='\/kaggle\/input\/realestatepriceprediction\/test.csv'\nRESULT_PATH='.\/predictions.csv'\ndata=pd.read_csv(DATA_TRAIN_PATH)\nX_test=pd.read_csv(DATA_TEST_PATH)","8c022221":"df=data.copy()","ca45a4cb":"df.info()","73e48fa9":"df.DistrictId=df.DistrictId.astype('object')\ndf.Id=df.Id.astype('object')\ndf.info()","a9e86156":"digital_features=df.select_dtypes(exclude=['object'])\ndigital_features.hist(figsize=(18,12), bins=30)\nplt.show()","c7d5ff8e":"df.describe()","7db86b4a":"plt.figure(figsize=(16, 8))\nsns.boxplot(data=df[['Square', 'LifeSquare', 'KitchenSquare']], orient='h')\nplt.xscale('symlog')\nplt.xlim(left=-1)\nplt.show()","5d433981":"df.insert(2,'SqMeterPrice', df.Price\/df.Square)","627c4488":"df.describe()","62b92586":"# \u0412\u0437\u0433\u043b\u044f\u043d\u0435\u043c \u043d\u0430 \u0432\u044b\u0431\u0440\u043e\u0441\u044b:\nplt.figure(figsize=(16, 8))\nsns.boxplot(data=df.SqMeterPrice, orient='h')\nplt.show()","0ad574ce":"df.sort_values(by='Square', axis=0, ascending=True).head()","f616bed3":"df.loc[(df.Square<1.3), 'Square']= df['Square']*100\ndf.loc[(df.Square<=13), 'Square']= df['Square']*10\ndf.loc[(df.Square>300), 'Square']= df['Square']\/10","6dd2e434":"df.drop('SqMeterPrice', axis=1, inplace=True)\ndf.insert(2,'SqMeterPrice', df.Price\/df.Square)","ae8f0822":"plt.figure(figsize=(16, 8))\nsns.boxplot(data=df.SqMeterPrice, orient='h')\nplt.show()","025e1e5b":"df_corr=df.corr()\nplt.figure(figsize=(18,16))\nsns.heatmap(df_corr, annot=True, linewidths=0.3, cmap='coolwarm', square=True)\nplt.show()","a996b404":"df.Rooms.unique()","ccc883e7":"df[df.Rooms>6]","d80e8cd7":"df[df.Rooms==0]","a7b4e3e4":"df.Rooms.mode()","dc555b82":"df.isnull().sum()","789c8a19":"for feature in df.columns[10:-1]:\n    print(f'\u041f\u0440\u0438\u0437\u043d\u0430\u043a - {feature}\\t\u0423\u043d\u0438\u043a\u0430\u043b\u044c\u043d\u044b\u0445 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0439 - {df[feature].nunique()}.')","8f264ed4":"class FeaturesGenerator():\n    \n    def __init__(self):\n        \n        self.current_year=None\n        self.cat_columns=None\n        self.rsqm=None\n        self.nsqm=None\n        \n        \n    def fit(self,df):\n        \n        # \u0422\u0435\u043a\u0443\u0449\u0438\u0439 \u0433\u043e\u0434, \u0434\u043b\u044f \u0432\u044b\u0447\u0438\u0441\u043b\u0435\u043d\u0438\u044f \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u0430 \u0434\u043e\u043c\u0430\n        \n        self.current_year=datetime.datetime.now().year\n        \n        # \u041a\u043e\u0440\u0440\u0435\u043a\u0442\u0438\u0440\u0443\u0435\u043c \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u043f\u043b\u043e\u0449\u0430\u0434\u0435\u0439, \u0438 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043a\u043e\u043c\u043d\u0430\u0442\n        \n        df.loc[(df.Square<1.3), 'Square']= df['Square']*100\n        df.loc[(df.Square<=13), 'Square']= df['Square']*10\n        df.loc[(df.Square>300), 'Square']= df['Square']\/10\n        df.loc[((df.Rooms>6)|(df.Rooms==0)), 'Rooms']=2\n       \n        \n        # \u0414\u043e\u0431\u0430\u0432\u043b\u044f\u0435\u043c \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u043a\u0432.\u043c \u0434\u043b\u044f \u043f\u043e\u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0439 \u0430\u0433\u0440\u0435\u0433\u0430\u0446\u0438\u0438 \u0434\u0430\u043d\u043d\u044b\u0445 \u0432 \u043e\u0431\u0443\u0447\u0430\u044e\u0449\u0435\u0439 \u0438 \u0442\u0435\u0441\u0442\u043e\u0432\u043e\u0439 \u0432\u044b\u0431\u043e\u0440\u043a\u0430\u0445\n        \n        df.insert(1, 'SqMeterPrice',df['Price']\/df['Square'])\n        \n        # \u0412\u044b\u0447\u0438\u0441\u043b\u044f\u0435\u043c \u043d\u0430 \u0442\u0435\u0441\u0442\u0435 \u0441\u0440\u0435\u0434\u043d\u044e\u044e \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u043a\u0432.\u043c \u0432 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u043a-\u0432\u0430 \u043a\u043e\u043c\u043d\u0430\u0442(\u0434\u043b\u044f \u043e\u0431\u0440\u0430\u0431\u043e\u0442\u043a\u0438 \u043f\u0440\u043e\u043f\u0443\u0441\u043a\u043e\u0432 \u0432 \u0442\u0435\u0441\u0442\u043e\u0432\u043e\u043c \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0435),\n        # \u0438 \u0441\u0440\u0435\u0434\u043d\u044e\u044e \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u043a\u0432.\u043c \u0432 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u0440\u0430\u0439\u043e\u043d\u0430 \u0438 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430 \u043a\u043e\u043c\u043d\u0430\u0442\n        \n        self.rsqm=df.groupby(by=['Rooms'])['SqMeterPrice'].mean().rename('RoomSqMeterPrice', inplace=True)\n        self.nsqm=df.groupby(by=['DistrictId','Rooms'])['SqMeterPrice'].mean().rename('DistSqMeterPrice', inplace=True) \n        \n        # \u0423\u0434\u0430\u043b\u044f\u0435\u043c \u043b\u0438\u0448\u043d\u0435\u0435\n        \n        df.drop('SqMeterPrice', axis=1, inplace=True)\n        \n        # \u041d\u0430\u0437\u043d\u0430\u0447\u0430\u0435\u043c \u0441\u0442\u043e\u043b\u0431\u0446\u044b \u0434\u043b\u044f \u043f\u043e\u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0433\u043e \u0434\u0430\u043c\u043c\u0438-\u043a\u043e\u0434\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u044f\n        \n        self.cat_columns=df.select_dtypes('object').columns\n        \n        \n        \n    def transform(self, df):\n        \n        # \u0417\u0430\u043c\u0435\u043d\u044f\u0435\u043c \u0433\u043e\u0434 \u043f\u043e\u0441\u0442\u0440\u043e\u0439\u043a\u0438 \u0434\u043e\u043c\u0430 \u043d\u0430 \u0432\u043e\u0437\u0440\u0430\u0441\u0442 \u0434\u043e\u043c\u0430(\u0432\u044b\u0431\u0440\u043e\u0441\u044b \u043f\u043e \u0433\u043e\u0434\u0443 \u043f\u043e\u0441\u0442\u0440\u043e\u0439\u043a\u0438 \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0438\u0441\u043f\u0440\u0430\u0432\u043b\u044f\u0435\u043c \u043d\u0430 \u0442\u0435\u043a\u0443\u0449\u0438\u0439 \u0433\u043e\u0434)\n        \n        df.loc[(df.HouseYear>self.current_year),'HouseYear']=self.current_year-1\n        df.insert(df.columns.get_loc('HouseYear'), 'HousAge', self.current_year - df['HouseYear'])\n        df.drop('HouseYear', axis=1, inplace=True)\n       \n        # \u041a\u043e\u0434\u0438\u0440\u0443\u0435\u043c \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0430\u043b\u044c\u043d\u044b\u0435 \u043f\u0435\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0435\n        \n        df=pd.get_dummies(df, columns=self.cat_columns)\n        \n        # \u041a\u043e\u0440\u0440\u0435\u043a\u0442\u0438\u0440\u0443\u0435\u043c \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u043f\u043b\u043e\u0449\u0430\u0434\u0435\u0439 \u0432 \u0442\u0435\u0441\u0442\u043e\u0432\u043e\u043c \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0435\n        \n        df.loc[(df.Square<1.3), 'Square']= df['Square']*100\n        df.loc[(df.Square<=13), 'Square']= df['Square']*10\n        df.loc[(df.Square>300), 'Square']= df['Square']\/10\n        \n        # \u041a\u043e\u0440\u0440\u0435\u043a\u0442\u0438\u0440\u0443\u0435\u043c \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043a\u043e\u043c\u043d\u0430\u0442 \u0432 \u0442\u0435\u0441\u0442\u043e\u0432\u043e\u043c \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0435\n        \n        df.loc[((df.Rooms>6)|(df.Rooms==0)), 'Rooms']=2\n                \n        # \u041e\u0431\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u0435\u043c \u043f\u0440\u043e\u043f\u0443\u0441\u043a\u0438 \u043f\u043e\u043b\u044f\u0445 Healthcare_1 \u0438 LifeSquare, \u0437\u0430\u043f\u043e\u043b\u043d\u044f\u044f \u0438\u0445 \u043c\u0435\u0442\u043e\u0434\u043e\u043c 'pad'\n        \n        df=df.sort_values(by='DistrictId')\n        df.Healthcare_1.fillna(method='pad',inplace=True)\n        df=df.sort_values(by='Square')\n        df.LifeSquare.fillna(method='pad',inplace=True)\n        df.sort_index(inplace=True)\n        \n        # \u0414\u043e\u0431\u0430\u0432\u043b\u044f\u0435\u043c \u043f\u043e\u043b\u0435 DistSqMeterPrice \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u0449\u0435\u0435 \u0441\u0440\u0435\u0434\u043d\u044e\u044e \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u043a\u0432.\u043c \u0432 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u0440\u0430\u0439\u043e\u043d\u0430 \u0438 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430 \u043a\u043e\u043c\u043d\u0430\u0442\n        \n        df=df.merge(self.rsqm, how='left', on='Rooms')\n        df=df.merge(self.nsqm, how='left', on=['DistrictId', 'Rooms'])\n        df.insert(1, 'DistSqMeterPrice', df.pop('DistSqMeterPrice'))\n        \n        # \u0417\u0430\u043c\u0435\u043d\u044f\u0435\u043c \u043f\u0440\u043e\u043f\u0443\u0441\u043a\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u043e\u043b\u0443\u0447\u0438\u043b\u0438\u0441\u044c \u0438\u0437-\u0437\u0430 \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u0432 \u0442\u0435\u0441\u0442\u0435 \u0438 \u0442\u0440\u0435\u0439\u043d\u0435 \u0435\u0441\u0442\u044c \u043d\u0435\u0441\u043e\u0432\u043f\u0430\u0434\u0430\u044e\u0449\u0438\u0435 \u043f\u043e Id \u0440\u0430\u0439\u043e\u043d\u044b,\n        # \u043d\u0430 \u0441\u0440\u0435\u0434\u043d\u044e\u044e \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u043a\u0432.\u043c \u0432 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u043f\u043b\u043e\u0449\u0430\u0434\u0438\n        \n        df.DistSqMeterPrice.fillna(df.RoomSqMeterPrice, inplace=True)\n        \n        # \u0423\u0434\u0430\u043b\u044f\u0435\u043c \u043b\u0438\u0448\u043d\u0435\u0435\n        \n        df.drop('RoomSqMeterPrice', axis=1, inplace=True)\n        \n        # \u041f\u0435\u0440\u0435\u0432\u043e\u0434\u0438\u043c DistrictId, \u0438 Id \u0432 \u043d\u0435\u0447\u0438\u0441\u043b\u043e\u0432\u044b\u0435 \u043f\u0440\u0438\u0437\u043d\u0430\u043a\u0438\n        \n        df['DistrictId']=df['DistrictId'].astype('object')\n        df['Id']=df['Id'].astype('object')\n        \n        return df","8a9a124f":"generator=FeaturesGenerator()\ngenerator.fit(data)\nDF=generator.transform(data)\nX_test=generator.transform(X_test)","3f4dae49":"DF.info()","e457eb9c":"DF.shape, X_test.shape","082e6360":"y_train=DF.Price\nX_train=DF.drop(['Id', 'Price'], axis=1)\n# \u0421\u0442\u043e\u043b\u0431\u0435\u0446 'Id' \u0438\u0437 \u0442\u0435\u0441\u0442\u043e\u0432\u044b\u0445 \u0434\u0430\u043d\u043d\u044b\u0445 \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u043c, \u043e\u043d \u0431\u0443\u0434\u0435\u0442 \u043d\u0443\u0436\u0435\u043d \u0434\u043b\u044f \u0444\u0430\u0439\u043b\u0430 \u0441 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u043e\u043c\ntest_idx=X_test.pop('Id')","e3d47f36":"X_train.shape, X_test.shape, y_train.shape","fd6bfae1":"%%time\nfrom sklearn.ensemble import GradientBoostingRegressor\nmodel=GradientBoostingRegressor(learning_rate=0.02, max_depth=5, max_features=0.5, n_estimators=700, random_state=24 )\nmodel.fit(X_train, y_train)\npred=model.predict(X_test)\n","2dba276f":"model.score(X_train,y_train)","0ead0c2c":"sns.jointplot(x=model.predict(X_train), y=y_train, kind='reg', height=12)\nplt.show()","c151316b":"plt.figure(figsize=(16,8))\nplt.barh(X_train.columns.tolist(), model.feature_importances_)\nplt.xlabel('\u0412\u0435\u0441 \u043f\u0440\u0438\u0437\u043d\u0430\u043a\u0430')\nplt.ylabel('\u041f\u0440\u0438\u0437\u043d\u0430\u043a')\nplt.show()","98afde5a":"result=pd.DataFrame({'Id':test_idx, 'Price':pred})\nresult.head()","78491cde":"result.to_csv(RESULT_PATH, index=False)","c9089892":"### 4. \u0412\u044b\u0431\u043e\u0440 \u0438 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u0435 \u043c\u043e\u0434\u0435\u043b\u0438","475d9669":"\u0414\u0430\u0442\u0430\u0441\u0435\u0442 \u044f\u0432\u043d\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u0435\u043d \u043c\u0443\u0441\u043e\u0440\u043d\u044b\u043c\u0438 \u0434\u0430\u043d\u043d\u044b\u043c\u0438, \u0442\u0440\u0443\u0434\u043d\u043e \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u0442\u044c \u0441\u0435\u0431\u0435 \u0436\u0438\u043b\u044b\u0435 \u043f\u043e\u043c\u0435\u0449\u0435\u043d\u0438\u044f \u043f\u043b\u043e\u0449\u0430\u0434\u044c\u044e 6 \u043a\u0432. \u043c.<br>\u0426\u0435\u043d\u0430 \u0437\u0430 \u043a\u0432.\u043c \u044