ch',data=train_data)","b544578e":"train_data.head(10)","a5791e48":"sns.violinplot(x=\"Survived\", y=\"Age\", data = train_data, size = 9)","1ece0bf8":"sns.countplot(x=\"Survived\",hue = \"Embarked\",data=train_data)","97531e45":"train_data['Sex'] = train_data['Sex'].map( {'female': 1, 'male': 0} ).astype(int)\ntest_data['Sex']  = test_data['Sex'].map( {'female': 1, 'male': 0} ).astype(int)","84711768":"train_data.head(10)","b870ce55":"emb_dummy = pd.get_dummies(train_data['Embarked'])\ntrain_data = pd.concat([train_data,emb_dummy], axis = 1)","79df93bb":"emb_dummy2 = pd.get_dummies(test_data['Embarked'])\ntest_data = pd.concat([test_data,emb_dummy2], axis = 1)","359d4018":"drop_cols = ['Name','Ticket','Fare','Embarked']\ntrain_data = train_data.drop(drop_cols, axis = 1)\ntrain_data = train_data.drop(['PassengerId'],axis=1)\ntest_data = test_data.drop(drop_cols, axis = 1)","b53f2d29":"train_data.head(10)","3a77c080":"X_train = train_data.drop(['Survived'], axis = 1).values\nY_train = train_data['Survived'].values","022b74e6":"from sklearn.model_selection import train_test_split\nx_train,x_test,y_train,y_test=train_test_split(X_train,Y_train,test_size=0.20)","5733ef91":"from sklearn.linear_model import LogisticRegression\nmodel=LogisticRegression()\nmodel.fit(x_train,y_train)","24f9254c":"pred = model.predict(x_test)","e29c20e9":"from sklearn.metrics import accuracy_score\naccuracy = accuracy_score(y_test,pred)\naccuracy","502d1ede":"final_model=LogisticRegression()\nfinal_model.fit(X_train,Y_train)     # On complete training data","1e66beeb":"test_data.head(10)","e60ce04a":"test = test_data.drop(['PassengerId'],axis = 1)\nfinal_pred = final_model.predict(test)","a31ca069":"test_data['Survived'] = final_pred\ntest_data.drop(['Pclass','Age','Sex','SibSp','Parch','C','Q','S'],inplace=True,axis=1)\ntest_data.head()","72858df2":"test_data.to_csv('Titanic_submission.csv', index=False)","96ee992b":"## Fitting The Model","3050bee7":"## Handling categorical variables","e8caf2