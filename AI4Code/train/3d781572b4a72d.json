ab6c8":"data","77db8969":"data[(data.Sex==\"female\")&(data.Pclass==1)]['Age'].mean()","b44911d7":"sex = pd.get_dummies(data['Sex'],drop_first=True)\nsex","8e560a4e":"embark = pd.get_dummies(data['Embarked'],drop_first=True)\nembark","8a79d766":"pcl = pd.get_dummies(data['Pclass'],drop_first=True)\npcl","4f66e92e":"titanic = pd.concat([data,sex,embark,pcl],axis=1)\ntitanic","3cd5c1a0":"titanic.isnull().sum()","b49e28d3":"data.isnull().sum()","922e88c5":"titanic.drop(['Pclass','Embarked','Sex','Name','Ticket'],axis=1,inplace=True)","88897f0c":"titanic\n","e5264312":"data=data.drop('PassengerId',axis=1)","53806225":"X = titanic.drop(['Survived', 'PassengerId'],axis=1)\nY = titanic['Survived']","57e692b3":"from sklearn.model_selection import train_test_split","14418098":"x_train,x_test,y_train,y_test = train_test_split(X,Y,test_size=0.3,random_state=1)","25a30d24":"from sklearn.linear_model import LogisticRegression\nfrom sklearn.ensemble import RandomForestClassifier \nfrom sklearn.neighbors import KNeighborsClassifier\nfrom sklearn.naive_bayes import GaussianNB\nfrom sklearn.tree import DecisionTreeClassifier\nfrom sklearn.metrics import classification_report\nfrom sklearn.metrics import accuracy_score\nfrom sklearn.metrics import confusion_matrix\nfrom sklearn.svm import SVC","9660a295":"log_model = LogisticRegression(max_iter=1000)\nrandom = RandomForestClassifier()\nknn = KNeighborsClassifier()\ngauss = GaussianNB()\nd_tree = DecisionTreeClassifier()\nsvc = SVC()\nmodels = [log_model,random,knn,gauss,d_tree,svc]","58212c26":"acc = []\nfor model in models:\n    print(model,\"\\n\")\n    x_train,x_test,y_train,y_test = train_test_split(X,Y,test_size=0.3,random_state=1)\n    model.fit(x_train,y_train)\n    predictions = model.predict(x_test)\n    print(classification_report(y_test,predictions))\n    print(\"Confusion Matrix\\n\",confusion_matrix(y_test,predictions))\n    acc = np.append(acc,accuracy_score(y_test,predictions))\n    print(\"\\n---------------------------------------------------------\\n\")","fed993cb":"for i in range(len(acc)):\n    print(models[i],\":\",acc[i])","a4583784":"## Analyze","5d65a492":"      1    2      3\nmale 41   30     26\nfe   34   28     21\n","44775e47":"# Mean","8b6f0520":"## Data Wrangling","473f1acf":"      1    2      3\nmale 41   30     26\nfe   34   28     21\n","ce7b1cfd":"### Train Data"}}