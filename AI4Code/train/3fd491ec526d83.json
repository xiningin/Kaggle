val_score(model, features_train, target_train, cv=k_fold, n_jobs=1, scoring=scoring)\nprint(accuracy_knn)","4c2a3749":"# kNN Score\naccuracy_knn=round(np.mean(accuracy_knn)*100, 2)\nprint(accuracy_knn, '\u043f\u0440\u043e\u0446\u0435\u043d\u0442\u043e\u0432')","f0790578":"model = DecisionTreeClassifier()\nscoring = 'accuracy'\naccuracy_decisiontree = cross_val_score(model, features_train, target_train, cv=k_fold, n_jobs=1, scoring=scoring)\nprint(accuracy_decisiontree)","c91ece50":"# decision tree Score\naccuracy_decisiontree=round(np.mean(accuracy_decisiontree)*100, 2)\nprint(accuracy_decisiontree, '\u043f\u0440\u043e\u0446\u0435\u043d\u0442\u043e\u0432')","5cfa8b43":"model = RandomForestClassifier(n_estimators=100, max_features=5, max_depth=5)\nscoring = 'accuracy'\naccuracy_randomforest = cross_val_score(model, features_train, target_train, cv=k_fold, n_jobs=1, scoring=scoring)\nprint(accuracy_randomforest)","3c76878d":"# Random Forest Score\naccuracy_randomforest=round(np.mean(accuracy_randomforest)*100, 2)\nprint(accuracy_randomforest, '\u043f\u0440\u043e\u0446\u0435\u043d\u0442\u043e\u0432')","de93f95e":"from lightgbm import LGBMClassifier\nmodel = LGBMClassifier(n_estimators=100, max_depth=5)\nscoring = 'accuracy'\naccuracy_lgbm = cross_val_score(model, features_train, target_train, cv=k_fold, n_jobs=1, scoring=scoring)\nprint(accuracy_lgbm)","0dc019c8":"# LightGBM\naccuracy_lgbm=round(np.mean(accuracy_lgbm)*100, 2)\nprint(accuracy_lgbm, '\u043f\u0440\u043e\u0446\u0435\u043d\u0442\u043e\u0432')","a689559a":"from xgboost import XGBClassifier\nmodel = XGBClassifier(n_estimators=100, max_depth=5)\nscoring = 'accuracy'\naccuracy_xgboost = cross_val_score(model, features_train, target_train, cv=k_fold, n_jobs=1, scoring=scoring)\nprint(accuracy_xgboost)","2ecfac99":"# LightGBM\naccuracy_xgboost=round(np.mean(accuracy_xgboost)*100, 2)\nprint(accuracy_xgboost, '\u043f\u0440\u043e\u0446\u0435\u043d\u0442\u043e\u0432')","bde5552d":"model = RandomForestClassifier(n_est