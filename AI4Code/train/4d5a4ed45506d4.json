e_mean\",data=data,col=\"diagnosis\",color=\"green\")","272bca7a":"sns.pairplot(data=data,hue=\"diagnosis\")","11b1dc49":"sns.catplot(y=\"radius_mean\",x=\"diagnosis\",kind=\"box\",data=data)","e3628312":"sns.catplot(y=\"texture_mean\",x=\"diagnosis\",kind=\"swarm\",data=data)","0065fd6a":"plt.figure(figsize=(7,7))\nsns.violinplot(x=\"diagnosis\",y=\"area_mean\",data=data)","b17b6bae":"sns.countplot(x=\"diagnosis\",data=data,label=\"count\",palette={\"blue\",\"red\"})","f0aecf5d":"df.head()","924808b5":"data1=df.drop([\"id\",\"diagnosis\"],axis=\"columns\")","3366523d":"data1","25b5b6d8":"target=df[\"diagnosis\"]","013124fa":"target","5b7c95eb":"features=(data1-data1.mean())\/(data1.std())","5b90083a":"features","9414cdc6":"cancer=pd.concat([target,features.iloc[:,4:10]],axis=1)","616f73e8":"cancer=pd.melt(cancer,id_vars=\"diagnosis\",var_name=\"features\",value_name=\"values\")","19d43ce7":"plt.figure(figsize=(12,10))\nsns.violinplot(x=\"features\",y=\"values\",data=cancer,hue=\"diagnosis\",split=True,inner=\"quart\",aspect=10,palette={\"M\":\"red\",\"B\":\"blue\"})\nplt.grid()","6488c5a8":"cancer1=pd.concat([target,features.iloc[:,11:]],axis=1)\ncancer1=pd.melt(cancer1,id_vars=\"diagnosis\",var_name=\"features\",value_name=\"values\")\n","fdd1fb8b":"sns.catplot(x=\"features\",y=\"values\",data=cancer1,kind=\"box\",aspect=5)","83e7afbe":"plt.figure(figsize=(10,20))\nsns.catplot(x=\"features\",y=\"values\",data=cancer,kind=\"boxen\",aspect=2,hue=\"diagnosis\")\n","5c44f00d":"features","890bb7e8":"target","7a092480":"from sklearn.model_selection import train_test_split","a965c839":"x_train,x_test,y_train,y_test=train_test_split(features,target,test_size=0.2)","9e597887":"from sklearn.ensemble import RandomForestClassifier","3510e72c":"model=RandomForestClassifier()","ad57e91b":"model.fit(x_train,y_train)","7e624f0c":"result=model.predict(x_test)","1662531c":"model.score(x_test,y_test)","9b6b1eb7":"from sklearn.metrics import confusion_matrix","618021f9":"cm=confusion_matrix(y_test,result)","988511f9":"cm","d93c65b7":"sns.heatmap(cm,annot=True)\nplt.xlabel(\"actual result\")\nplt.ylabel(\"predicted result\")","a7dedb26":"# its a boxen plot which plot 6 more features by seprately on different type cancer","19e26b56":"# its a box plot of radius_mean on two type of diagnosis","b1cb3c3e":"# its a swarm plot of texture_mean on two kind of cancer","44d501a1":"# its a violin plot of area_mean on two typw of cancer","0b99fa60":"# here 569 peoples records are taken","0af4fbd8":"# its a scatter plot between radius and area of individual type cancer","b90b8eac":"# Scatter plot between radius and parameter mean.... it shows that bengin type cancer has less radius and less parameter whereas \n# maligant has larger radius  ","fc88bb93":"# hence benign type cancer is 357 and maligant type is 212","1dc6d206":"# its a violin plot on other 6 features of cancer type ","f85e158d":"# its a pair plot between four parameter and it show there realtionship to each other","e2a0d225":"# prediction","b79c17d2":"# its a count plot which count no. of M daiagnosis and no. of B diagnosis","2fd52468":"# its a individual scatter plot on radius and texture mean of two type of cancer","da179bd9":"# its a scatter plot between radius and texture_mean"}}