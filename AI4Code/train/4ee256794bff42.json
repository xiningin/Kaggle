{"cell_type":{"2dec905f":"code","2f3a1751":"code","d304033a":"code","319e60ab":"code","ec4a864c":"code","bd618695":"code","4f599338":"code","6dc8f789":"code","f8b6da45":"code","2252c346":"code","69b669fe":"code","229682ae":"code","adf67b10":"code","b9c46c04":"markdown"},"source":{"2dec905f":"import numpy as np\nimport pandas as pd \nimport matplotlib.pyplot as plt\nplt.style.use(\"fivethirtyeight\")\nseries=pd.read_csv(\"\/kaggle\/input\/avatar-the-last-air-bender\/series_names.csv\")\nseries.head()","2f3a1751":"series.halfway.plot(kind=\"pie\",figsize=(10,10),autopct=\"%1.1f%%\",explode=(0,0.05,0),shadow=True)","d304033a":"data=pd.read_csv(\"..\/input\/avatar-the-last-air-bender\/avatar_data.csv\")\ndata.head()","319e60ab":"from sklearn.preprocessing import LabelEncoder\nlab=LabelEncoder()\ndata.book=lab.fit_transform(data.book)","ec4a864c":"import seaborn as sns\nsns.jointplot(data.book,data.imdb_rating,kind=\"kde\")","bd618695":"plt.figure(figsize=(15,9))\nsns.lineplot(data.imdb_rating, data.series_rating, color=\"g\")","4f599338":"scenes=pd.read_csv(\"..\/input\/avatar-the-last-air-bender\/scenes.csv\", encoding= 'unicode_escape')\nscenes.head()","6dc8f789":"def sentence(x):\n    a=x.split('[')\n    b=a[1].split(']')\n    return b[0]\nscenes[\"scene_description\"]=scenes[\"scene_description\"].apply(sentence)","f8b6da45":"scenes.head()","2252c346":"import re\ndef process(x):\n    processed_tweet = re.sub(r'\\W', ' ', str(x))\n    processed_tweet = re.sub(r'\\s+[a-zA-Z]\\s+', ' ', processed_tweet)\n    processed_tweet = re.sub(r'\\^[a-zA-Z]\\s+', ' ', processed_tweet) \n    processed_tweet= re.sub(r'\\s+', ' ', processed_tweet, flags=re.I)\n    processed_tweet = re.sub(r'^b\\s+', '', processed_tweet)\n    processed_tweet = processed_tweet.lower()\n    return processed_tweet\nscenes.scene_description=scenes.scene_description.apply(process)","69b669fe":"import nltk\nfrom nltk.stem import PorterStemmer,LancasterStemmer\nstemming =PorterStemmer()\ndef identify_tokens(row):\n    tokens = nltk.word_tokenize(row)\n    token_words = [w for w in tokens if w.isalpha()]\n    return token_words\ndef stem_list(row):\n    stemmed_list = [stemming.stem(word) for word in row]\n    return (stemmed_list)\ndef rejoin_words(row):\n    joined_words = ( \" \".join(row))\n    return joined_words\nscenes.scene_description=scenes.scene_description.apply(identify_tokens)\nscenes.scene_description=scenes.scene_description.apply(stem_list)\nscenes.scene_description=scenes.scene_description.apply(rejoin_words)","229682ae":"from textblob import TextBlob\nscenes.scene_description=scenes.scene_description.apply(lambda x:TextBlob(x).sentiment.subjectivity)","adf67b10":"scenes.head()","b9c46c04":"# AVATAR THE LAST AIR-BENDER\n\n<img src=\"data:image\/jpeg;base64,\/9j\/4AAQSkZJRgABAQAAAQABAAD\/2wCEAAkGBxISEhUTEhMWFhUXGB4bGBgYGBgdHRggHRoeHRsgHxgdHSgiHx0lHh0eIjEiJSkrLi4uGB8zODMsNygtLisBCgoKDg0OGxAQGy8mICYtLS0yLjUtLS0tMi8vLy81LTUtMi0tLS8tLy8tLS0tLy8tLS0tLS0tLS0tLS0tLS0tLf\/AABEIARMAtwMBIgACEQEDEQH\/xAAcAAACAwEBAQEAAAAAAAAAAAAFBgMEBwACAQj\/xABMEAACAgAEAwYCBgcDCQcFAQABAgMRAAQSIQUxQQYTIlFhcTKBFCNCkaGxB1JicsHR8DOC4RckQ1OSlKLU8RUWVFVjZHOTpLKzxDT\/xAAaAQACAwEBAAAAAAAAAAAAAAADBAECBQYA\/8QAMxEAAgIBAwIEBAUFAAMBAAAAAQIAAxEEEiExQQUTIlEyYXHwgZGhsdEUI0LB4UNTYgb\/2gAMAwEAAhEDEQA\/AKK55ftL92+JUaE8iB+GKs2W0\/aOK0sqrz2wbaD0i+8jrCj5GJxWxHlf8MUp+z\/+revRtx9\/Mfjih\/2jF+t+BxNFxReklff\/ABxIVh0kF1PUSvmMjInxIa8x4h+G4+YGKwAPLDHluLHrTeoO+PbcPgm3XZutbH7uR\/HFg5HWVKA9DFox4893g\/L2db7D36MP4j+WBeayUkXxqQPPmD8x\/HBAwPSUZGHUSvHCWIVRZJoAcycfHiIJBFEGiD0OGTJPlQEB0DSsb699WvX4xq8gANvXEh+iGvgI1AmwxcnXuSf1NPn0widaQ2PLbH0jY0YK53r+cVdGPcmXZQpZSAwtb6jzGGxfoV7CKq21A\/rHVe3lWn0xwmyrBBaHSiqBICQq6vEP39PLFD4g3atsfSX\/AKFf\/YufrFJMuzaioJ0i2roPM4jK4bYZsqkbBaCsNLCj3htvP9XTinneI8PWXQGgEmhtOq+6Bsae8AGzVf8AVYn+vIJzW2O3H7yP6EEDFgz9f2i2wwK4jkXeS61LVAAjbz5kbe2HBuLcM3VWj0P3upjq1IVru9PkreIj5YS+KcZq0iIO27j2+z\/M4MmoS5TkEY94JtO9LDBBkOZ4WS3KlrkKJB+\/YYny2RC6dqogk7Wa6Dr8uWIuz6ktJY6beu++Ck4Cgk7AbnBqqVYboG7UOpK4EhdrOBOa4lu