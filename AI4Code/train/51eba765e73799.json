{"cell_type":{"fab56c15":"code","a072b031":"code","0371e1bb":"code","9c528fcf":"code","53869613":"code","ebd7e124":"code","a2cc73b8":"code","9f977f2f":"code","d86f1801":"code","ceb06b21":"code","1621736e":"code","a0cf6c9f":"code","1726f64f":"code","0298a0ce":"code","7a1a4d40":"code","bc375c28":"code","b7e39fbc":"code","4cfa9e97":"code","badcf995":"code","38f3e29b":"code","9f4c107e":"code","d3de0696":"code","41825ba3":"markdown","7415d6f2":"markdown","a4d2f8ed":"markdown","e4688c1a":"markdown","64136c95":"markdown"},"source":{"fab56c15":"import numpy as np \nimport pandas as pd\nimport math\nfrom sklearn.model_selection import train_test_split\nfrom sklearn.ensemble import RandomForestRegressor,IsolationForest,GradientBoostingRegressor,ExtraTreesRegressor\nimport os\nfrom sklearn.metrics import mean_absolute_error,mean_squared_error,mean_squared_log_error","a072b031":"train = pd.read_csv('..\/input\/house-prices-advanced-regression-techniques\/train.csv')\ntest = pd.read_csv('..\/input\/house-prices-advanced-regression-techniques\/test.csv')\nsample = pd.read_csv('..\/input\/house-prices-advanced-regression-techniques\/sample_submission.csv')","0371e1bb":"train","9c528fcf":"train.corr()['SalePrice']","53869613":"def string_remover(df,list1=[],drop=[]):\n    a = df.select_dtypes(include='object')\n    for i in a.columns:\n        for x in a.index:\n            try:\n                c = list1.index(a[i].iloc[x:x+1][x])\n                a[i].iloc[x:x+1][x] = c\n            except:\n                list1.append(a[i].iloc[x:x+1][x])\n                a[i].iloc[x:x+1][x] = len(list1)-1\n    a.fillna(len(list1))\n    d = df.select_dtypes(exclude='object').fillna(0)\n    try:\n        return pd.concat([d,a],axis=1).fillna(0).drop([drop],axis=1)\n    except:\n        return pd.concat([d,a],axis=1).fillna(0)","ebd7e124":"b = []","a2cc73b8":"train = string_remover(train,list1=b)","9f977f2f":"test = string_remover(test,list1=b)","d86f1801":"X_train,X_test,y_train,y_test = train_test_split(train.drop('SalePrice',axis=1),train['SalePrice'],test_size=0.33,random_state=500)","ceb06b21":"forest = ExtraTreesRegressor()","1621736e":"from sklearn.feature_selection import SelectFromModel","a0cf6c9f":"select_from_model = SelectFromModel(forest)","1726f64f":"select_from_model.fit(X_train,y_train)","0298a0ce":"selected_features = X_train.columns[(select_from_model.get_support())]\nselected_features","7a1a4d40":"selected_features","bc375c28":"forest.fit(X_train[selected_features],y_train)","b7e39fbc":"i = 10\nprint(f'Predicted Value : {(forest.predict(X_test[selected_features].iloc[i-1:i]))[0]}')\nprint(f'Actual Value: {y_test.iloc[[i-1]][X_test[selected_features].iloc[i-1:i].index[0]]}')","4cfa9e97":"print(f'Root Mean Squared Errors : {math.sqrt(mean_squared_error(forest.predict(X_test[selected_features]),y_test))}') ","badcf995":"pred = forest.predict(test[selected_features])","38f3e29b":"sample['SalePrice'] = pd.Series(pred)","9f4c107e":"sample.to_csv('submission.csv',index=False)","d3de0696":"sample","41825ba3":"<html>\n    <h1 align=center><i><font face='Courier' size = 10>Importing Libraries<\/font><\/i><\/h1>","7415d6f2":"<html>\n    <h1 align=center><i><font face='Courier' size = 10>Train Test Split\n        and the Model<\/font><\/i><\/h1>","a4d2f8ed":"<html>\n    <h1 align='center'><b>\n    <img src='https:\/\/media3.giphy.com\/media\/KAq5w47R9rmTuvWOWa\/giphy.gif' width=100 height=100>\n    <font color='sky blue' face='lucida handwriting'size=300>Py<\/font><font color='yellow' face='lucida handwriting' size=300>Tech<\/font><\/b><\/h1>","e4688c1a":"<html>\n    <h1 align=center><i><font face='Courier' size = 10>Data Cleaning<\/font><\/i><\/h1>","64136c95":" <html>\n    <h1 align=center><i><font face='Courier' size = 10>Prediction and Submission file<\/font><\/i><\/h1>"}}