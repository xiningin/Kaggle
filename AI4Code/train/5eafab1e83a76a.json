m\n\nfrom sklearn.svm import SVC","705cd612":"sv=SVC()","90810796":"sv.fit(xtrain,ytrain)","26d8cd09":"sv.score(xtest,ytest)","bcc77675":"##Decission Tree\n\nfrom sklearn.tree import DecisionTreeClassifier","ad6502dc":"dt= DecisionTreeClassifier()","fac5f9d9":"dt.fit(xtrain,ytrain)","b4557cc5":"dt.score(xtest,ytest)","91535571":"import seaborn as sns\n","4cfb530a":"df=pd.read_csv('..\/input\/heart-diseases\/datasets_4123_6408_framingham.csv')","65ad3c55":"df.head(2)","eac5c5db":"sns.countplot(df['TenYearCHD'],palette=['#137909','#ff0707'])","b42ab79b":"sns.kdeplot(df['glucose'], shade=True)","5be03511":"sns.countplot(df['currentSmoker'],palette=['#137909','#ff0707'])","2064a799":"df['currentSmoker'].value_counts()","f60665f4":"sns.countplot(x='age',hue='TenYearCHD',data=df,palette=['#137909','#ff0707'],edgecolor=sns.color_palette('dark',n_colors=1))","d38ad227":"sns.countplot(x='sysBP',hue='TenYearCHD',data=df,palette=['#137909','#ff0707'])","6d055923":"sns.jointplot(\"heartRate\", \"totChol\", data=df, kind='reg');\n","5e8040b6":"from sklearn.ensemble import ExtraTreesClassifier","06fe5dd2":"et=ExtraTreesClassifier()","f79f6af5":"et.fit(x,y)","1e230ace":"et.feature_importances_","300fef90":"top=pd.Series(et.feature_importances_,index=x.columns)","1b27d9ad":"result=top.nlargest(10)","0a5f5c87":"result.plot()","96770538":"result.plot(kind='bar')","c04e9d60":"result","e4f03704":"result.plot(kind='pie')","9e3458ad":"result","1d4ec733":"x=df.drop(['prevalentHyp','currentSmoker','male','BPMeds','diabetes'],axis=1)","537d1d01":"from sklearn.ensemble import RandomForestClassifier","ce0c4ceb":"rf=RandomForestClassifier()","6b9d85f6":"rf.fit(xtrain,ytrain)","6b4d285e":"rf.score(xtest,ytest)","b6071624":"<h1>Data Visualization <h2>","cdca1eb8":"<h1>Cardio Diseases Rate According to Age <\/h1>\n","5956dbbb":"* <h1>Feature Importance<\/h2>","354de1a3":"<img src=\"https:\/\/biox.stanford.edu\/sites\/g\/files\/sbiybj7941\/f\/cardiovascular_banner_highlight.png\"\/>","342c7702":"<