{"cell_type":{"3d6d9dd1":"code","3f0e8178":"code","05280577":"code","3b61d271":"code","3f0cb31d":"code","2b4300b4":"code","a9905eb0":"code","bfb095da":"code","4c289924":"code","d395bfb2":"code","fdb38ba0":"code","af9b71e4":"code","b0943daf":"code","a6be624a":"code","9e3c4f17":"code","3373301b":"code","e68fb230":"code","5debff31":"code","87171ffc":"markdown","c0bc66d4":"markdown","634cace5":"markdown","33e8b812":"markdown","460af7b6":"markdown","64bc8af7":"markdown","0bac1494":"markdown","fe249511":"markdown","e2832fc4":"markdown","3c0955e3":"markdown","4ec5a4f1":"markdown","bafa4fd7":"markdown","ea2e81eb":"markdown","0c439cf4":"markdown","c541efdc":"markdown"},"source":{"3d6d9dd1":"!pip install pycaret[full]","3f0e8178":"import numpy as np\nimport pandas as pd\nfrom pycaret.regression import *","05280577":"train = pd.read_csv(\"\/kaggle\/input\/tabular-playground-series-aug-2021\/train.csv\")\ntest = pd.read_csv(\"\/kaggle\/input\/tabular-playground-series-aug-2021\/test.csv\")\nsubmit_file = pd.read_csv(\"\/kaggle\/input\/tabular-playground-series-aug-2021\/sample_submission.csv\")","3b61d271":"train.head()","3f0cb31d":"test.head()","2b4300b4":"submit_file.head()","a9905eb0":"train.drop(columns= \"id\", inplace = True)\ntest.drop(columns= \"id\", inplace = True)","bfb095da":"setup(data = train, target= \"loss\",silent = True, session_id = 2021, use_gpu = True)","4c289924":"xgboost = create_model(\"xgboost\")","d395bfb2":"lgbm = create_model(\"lightgbm\")","fdb38ba0":"catboost = create_model(\"catboost\")","af9b71e4":"xgboost_tuned = tune_model(xgboost, optimize= \"RMSE\")","b0943daf":"lgbm_tuned = tune_model(lgbm, optimize = \"RMSE\")","a6be624a":"catboost_tuned = tune_model(catboost, optimize = \"RMSE\")","9e3c4f17":"blended_models = blend_models([xgboost_tuned,lgbm_tuned, catboost_tuned], optimize= \"RMSE\")","3373301b":"final_model = finalize_model(blended_models)","e68fb230":"submit_file[\"loss\"] = final_model.predict(test)","5debff31":"submit_file.to_csv(\"su