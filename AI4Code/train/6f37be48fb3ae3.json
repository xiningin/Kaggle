gure(figsize=(15,8))\nplt.title('Trtbps VS Count',size=20)\nsns.histplot(df['trtbps'],bins=115)\nplt.xticks(list(range(90,205,5)))\n#plt.yticks(list(range(0,20,1)))\nplt.show()","1af28206":"plt.figure(figsize=(15,8))\nplt.title('Chol VS Count',size=20)\nsns.histplot(df['chol'])\n#plt.xticks(list(range(29,79,1)))\n#plt.yticks(list(range(0,20,1)))\nplt.show()","8c8052d3":"plt.figure(figsize=(15,8))\nplt.title('Thalachh VS Count',size=20)\nsns.histplot(df['thalachh'])\nplt.xticks(list(range(70,210,5)))\nplt.yticks(list(range(0,60,2)))\nplt.show()","40b423d0":"fbs=df['fbs'].value_counts()\nplt.title('Finding the FBS',size=20)\nsns.barplot(x=fbs.index,y=fbs.values)\nprint(df.fbs.value_counts())\n","1cc8ffb6":"plt.figure(figsize=(6,4))\nfbs=df['fbs'].value_counts().reset_index()\npx.pie(fbs,names='index',values='fbs')","ecdc86ce":"restecg=df['restecg'].value_counts()\nplt.title('Resting Electrocardiographic Results',size=20)\nsns.barplot(x=restecg.index,y=restecg.values)\nplt.show()\nprint(df.restecg.value_counts())","ae29a13a":"exng=df['exng'].value_counts()\nplt.title('Exercise induced angina (1 = yes; 0 = no)',size=20)\nsns.barplot(x=exng.index,y=exng.values)\nplt.show()\nprint(df.exng.value_counts())","63742f1b":"output=df['output'].value_counts()\nplt.title(' 0= less chance of heart attack 1= more chance of heart attack ',size=20)\nsns.barplot(x=output.index,y=output.values)\nplt.show()\nprint(df.output.value_counts())","95e4abd6":"thall=df['thall'].value_counts()\nplt.title('Thall vs Counts')\nsns.barplot(x=thall.index,y=thall.values)\nplt.xlabel('Thall',size=20)\nplt.ylabel('Counts',size=20)\nprint(df.thall.value_counts())","263eaed5":"caa=df['caa'].value_counts()\nplt.title('CAA vs Counts')\nsns.barplot(x=caa.index,y=caa.values)\nplt.xlabel('CAA',size=20)\nplt.ylabel('Counts',size=20)\nprint(df.caa.value_counts())","c6e606d2":"slp=df['slp'].value_counts()\nplt.title('SLP vs Counts')\nsns.barplot(x=slp.index,y=slp.values)\nplt.xlabel('SLP',size=20)\nplt.ylabel('Counts',size=20)\nprint(df.slp.value_counts())","00ff920d":"df[['age','output']].value_counts().sort_values()","fa34b74f":"A=pd.crosstab(df['age'],df['output']).reset_index()\nA.columns=['age','lowrisk','highrisk']\nA.head(10)","d7deba45":"px.line(A,A['age'],A['highrisk'],range_x=(25,80),title='Age with High chance of getting stroke')\n\n","176ae51a":"px.line(A,A['age'],A['lowrisk'],range_x=(25,80),title='Age with Low chance of getting stroke')\n","f0a48d8f":"plt.figure(figsize=(10,8))\nsns.lmplot(x='age',y='trtbps',hue='output',data=df)\nplt.title('Shows relation between age and Resting blood pressure',size=20)\nplt.show()\n","ad88cddf":"plt.figure(figsize=(10,8))\nsns.lmplot(x='age',y='chol',hue='output',data=df)\nplt.title('Age vs cholestrol with respect to heart attack',size=20)\nplt.show()\n","85067a9e":"plt.figure(figsize=(10,8))\nsns.lmplot(x='age',y='thalachh',hue='output',data=df)\nplt.title('Age vs Maximum heart rate achieved with respect to heart attack',size=20)\nplt.show()\n","17a53fc6":"sns.kdeplot(x='chol',y='output',data=df)","c7a3f3ed":"sns.scatterplot(x='chol',y='output',data=df)","4352107d":"sns.histplot(x='chol',y='output',data=df)","bf60dea1":"sns.swarmplot(y='chol',x='output',data=df)","bcf1005d":"plt.style.use('classic')\nsns.histplot(x='age',data=df,hue='output',palette='pastel')\nplt.title('Age vs stroke analysis',size=20)\nplt.grid()\nplt.show()","c97a7f2a":"``There are 258 false values and 45 true values``","4406efd3":"**Showing unique values in the dataset**","7280b0b0":"`` Data shows the value of Caa \n0    175\n1     65\n2     38\n3     20\n4      5\n``\n","0033137b":"``The data shows that 2 ~ 166   , 3  ~  117 ,  1  ~  18 ,0  ~  2  are the values of thall with thier index``","5925fb17":"``Shows data ranging from 71 to 202``","82fce393":"``At age 58 it the most chance of getting a heart stroke while it increases with increase in age``","94b73e3e":"<img src='data:image\/png;base64,iVBORw0KGgoAAAANSUhEUgAAASsAAACoCAMAAACPKThEAAABiVBMVEX\/\/\