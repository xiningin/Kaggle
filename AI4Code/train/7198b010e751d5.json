down","9bc591b5":"markdown"},"source":{"2ed344e6":"import numpy as np\nimport pandas as pd\nimport seaborn as sns\nimport matplotlib.pyplot as plt","b7cbabac":"df = pd.read_csv(\"..\/input\/indian-food-101\/indian_food.csv\")\ndf.head()","f0754881":"df.info()","4f03fdb2":"df[\"prep_time\"] = df[\"prep_time\"]\/60\ndf[\"cook_time\"] = df[\"cook_time\"]\/60","034d1b34":"df[\"diet\"] = pd.get_dummies(df[\"diet\"]).iloc[:, -1]","b2f83699":"df = df.join(pd.get_dummies(df[\"course\"], prefix=\"course\", prefix_sep=\"_\"))\ndf = df.drop(columns=[\"course\"])","8383065a":"df = df.join(pd.get_dummies(df[\"flavor_profile\"], prefix=\"flavour\", prefix_sep=\"_\"))\ndf = df.drop(columns=[\"flavor_profile\"])\ndf = df.drop(columns=[\"flavour_-1\"])","68039805":"df = df.join(pd.get_dummies(df[\"state\"], prefix=\"state\", prefix_sep=\"_\"))\ndf = df.drop(columns=[\"state\"])\ndf = df.drop(columns=[\"state_-1\"])","9026e837":"df = df.join(pd.get_dummies(df[\"region\"], prefix=\"region\", prefix_sep=\"_\"))\ndf = df.drop(columns=[\"region\"])\ndf = df.drop(columns=[\"region_-1\"])","eb92e752":"temp = df[\"ingredients\"].str.split(\",\",expand=True)\ntemp.columns = ['ingredient_{}'.format(i) for i in range(1, 11)]\ntemp =temp.replace(np.nan, \"\")","116c1978":"ingredients = pd.DataFrame(np.reshape(temp.values, 2550), columns=[\"ingredients\"])\ningredients = ingredients[\"ingredients\"]\n\nfrom sklearn.preprocessing import LabelEncoder\nle = LabelEncoder()\nle.fit(ingredients)\n\nfor i in range(1, 11):\n    temp[\"ingredient_{}\".format(i)] = le.transform(temp[\"ingredient_{}\".format(i)])\n    \ndf = df.join(temp)\ndf = df.drop(columns=[\"ingredients\"])","bc6fe7f4":"df.info()","99ad8e31":"def top_ingredient(state):\n    temp = pd.DataFrame((df[\"state_{}\".format(state)]*df[\"ingredient_1\"]).value_counts().iloc[1:7])\n    for i in range(2, 11):\n        temp = temp.add(pd.DataFrame((df[\"state_{}\".format(state)]*df[\"ingredient_{}\".format(i)]).value_counts().iloc[1:7]), fill_value=0)\n    temp.plot(kind='bar')","f065dad4":"def top_flavour(state):\n    print(\"flavour_sweet\", (df[\"flavour_sweet\"]*df[\"state_{}\".format(state)]).replace(0,np.NaN).value_counts())\n    print(\"flavour_bitter\", (df[\"flavour_bitter\"]*df[\"state_{}\".format(state)]).replace(0,np.NaN).value_counts())\n    print(\"flavour_sour\", (df[\"flavour_sour\"]*df[\"state_{}\".format(state)]).replace(0,np.NaN).value_counts())\n    print(\"flavour_spicy\", (df[\"flavour_spicy\"]*df[\"state_{}\".format(state)]).replace(0,np.NaN).value_counts())","d287ae44":"top_ingredient(\"Andhra Pradesh\")","733540bf":"le.inverse_transform([402, 146, 122, 254])","47ed37b9":"top_flavour(\"Andhra Pradesh\")","7f588498":"top_ingredient(\"Assam\")","22468adf":"le.inverse_transform([184, 21, 269, 271, 319, 402])","f8aed017":"top_flavour(\"Assam\")","02205e57":"top_ingredient(\"Bihar\")","0f6632de":"top_flavour(\"Bihar\")","c6ba45b4":"top_ingredient(\"Chhattisgarh\")","f3402da2":"top_flavour(\"Chhattisgarh\")","4212975c":"top_ingredient(\"Goa\")","49af5f6e":"top_flavour(\"Goa\")","2019eeec":"top_ingredient(\"Gujarat\")","e788454d":"le.inverse_transform([26, 254, 422, 316])","c0b7da21":"top_flavour(\"Gujarat\")","76b8fd1d":"top_ingredient(\"Haryana\")","f70eda7f":"top_flavour(\"Haryana\")","6a86b3d9":"top_ingredient(\"Jammu & Kashmir\")","27744b60":"top_flavour(\"Jammu & Kashmir\")","76677ef8":"top_ingredient(\"Karnataka\")","6b09ab91":"le.inverse_transform([67, 79, 122, ])","b2acb47e":"top_flavour(\"Karnataka\")","74ceef86":"top_ingredient(\"Kerala\")","016450fe":"top_flavour(\"Kerala\")","880e689c":"top_ingredient(\"Madhya Pradesh\")","87c2b7c7":"top_flavour(\"Madhya Pradesh\")","a41dc45a":"top_ingredient(\"Maharashtra\")","71793d44":"le.inverse_transform([79, 402, 122, 218, 254, 423])","ac25e4bf":"top_flavour(\"Maharashtra\")","aa39852a":"top_ingredient(\"Manipur\")","08b3ecda":"top_flavour(\"Manipur\")","c288b1da":"top_ingredient(\"NCT of Delhi\")","70856b5b":"top_flavour(\"NCT of Delhi\