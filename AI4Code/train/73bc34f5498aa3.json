=True)","ecd645de":"fig = plt.figure(figsize=(18,18))\nsns.countplot(x='Rating',hue='Type',data=df,dodge=True)\nplt.show()","4e71c391":"f, ax = plt.subplots(figsize=(12,12))\nsns.scatterplot(data=df,x=\"Reviews\",y=\"Rating\",hue=\"Type\")\n","b9e10fc8":"f, ax = plt.subplots(figsize=(12,12))\nsns.scatterplot(data=df,x=\"Reviews\",y=\"Rating\",hue=\"Type\")\nplt.xlim(0,10**6)\n\nplt.show()\n","e5b8d277":"f, ax = plt.subplots(figsize=(12,12))\nsns.scatterplot(data=df,x=\"Reviews\",y=\"Rating\",hue=\"Type\",size='Installs')\n\nplt.show()\n","82537fe3":"fig, ax = plt.subplots(figsize=(12,12))\nplt.scatter('Reviews','Rating',data=df)\nplt.show()","b57963e1":"f, ax = plt.subplots(figsize=(10,10))\ng =sns.regplot(data=df,x=\"Reviews\",y=\"Installs\",fit_reg=False,x_jitter=.1)\ng.set(xscale='log',yscale='log')\nplt.show()\n","21a346b8":"f, ax = plt.subplots(figsize=(10,10))\nsns.countplot(x=df.loc[(df.Type == \"Paid\") & (df.Price<5),\"Price\"])\nplt.xticks(rotation=60)","00260d4c":"f, ax = plt.subplots(figsize=(10,10))\nsns.countplot(x=df.loc[(df.Type == \"Paid\") & (df.Price >5) & (df.Price<10) ,\"Price\"])\nplt.xticks(rotation=60)","8c49f0c7":"f, ax = plt.subplots(figsize=(10,10))\ng =sns.regplot(x=df.loc[df.Type == \"Paid\",\"Price\"],y=df.loc[df.Type == \"Paid\",\"Rating\"],fit_reg=False)\n\nplt.show()\n","f46ed866":"f, ax = plt.subplots(figsize=(10,10))\ng =sns.regplot(x=df.loc[(df.Type == \"Paid\")&(df.Price<50),\"Price\"],y=df.loc[(df.Type == \"Paid\")&(df.Price<50),\"Rating\"],fit_reg=False)\n\nplt.show()\n","4e2e3201":"df.Category.value_counts().head(4)","4080a365":"df.Category.value_counts().head(4)","d0cbc1f5":"sns.regplot(data=df, x=\"Size\", y=\"Rating\",scatter_kws={'alpha':0.3})","8d49952b":"sns.countplot(x=\"Content_Rating\",data=df)","26ad82bc":"sns.countplot(x=df.Rating.loc[df.Content_Rating == \"Everyone\"],data=df)\nplt.xticks(rotation=60)","d91e21b7":"sns.catplot(x=\"Rating\",y=\"Category\", data=df)","5658d366":"## Relationship between Rating and Size