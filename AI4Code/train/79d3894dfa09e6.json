_type","500e4a37":"font ={\n    \"size\" :20\n}\nplt.figure(figsize=(8,6))\ndatax_major_type.plot(kind=\"bar\")\nplt.xticks(rotation=30)\nplt.xlabel(\"Types of Calls\",fontdict=font)\nplt.ylabel(\"No. of Calls\",fontdict=font)\nplt.title(\"Types of Calls Vs No. of Calls\",fontdict=font)\nplt.savefig(\"Types-of-Calls-vs-No-of-Calls.png\")","c8477a37":"data['Year'] = data['timeStamp'].dt.year\ndata.head()","cf8df382":"data['Month'] = data['timeStamp'].dt.month_name()\ndata.head()","fccf997c":"data[\"Day\"] = data['timeStamp'].dt.day_name()\ndata.head()","4387a3ad":"data[\"Hour\"] = data['timeStamp'].dt.hour\ndata.head()","c9485dcf":"def actual_type_call(x):\n    x= x.split(':')\n    return x[1]","db6a7658":"data[\"emergency_reason\"] = data[\"title\"].apply(actual_type_call)\ndata.head()","a2ab10a2":"ems_data = data.copy(deep=True)","0106a55a":"ems_data.query('call_type == \"EMS\"',inplace = True)\nems_data.head()","1e8ec8e1":"count_ems = ems_data['emergency_reason'].value_counts()","220ea772":"plt.figsize=(15,10)\nplt.pie(count_ems.values[:7],labels=count_ems.index[:7],autopct=\"%.2f\")\nplt.savefig(\"Types-of-EMS-Calls-vs-No-of-Calls.png\")","f5cab047":"### Fire Data first 7 pie plot","ecb7461d":"fire_data = data.copy(deep = True)","4f360ce0":"fire_data.query('call_type == \"Fire\"',inplace = True)\nfire_data.head(2)","91784fbc":"fire_data['emergency_reason'].nunique()","379f1ae8":"count_fire = fire_data['emergency_reason'].value_counts()\ncount_fire.head(2)","3e0db679":"plt.figsize=(15,10)\nplt.pie(count_fire.values[:7],labels=count_fire.index[:7],autopct=\"%.2f\")\nplt.savefig(\"Types-of-fire-Calls-vs-No-of-Calls.png\")","20307a57":"### Traffic Pie Chart","6ee5a3c8":"traffic_data = data.copy(deep=True)","2b1a4bc8":"traffic_data.query('call_type == \"Traffic\"',inplace =True)\ntraffic_data.head(2)","dd84409a":"traffic_data['emergency_reason'].nunique()","32595c83":"count_traffic = traffic_data['emergency_reason'].value_counts()\ncount_traffic.head(7)","d6095fc5":"p