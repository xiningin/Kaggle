:"from sklearn.impute import SimpleImputer","0eefd7d8":"X[:,1:3]                                     #here we are giving Age and Salary column","1a3e619e":"imp=SimpleImputer(missing_values=np.nan, strategy='mean')                 #1) INTIALIZATION\nimp.fit(X[:,1:3])                                                         #2) FITTING\nX[:,1:3]=imp.transform(X[:,1:3])                                          #3) TRANSFORM","4339ecbf":"X                                                        #null values replaced with mean","c68239d8":"from sklearn.preprocessing import StandardScaler","e99a163b":"X[:,1:]","681fc0c9":"sc = StandardScaler()\nX[:,1:] = sc.fit_transform(X[:,1:])\nX","776647c1":"dummy1=pd.get_dummies(df['Country'])\ndummy1","9fc40d62":"df = pd.concat([df,dummy1], axis=1)","576a6114":"df.head()","07d09f11":"dummy = df.iloc[:,4:].values\ndummy","317a1105":"np.concatenate([dummy,X[:,1:]], axis=1)\nX","b907e24f":"dummy2=pd.get_dummies(df['Purchased'])\ndummy2","d7b5fbeb":"from sklearn.compose import ColumnTransformer\nfrom sklearn.preprocessing import OneHotEncoder","50ea0593":"ct = ColumnTransformer(transformers = [('encoder',OneHotEncoder(),[0])],remainder = 'passthrough')\nX = np.array(ct.fit_transform(X))","cae0e4b1":"X","2605abd8":"from sklearn.preprocessing import LabelEncoder","1a5e9262":"le = LabelEncoder()\nY= le.fit_transform(Y)","5b356d8c":"Y","b712c1c7":"from sklearn.model_selection import train_test_split","71b930a7":"X_train,X_test,Y_train,Y_test = train_test_split(X,Y,test_size=0.2, random_state= 0)","8b6763d9":"X_train","97c9315e":"Y_train","db8bfa12":"X_test","ced92c4c":"Y_test","260c184f":"# Handeling Categorical Data (ONE HOT ENCODING AND LABEL ENCODING)","57cb3ac1":"# 1. Handling Missing and Duplicate Data","1a451f27":"# Handlining Categorical Data Using Scikit Learn","a8f9542f":"# Splitting data into train and test dataset","fc91a7db":"# Using Scikit learn","9a10cd5d":"# Normalization ","d2ca98f5":"# IMPUTING NULL VALUES USING SKLEARN"}}