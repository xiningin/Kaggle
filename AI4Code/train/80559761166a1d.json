oupby('KernelId').agg({'TotalVotes' : 'sum', 'Title' : lambda x : \"#\".join(x).split(\"#\")[0]})\n    results = relevant.reset_index().sort_values('TotalVotes', ascending = False).head(n)\n    results = results.rename(columns={'KernelId' : 'Id', 'TotalVotes': 'Votes'})\n    results = results.merge(kernels, on=\"Id\").sort_values('TotalVotes', ascending = False)\n    results = results.merge(users.rename(columns={'Id':\"AuthorUserId\"}), on='AuthorUserId')\n    return results[['Title', 'CurrentUrlSlug', 'TotalViews', 'TotalComments', 'TotalVotes', \"DisplayName\",\"UserName\"]]\n\n\ndef best_kernels(tokens, n = 10):\n    response = get_kernels(tokens, n)     \n    hs = \"\"\"<style>\n                .rendered_html tr {font-size: 12px; text-align: left}\n            <\/style>\n            <h3><font color=\"#1768ea\">\"\"\"+tokens[0].title()+\"\"\"<\/font><\/h3>\n            <table>\n            <th>\n                <td><b>Kernel Title<\/b><\/td>\n                <td><b>Author<\/b><\/td>\n                <td><b>Total Views<\/b><\/td>\n                <td><b>Total Comments<\/b><\/td>\n                <td><b>Total Votes<\/b><\/td>\n            <\/th>\"\"\"\n    for i, row in response.iterrows():\n        url = \"https:\/\/www.kaggle.com\/\"+row['UserName']+\"\/\"+row['CurrentUrlSlug']\n        aurl= \"https:\/\/www.kaggle.com\/\"+row['UserName']\n        hs += \"\"\"<tr>\n                    <td>\"\"\"+str(i+1)+\"\"\"<\/td>\n                    <td><a href=\"\"\"+url+\"\"\" target=\"_blank\"><b>\"\"\"  + row['Title'] + \"\"\"<\/b><\/a><\/td>\n                    <td><a href=\"\"\"+aurl+\"\"\" target=\"_blank\">\"\"\"  + row['DisplayName'] + \"\"\"<\/a><\/td>\n                    <td>\"\"\"+str(row['TotalViews'])+\"\"\"<\/td>\n                    <td>\"\"\"+str(row['TotalComments'])+\"\"\"<\/td>\n                    <td>\"\"\"+str(row['TotalVotes'])+\"\"\"<\/td>\n                    <\/tr>\"\"\"\n    hs += \"<\/table>\"\n    display(HTML(hs))","50ded7d7":"tokens = [\"linear regression\"]\nbest_kernels(tokens, 10)","f2079fb7":"tokens = ['logistic regression', \"logistic\"]\nbest_kernels(tokens, 10)","89e6ec02":"tokens = ['Stepwise regression']\nbest_kernels(tokens, 10)","5783ab39":"tokens = ['polynomial regression']\nbest_kernels(tokens, 10)","42edac89":"tokens = ['multivariate regression']\nbest_kernels(tokens, 10)","af38d0ed":"tokens = ['Ridge']\nbest_kernels(tokens, 10)","b31409a2":"tokens = ['Lasso']\nbest_kernels(tokens, 10)","ce66e061":"tokens = ['ElasticNet']\nbest_kernels(tokens, 10)","2582623c":"tokens = ['Decision Tree']\nbest_kernels(tokens, 10)","24df9f3d":"tokens = ['random forest']\nbest_kernels(tokens, 10)","6f0a7f4c":"tokens = ['lightgbm', 'light gbm', 'lgb']\nbest_kernels(tokens, 10)","7e3915de":"tokens = ['xgboost', 'xgb']\nbest_kernels(tokens, 10)","b5f6890d":"tokens = ['catboost']\nbest_kernels(tokens, 10)","2f1ab997":"tokens = ['adaboost']\nbest_kernels(tokens, 10)","e79b3076":"tokens = ['neural network']\nbest_kernels(tokens, 10)","2f85f78a":"tokens = ['backpropagation']\nbest_kernels(tokens, 10)","66cdf4be":"tokens = ['autoencoder']\nbest_kernels(tokens, 10)","59c8dbfb":"tokens = ['deep learning']\nbest_kernels(tokens, 10)","0884d764":"tokens = ['convolutional neural networks', 'cnn']\nbest_kernels(tokens, 10)","142def56":"tokens = ['lstm']\nbest_kernels(tokens, 10)","d47a1e40":"tokens = ['gru']\nbest_kernels(tokens, 10)","c18a564f":"tokens = ['mxnet']\nbest_kernels(tokens, 10)","e6e7e76f":"tokens = ['resnet']\nbest_kernels(tokens, 10)","73b682f2":"tokens = ['Capsule network', 'capsulenet']\nbest_kernels(tokens, 10)","72fade88":"tokens = ['kmeans', 'k means']\nbest_kernels(tokens, 10)","a4b07f79":"tokens = ['hierarchical clustering']\nbest_kernels(tokens, 10)","b26a2291":"tokens = ['dbscan']\nbest_kernels(tokens, 10)","1c342e89":"tokens = ['naive bayes']\nbest_kernels(tokens, 10)","40595ceb":"tokens = ['svm']\nbest_kernels(tokens, 10)","b1afd2c2":"tokens = ['ensemble']\nbest_kernels(tokens, 10)","373bafaa":"to