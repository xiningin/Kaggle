{"cell_type":{"d46754ce":"code","b9f1429b":"code","c616ad6e":"code","2cb9b4e7":"code","6153bcf8":"code","8b2281f5":"code","cc4d3973":"code","188fd12d":"code","b676e1e2":"code","c7dc1a8c":"code","76068534":"code","ff7bcfdb":"code","803718c6":"code","41a3add2":"code","cc60fed1":"code","08efd9a3":"code","b141fd53":"code","dd51bac9":"code","f6e71564":"code","23c149f8":"code","3a7f6ad2":"code","8d9b95e4":"code","f02292a6":"code","bdd9be3a":"code","eb9e5d60":"code","4a11197d":"code","02c76720":"markdown","ca3da42f":"markdown","a12c7680":"markdown","011e16de":"markdown","e5d6ab02":"markdown","1401fb25":"markdown","ad2a3d39":"markdown","a55c5565":"markdown","d5361bba":"markdown"},"source":{"d46754ce":"import numpy as np\nimport matplotlib.pyplot as plt\nimport random\nimport math","b9f1429b":"abscissa = np.arange(0, 1.01, 0.01)\nabscissa","c616ad6e":"ordinata = np.sin(abscissa *  math.pi \/ 2)\nordinata","2cb9b4e7":"plt.plot(abscissa, ordinata)","6153bcf8":"uniform = np.random.rand(30)\nplt.hist(uniform)","8b2281f5":"sample = np.arcsin(uniform) * 2 \/ math.pi","cc4d3973":"plt.hist(sample)","188fd12d":"edf = []\nsample = np.array(sorted(sample))\nfor i in range(len(sample)):\n    edf.append(sample[ sample <= sample[i] ].size \/ sample.size)\nedf = np.array(edf)","b676e1e2":"#plt.scatter(sample, edf)\nplt.plot(sample, edf, drawstyle=\"steps-post\")","c7dc1a8c":"from scipy import stats","76068534":"def my_sin(x):\n    return np.sin(x *  math.pi \/ 2)","ff7bcfdb":"stats.kstest(sample, my_sin)","803718c6":"def find_epsilon(n):\n    return (np.log(2 \/ 0.05) \/ (2 * n)) ** 0.5","41a3add2":"n_array = np.arange(1, 200)\nepsilons = find_epsilon(n_array)","cc60fed1":"plt.plot(n_array, epsilons)","08efd9a3":"def gen_emp_sample(count):\n    uniform = np.random.rand(count)\n    return np.arcsin(uniform) * 2 \/ math.pi","b141fd53":"def gen_ecdf(my_sample):\n    sample = np.array(my_sample)\n    def ecdf(x):\n        return sample[ sample <= x ].size \/ sample.size\n    return ecdf","dd51bac9":"def get_distance(sample, my_ecdf, real_function):\n    points = np.array(sample)\n    values_2 = real_function(points)\n    values_1 = np.array([my_ecdf(point) for point in points])\n    #values_1 = np.array(list(map(my_ecdf, points)))\n    return (np.absolute(values_1 - values_2)).max()","f6e71564":"n_array = np.arange(1, 200)\nn_samples = []\nn_ecdf = []\nfor i in n_array:\n    sample = gen_emp_sample(i)\n    n_samples.append(sample)\n    n_ecdf.append(gen_ecdf(sample))\nn_distance = []\nfor i in range(len(n_samples)):\n    n_distance.append(get_distance(n_samples[i], n_ecdf[i], my_sin))\nn_distance = np.array(n_distance)","23c149f8":"plt.plot(n_array, epsilons)\nplt.plot(n_array, n_distance)","3a7f6ad2":"n_samples","8d9b95e4":"plt.hist(n_samples[-1])","f02292a6":"stats.kstest(n_samples[-1], my_sin)","bdd9be3a":"get_distance(n_samples[-1], n_ecdf[-1], my_sin)","eb9e5d60":"map.__doc__","4a11197d":"plt.scatter(n_samples[-1], list(map(n_ecdf[-1], n_samples[-1])))","02c76720":"\u0414\u043e\u043f\u0443\u0441\u0442\u0438\u043c, \u0447\u0442\u043e \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u0435 \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u043e\u0439 \u0432\u0435\u043b\u0435\u0447\u0438\u043d\u044b, \u043a\u043e\u0442\u043e\u0440\u0443\u044e \u043c\u044b \u0445\u043e\u0442\u0438\u043c \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c, \u0437\u0430\u0434\u0430\u0451\u0442\u0441\u044f \u0444\u0443\u043d\u043a\u0446\u0438\u0435\u0439 $\\sin (\\frac{\\pi}{2} x)$ \u0434\u043b\u044f $x \\in [0, 1]$","ca3da42f":"\u041f\u043e\u0441\u0442\u0440\u043e\u0438\u043c \u0433\u0438\u0441\u0442\u043e\u0433\u0440\u0430\u043c\u043c\u0443 \u0434\u043b\u044f \u044d\u0442\u043e\u0433\u043e \u0441\u044d\u043c\u043f\u043b\u0430","a12c7680":"\u0422\u0435\u043f\u0435\u0440\u044c \u043f\u0435\u0440\u0435\u0432\u0435\u0434\u0451\u043c \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043d\u044b\u0435 \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u044b\u0435 \u0432\u0435\u043b\u0438\u0447\u0438\u043d\u044b \u0441 \u043f\u043e\u043c\u043e\u0449\u044c\u044e \u043e\u0431\u0440\u0430\u0442\u043d\u043e\u0439 \u0444\u0443\u043d\u043a\u0446\u0438\u0438 \u0432 \u0441\u044d\u043c\u043f\u043b \u0434\u043b\u044f \u043d\u0430\u0448\u0435\u0433\u043e \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u044f (\u0441 \u0441\u0438\u043d\u0443\u0441\u043e\u043c)","011e16de":"\u0422\u0435\u043f\u0435\u0440\u044c \u0432\u0437\u0433\u043b\u044f\u043d\u0435\u043c \u043d\u0430 \u0440\u0430\u0437\u043d\u0438\u0446\u0443 \u043c\u0435\u0436\u0434\u0443 \u043d\u0430\u0448\u0438\u043c \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u0435\u043c (\u0441\u0438\u043d\u0443\u0441\u043e\u043c) \u0438 \u044d\u043c\u043f\u0438\u0440\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0444\u0443\u043d\u043a\u0446\u0438\u0435\u0439 \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u044f.\n\n\u0427\u0442\u043e\u0431\u044b \u0431\u044b\u043b\u043e \u043f\u0440\u043e\u0449\u0435 \u0437\u0430\u0434\u0430\u0434\u0438\u043c \u0444\u0443\u043d\u043a\u0446\u0438\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0432\u0441\u0451 \u044d\u0442\u043e \u0431\u0443\u0434\u0443\u0442 \u0441\u0447\u0438\u0442\u0430\u0442\u044c.","e5d6ab02":"\u041e\u0431\u0440\u0430\u0442\u043d\u043e\u0439 \u0444\u0443\u043d\u043a\u0446\u0438\u0435\u0439 \u0434\u043b\u044f \u043f\u043e\u0434\u043e\u0431\u043d\u043e\u0433\u043e \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u044f, \u043e\u0447\u0435\u0432\u0438\u0434\u043d\u043e, \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0430\u0440\u043a\u0441\u0438\u043d\u0443\u0441. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043c\u044b \u043c\u043e\u0436\u0435\u043c \u0432\u0437\u044f\u0442\u044c \u0441\u044d\u043c\u043f\u043b \u0438\u0437 \u0440\u0430\u0432\u043d\u043e\u043c\u0435\u0440\u043d\u043e\u0433\u043e \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\