{"cell_type":{"172b46ad":"code","72ec8f18":"code","790a8c5c":"code","241dfb76":"code","47eb129e":"code","3c14f2b0":"code","328c8449":"code","8caa3f71":"code","6df8b513":"code","2c6ec958":"code","381ae509":"code","761800db":"code","0382e585":"code","97f77221":"code","1b0d4206":"code","17cb1f5a":"code","3652e51f":"code","50c310ef":"code","37f5fd3e":"code","987f0514":"code","88ce59a5":"code","4f989085":"code","ddef3564":"markdown","95011450":"markdown","5053483c":"markdown","941668a2":"markdown","37482ec0":"markdown","b0f2eecd":"markdown","0602c11f":"markdown","4e01d400":"markdown","bce3ce74":"markdown","581d1326":"markdown","c2fb52c2":"markdown","0c51be01":"markdown","0c58ad3e":"markdown","3a129e72":"markdown","03376946":"markdown"},"source":{"172b46ad":"import numpy as np # linear algebra\nimport pandas as pd # data processing, CSV file I\/O (e.g. pd.read_csv)\nfrom sklearn.model_selection import train_test_split\nfrom sklearn.linear_model import LogisticRegression\nfrom sklearn.svm import SVC\nfrom sklearn.metrics import accuracy_score, confusion_matrix, plot_confusion_matrix, plot_roc_curve, f1_score\nimport matplotlib.pyplot as plt","72ec8f18":"df = pd.read_csv('\/kaggle\/input\/creditcardfraud\/creditcard.csv')\ndf.head()","790a8c5c":"df.Class.hist()\nprint(f'\u0414\u043e\u043b\u044f \u043f\u043e\u0434\u043e\u0437\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0445 \u0442\u0440\u0430\u043d\u0437\u0430\u043a\u0446\u0438\u0439 {np.sum(df.Class==1)\/len(df.Class):.4f}')","241dfb76":"X_train, X_test, y_train, y_test = train_test_split(df.drop('Class', axis=1), df.Class)","47eb129e":"logReg = LogisticRegression(max_iter=1000)\nsvm = SVC()","3c14f2b0":"def process_model(clf, X_train, X_test, y_train, y_test):\n    clf.fit(X_train, y_train)\n    print(f'Accuracy \u043d\u0430 \u0442\u0440\u0435\u043d\u0438\u0440\u043e\u0432\u043e\u0447\u043d\u044b\u0445 \u0434\u0430\u043d\u043d\u044b\u0445 {logReg.score(X_train, y_train):.4f}')\n    y_pred = clf.predict(X_test)\n    print(f'Accuracy \u043d\u0430 \u0442\u0435\u0441\u0442\u043e\u0432\u044b\u0445 \u0434\u0430\u043d\u043d\u044b\u0445 {accuracy_score(y_pred, y_test):.4f}')\n    print(f'F1-score \u043d\u0430 \u0442\u0435\u0441\u0442\u043e\u0432\u044b\u0445 \u0434\u0430\u043d\u043d\u044b\u0445 {f1_score(y_pred, y_test):.4f}')\n    plot_confusion_matrix(clf, X_test, y_test) ","328c8449":"process_model(logReg, X_train, X_test, y_train, y_test)","8caa3f71":"process_model(svm, X_train, X_test, y_train, y_test)","6df8b513":"ax = plt.gca()\nplot_roc_curve(logReg, X_test, y_test, ax=ax)\nplot_roc_curve(svm, X_test, y_test, ax=ax);","2c6ec958":"df_test = pd.concat( (X_test, y_test), axis=1 )","381ae509":"from imblearn.over_sampling import RandomOverSampler\nlogRegOver = LogisticRegression(max_iter=1000)","761800db":"ros = RandomOverSampler(random_state=0)\nX_resampled, y_resampled = ros.fit_sample(X_train, y_train)","0382e585":"print('\u0414\u043e\u043b\u044f \u043f\u043e\u0434\u043e\u0437\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0445 \u0442\u0440\u0430\u043d\u0437\u0430\u043a\u0446\u0438\u0439 \u043f\u043e\u0441\u043b\u0435 oversampling', np.sum(y_resampled==0)\/len(y_resampled))","97f77221":"process_model(logRegOver, X_resampled, X_test, y_resampled, y_test)","1b0d4206":"svmOver = SVC(verbose=True, max_iter=1000) #\u043c\u043e\u0436\u043d\u043e \u0438 \u0442\u0430\u043a \u0431\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043a\u043b\u0430\u0441\u0441\u044b: class_weight='balanced'\nprocess_model(svmOver, X_resampled, X_test, y_resampled, y_test)","17cb1f5a":"ax = plt.gca()\nplot_roc_curve(logReg, X_test, y_test, ax=ax, name='LogReg')\nplot_roc_curve(logRegOver, X_test, y_test, ax=ax, name='LogReg Oversampling');\nplot_roc_curve(svm, X_test, y_test, ax=ax, name='SVM')\nplot_roc_curve(svmOver, X_test, y_test, ax=ax, name='SVM Oversampling');","3652e51f":"from imblearn.under_sampling import RandomUnderSampler\nlogRegUnder = LogisticRegression(max_iter=1000)\nsvmUnder = SVC(verbose=True, max_iter=1000)","50c310ef":"ros = RandomUnderSampler(random_state=0)\nX_resampled, y_resampled = ros.fit_sample(X_train, y_train)","37f5fd3e":"process_model(logRegUnder, X_resampled, X_test, y_resampled, y_test)","987f0514":"process_model(svmUnder, X_resampled, X_test, y_resampled, y_test)","88ce59a5":"ax = plt.gca()\nplot_roc_curve(logReg, X_test, y_test, ax=ax, name='LogReg')\nplot_roc_curve(logRegOver, X_test, y_test, ax=ax, name='LogReg Oversampling');\nplot_roc_curve(logRegUnder, X_test, y_test, ax=ax, name='LogReg Undersampling');","4f989085":"ax = plt.gca()\nplot_roc_curve(svm, X_test, y_test, ax=ax, name='SVM')\nplot_roc_curve(svmOver, X_test, y_test, ax=ax, name='SVM Oversampling');\nplot_roc_curve(svmUnder, X_test, y_test, ax=ax, name='SVM Undersampling');","ddef3564":"\u0421\u043e\u0432\u0441\u0435\u043c \u043f\u043b\u043e\u0445\u043e, \u0442\u0435\u043f\u0435\u0440\u044c \u043b\u043e\u0436\u043d\u044b\u0445 \u0441\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u043d\u0438\u0439 \u0441\u0442\u0430\u043b\u043e \u0441\u043b\u0438\u0448\u043a\u043e\u043c \u043c\u043d\u043e\u0433\u043e","95011450":"\u0415\u0441\u043b\u0438 \u0441\u0443\u0434\u0438\u0442\u044c \u043f\u043e Accuracy, \u0442\u043e \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442 \u043a\u043b\u0430\u0441\u0441\u0438\u0444\u0438\u043a\u0430\u0446\u0438\u0438 \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0439 (\u043d\u043e \u044d\u0442\u043e \u043d\u0435\u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e)\n\nF1-score \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442, \u0447\u0442\u043e \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442, \u043a\u0430\u043a \u043c\u0430\u043a\u0441\u0438\u043c\u0443\u043c, \u0445\u043e\u0440\u043e\u0448\u0438\u0439, \u0430 \u0438\u0437 \u043c\u0430\u0442\u0440\u0438\u0446\u044b \u043e\u0448\u0438\u0431\u043e\u043a \u0432\u0438\u0434\u0438\u043c, \u0447\u0442\u043e ~50 \u0443\u0441\u043b\u043e\u0432\u043d\u044b\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u043c\u043e\u0433\u043b\u0438 \u0431\u044b \u043b\u0438\u0448\u0438\u0442\u044c\u0441\u044f \u0434\u0435\u043d\u0435\u0433, \u0442.\u043a. \u043d\u0430\u0448 \u043a\u043b\u0430\u0441\u0441\u0438\u0444\u0438\u043a\u0430\u0442\u043e\u0440 \u043d\u0435\u0432\u0435\u0440\u043d\u043e \u0441\u0447\u0438\u0442\u0430\u0435\u0442 \u0442\u0440\u0430\u043d\u0437\u0430\u043a\u0446\u0438\u0438 \u0441 \u0438\u0445 \u0441\u0447\u0435\u0442\u043e\u0432 \u043d\u0435 \u043c\u043e\u0448\u0435\u043d\u043d\u0438\u0447\u0435\u0441\u043a\u0438\u043c\u0438","5053483c":"\u041a\u0430\u043a \u0438 \u043e\u0436\u0438\u0434\u0430\u043b\u043e\u0441\u044c, \u043f\u0440\u043e\u0438\u0437\u043e\u0448\u043b\u0430 \u0431\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u043a\u0430 \u043a\u043b\u0430\u0441\u0441\u043e\u0432","941668a2":"\u041a\u043b\u0430\u0441\u0441\u044b \u044f\u0432\u043d\u043e \u043d\u0435 \u0441\u0431\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u0430\u043d\u044b\n\n\u0420\u0430\u0437\u0431\u0438\u0432\u0430\u044e \u0434\u0430\u0442\u0430\u0441\u0435\u0442 \u043d\u0430 \u0442\u0440\u0435\u043d\u0438\u0440\u043e\u0432\u043e\u0447\u043d\u0443\u044e \u0438 \u0442\u0435\u0441\u0442\u043e\u0432\u0443\u044e \u0447\u0430\u0441\u0442\u0438 (\u043a\u043e\u0442\u043e\u0440\u0443\u044e \u043c\u0435\u043d\u044f\u0442\u044c \u0434\u0430\u043b\u044c\u0448\u0435 \u043d\u0435 \u0431\u0443\u0434\u0443)","37482ec0":"\u0417\u0434\u0435\u0441\u044c \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442 \u0435\u0449\u0451 \u0431\u043e\u043b\u0435\u0435 \u043f\u0435\u0447\u0430\u043b\u044c\u043d\u044b\u0439: SVM \u0432\u043e\u043e\u0431\u0449\u0435 \u043d\u0435 \u0437\u043d\u0430\u0435\u0442, \u0447\u0442\u043e \u0442\u0430\u043a\u043e\u0435 \u043c\u043e\u0448\u0435\u043d\u043d\u0438\u043a\u0438","b0f2eecd":"Oversampling \u0434\u043b\u044f \u043b\u043e\u0433\u0440\u0435\u0433\u0440\u0435\u0441\u0441\u0438\u0438 \u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u043f\u0440\u043e\u0442\u0438\u0432\u043e\u0440\u0435\u0447\u0438\u0432\u044b\u0435 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b: \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043e\u0431\u043c\u0430\u043d\u0443\u0442\u044b\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u0441\u0442\u0430\u043b\u043e \u043c\u0435\u043d\u044c\u0448\u0435 (11), \u043d\u043e \u0437\u0430\u0442\u043e \u0440\u0435\u0437\u043a\u043e \u043f\u043e\u0434\u0441\u043a\u043e\u0447\u0438\u043b\u043e \u0447\u0438\u0441\u043b\u043e \u043b\u043e\u0436\u043d\u044b\u0445 \u0441\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u043d\u0438\u0439 (2582), \u0437\u0434\u0435\u0441\u044c \u043d\u0443\u0436\u043d\u043e \u043f\u043e\u043d\u044f\u0442\u044c, \u043d\u0430\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0434\u0435\u043d\u044c\u0433\u0438 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u0432\u0430\u0436\u043d\u0435\u0435 \u0438\u0445 \u043d\u0435\u0440\u0432\u043e\u0432 (\u043d\u0435\u043e\u043f\u0440\u0430\u0432\u0434\u0430\u043d\u043d\u044b\u0435 \u043e\u043f\u043e\u0432\u0435\u0449\u0435\u043d\u0438\u044f \u043e \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u044b\u0445 \u043c\u043e\u0448\u0435\u043d\u043d\u0438\u043a\u0430\u0445 \u0440\u0430\u0437\u0434\u0440\u0430\u0436\u0430\u044e\u0442)","0602c11f":"# \u041d\u0435\u0441\u0431\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0435 \u043a\u043b\u0430\u0441\u0441\u044b\n\n\u0412 \u0437\u0430\u0434\u0430\u0447\u0430\u0445 \u043a\u043b\u0430\u0441\u0441\u0438\u0444\u0438\u043a\u0430\u0446\u0438\u0438 \u0447\u0430\u0441\u0442\u043e \u0432\u043e\u0437\u043d\u0438\u043a\u0430\u0435\u0442 \u0434\u0438\u0441\u0431\u0430\u043b\u0430\u043d\u0441 \u043a\u043b\u0430\u0441\u0441\u043e\u0432, \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440 \u043c\u043e\u0434\u0435\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0430\u0440\u0438\u0435\u0432 \u0438\u043b\u0438 \u043f\u043e\u0438\u0441\u043a \u043c\u043e\u0448\u0435\u043d\u043d\u0435\u0447\u0435\u0441\u043a\u0438\u0445 \u0442\u0440\u0430\u043d\u0437\u0430\u043a\u0446\u0438\u0439 (\u043a\u0430\u043a \u0437\u0434\u0435\u0441\u044c).\n\n\u0412\u043b\u0438\u044f\u0435\u0442 \u043b\u0438 \u0434\u0438\u0441\u0431\u0430\u043b\u0430\u0441\u043d\u0441 \u043a\u043b\u0430\u0441\u0441\u043e\u0432 \u043d\u0430 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043c\u043e\u0434\u0435\u043b\u0438? \u0417\u0430\u0432\u0438\u0441\u0438\u0442 \u043e\u0442 \u043c\u043e\u0434\u0435\u043b\u0438, \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u043d\u0430 \u043b\u043e\u0433\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0443\u044e \u0440\u0435\u0433\u0440\u0435\u0441\u0441\u0438\u044e \u043d\u0435 \u043e\u0441\u043e\u0431\u043e, \u0430 \u0432\u043e\u0442 \u043d\u0430 SVM \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440 \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u0441\u0438\u043b\u044c\u043d\u043e, \u0442.\u043a. \u0432 \u044d\u0442\u043e\u0439 \u043c\u043e\u0434\u0435\u043b\u0438 \u043c\u044b \u0441\u0442\u0440\u0435\u043c\u0438\u043c\u0441\u044f \u043f\u0440\u043e\u0432\u0435\u0441\u0442\u0438 \u0433\u0438\u043f\u0435\u0440\u043f\u043b\u043e\u0441\u043a\u043e\u0441\u0442\u044c \u043a\u0430\u043a \u043c\u043e\u0436\u043d\u043e \u0434\u0430\u043b\u044c\u0448\u0435 \u0441 \u0434\u0432\u0443\u0445 \u0441\u0442\u043e\u0440\u043e\u043d \u043e\u0442 \u043d\u0430\u0431\u043b\u044e\u0434\u0435\u043d\u0438\u0439 \u0438 \u0432 \u0441\u043b\u0443\u0447\u0430\u0435 \u0434\u0438\u0441\u0431\u0430\u043b\u0430\u043d\u0441\u0430 \u043a\u043b\u0430\u0441\u0441\u043e\u0432 \u043c\u043e\u0434\u0435\u043b\u044c \u043c\u043e\u0436\u0435\u0442 \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u0440\u0435\u043d\u0435\u0431\u0440\u0435\u0447\u044c \u043c\u0438\u043d\u043e\u0440\u043d\u044b\u043c \u043a\u043b\u0430\u0441\u0441\u043e\u043c \u0432 \u043f\u043e\u043b\u044c\u0437\u0443 \u0447\u0430\u0441\u0442\u043e\u0432\u0441\u0442\u0440\u0435\u0447\u0430\u044e\u0449\u0435\u0433\u043e\u0441\u044f \u0438 \u0432\u044b\u0431\u0440\u043e\u0441\u043e\u0432 \u0438\u0437 \u043d\u0435\u0433\u043e.\n\n## \u0421\u043f\u043e\u0441\u043e\u0431\u044b \u0431\u043e\u0440\u044c\u0431\u044b \u0441 \u043d\u0435\u0441\u0431\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u043c\u0438 \u043a\u043b\u0430\u0441\u0441\u0430\u043c\u0438\n\u0414\u0432\u0430 \u0441\u0430\u043c\u044b\u0445 \u043f\u0440\u043e\u0441\u0442\u044b\u0445 \u0441\u043f\u043e\u0441\u043e\u0431\u0430: **Oversampling** \u0438 **Undersampling** \u0442\u0440\u0435\u043d\u0438\u0440\u043e\u0432\u043e\u0447\u043d\u043e\u0433\u043e \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0430\n1. **Oversampling** \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u044b\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u0434\u0443\u0431\u043b\u0438\u0440\u0443\u0435\u0442 \u043e\u0431\u044a\u0435\u043a\u0442\u044b \u043c\u0438\u043d\u043e\u0440\u043d\u043e\u0433\u043e \u043a\u043b\u0430\u0441\u0441\u0430 \u0438 \u0442\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u0443\u0440\u0430\u0432\u043d\u0438\u0432\u0430\u0435\u0442 \u043a\u043b\u0430\u0441\u0441\u044b\n1. **Undersampling** \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u044b\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u0443\u0434\u0430\u043b\u044f\u0435\u0442 \u043e\u0431\u044a\u0435\u043a\u0442\u044b \u043c\u0430\u0436\u043e\u0440\u043d\u043e\u0433\u043e \u043a\u043b\u0430\u0441\u0441\u0430\n\n\u0411\u043e\u043b\u0435\u0435 \u0441\u043b\u043e\u0436\u043d\u044b\u0439 \u0441\u043f\u043e\u0441\u043e\u0431 **SMOTE** \u0434\u043e\u043f\u043e\u043b\u043d\u044f\u0435\u0442 \u043e\u0431\u044a\u0435\u043a\u0442\u044b \u043c\u0438\u043d\u043e\u0440\u043d\u043e\u0433\u043e \u043a\u043b\u0430\u0441\u0441\u0430 \u0441\u0438\u043d\u0442\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u043c\u0438 \u043e\u0431\u044a\u0435\u043a\u0442\u0430\u043c\u0438, \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u044b \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u0442\u0430\u043a\u043e\u0433\u043e \u043e\u0431\u044a\u0435\u043a\u0442\u0430 \u043b\u0435\u0436\u0430\u0442 \u043d\u0430 \u043b\u0438\u043d\u0438\u0438 \u043c\u0435\u0436\u0434\u0443 \u0441\u043b\u0443\u0447\u0430\u0439\u043d\u043e \u0432\u044b\u0431\u0440\u0430\u043d\u043d\u044b\u043c\u0438 \u0441\u043e\u0441\u0435\u0434\u044f\u043c\u0438 \u0438\u0437 \u043c\u0438\u043d\u043e\u0440\u043d\u043e\u0433\u043e \u043a\u043b\u0430\u0441\u0441\u0430.\n\n\u0412 \u043d\u043e\u0443\u0442\u0431\u0443\u043a\u0435 \u044f \u0432\u043e\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0441\u044c **Oversampling** \u0438 **Undersampling** \u0434\u043b\u044f \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u043e\u0433\u043e \u043d\u0430\u0431\u043e\u0440\u0430 \u0434\u0430\u043d\u043d\u044b\u0445\n\n## \u041c\u0435\u0442\u0440\u0438\u043a\u0438 \u0434\u043b\u044f \u043e\u0446\u0435\u043d\u043a\u0438 \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u043e\u0432\n\u0422\u0430\u043a \u043a\u0430\u043a \u0432 \u0442\u0435\u0441\u0442\u043e\u0432\u043e\u043c \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0435 \u0431\u0443\u0434\u0435\u0442 \u043d\u0430\u0431\u043b\u044e\u0434\u0430\u0442\u044c\u0441\u044f \u0442\u0430\u043a\u043e\u0439 \u0436\u0435 \u0434\u0438\u0441\u0431\u0430\u043b\u0430\u043d\u0441 \u043a\u043b\u0430\u0441\u0441\u043e\u0432, \u0442\u043e \u043e\u0431\u044b\u0447\u043d\u0443\u044e \u0442\u043e\u0447\u043d\u043e\u0441\u0442\u044c (`Accuracy`) \u043f\u0440\u0438\u043c\u0435\u043d\u044f\u0442\u044c **\u043f\u043b\u043e\u0445\u043e**, \u0442.\u043a. \u043e\u043d\u0430 \u0431\u0443\u0434\u0435\u0442 \u0440\u0435\u0430\u0433\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0442\u043e\u043b\u044c\u043a\u043e \u043d\u0430 \u043c\u0430\u0436\u043e\u0440\u043d\u044b\u0439 \u043a\u043b\u0430\u0441\u0441.\n\u0410, \u043d\u0430\u043f\u0440\u0438\u043c\u0435\u0440, \u043f\u0440\u043e\u0441\u0442\u0430\u044f \u043c\u0435\u0442\u0440\u0438\u043a\u0430 `F1-score` \u0443\u0436\u0435 \u0443\u0447\u0438\u0442\u044b\u0432\u0430\u0435\u0442 \u0434\u0438\u0441\u0431\u0430\u043b\u0430\u043d\u0441 \u043a\u043b\u0430\u0441\u0441\u043e\u0432.\n\u041c\u043e\u0436\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043c\u0430\u0442\u0440\u0438\u0446\u0443 \u043e\u0448\u0438\u0431\u043e\u043a (`confusion_matrix`), \u0442.\u043a. \u0438\u0437 \u043d\u0435\u0451 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043c\u043e\u0436\u043d\u043e \u043e\u0446\u0435\u043d\u0438\u0442\u044c \u043e\u0448\u0438\u0431\u043a\u0438 \u043f\u0435\u0440\u0432\u043e\u0433\u043e \u0438 \u0432\u0442\u043e\u0440\u043e\u0433\u043e \u0440\u043e\u0434\u0430 \u043a\u043b\u0430\u0441\u0441\u0438\u0444\u0438\u043a\u0430\u0442\u043e\u0440\u0430.\n\u0422\u0430\u043a\u0436\u0435 `ROC`-\u043a\u0440\u0438\u0432\u0430\u044f \u043f\u043e\u043a\u0430\u0436\u0435\u0442 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e \u043a\u043b\u0430\u0441\u0441\u0438\u0444\u0438\u043a\u0430\u0446\u0438\u0438.","4e01d400":"\u041e\u0431\u044a\u044f\u0432\u043b\u044f\u044e \u0434\u0432\u0430 \u043a\u043b\u0430\u0441\u0441\u0438\u0444\u0438\u043a\u0430\u0442\u043e\u0440\u0430: \u043b\u043e\u0433\u0440\u0435\u0433\u0440\u0435\u0441\u0441\u0438\u044e \u0438 SVM","bce3ce74":"\u041f\u043e\u0441\u043c\u043e\u0442\u0440\u044e \u043d\u0430 \u0434\u0430\u0442\u0430\u0441\u0435\u0442, \u043f\u0440\u043e\u0432\u0435\u0440\u044e, \u0447\u0442\u043e \u0442\u0430\u043c \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0434\u0438\u0441\u0431\u0430\u043b\u0430\u043d\u0441 \u043a\u043b\u0430\u0441\u0441\u043e\u0432","581d1326":"ROC-\u043a\u0440\u0438\u0432\u0430\u044f \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442, \u0447\u0442\u043e \u0432\u0441\u0451-\u0442\u0430\u043a\u0438 \u0434\u043b\u044f \u043b\u043e\u0433\u0440\u0435\u0433\u0440\u0435\u0441\u0441\u0438\u0438 \u0431\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u043a\u0430 \u043e\u043a\u0430\u0437\u0430\u043b\u0430\u0441\u044c \u043f\u043e\u043b\u0435\u0437\u043d\u043e\u0439, \u0430 SVM \u0438 \u044d\u0442\u043e \u043d\u0435 \u043f\u043e\u043c\u043e\u0433\u043b\u043e","c2fb52c2":"\u0414\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0441\u043c\u043e\u0442\u0440\u044e, \u043d\u0430 ROC-\u043a\u0440\u0438\u0432\u0443\u044e: \u043e\u043d\u0430 (\u0430 \u0442\u043e\u0447\u043d\u0435\u0435 \u043f\u043b\u043e\u0449\u0430\u0434\u044c \u043f\u043e\u0434 \u043d\u0435\u0439) \u043e\u0442\u0440\u0430\u0436\u0430\u0435\u0442 \u0442\u043e, \u0447\u0442\u043e \u044f \u0443\u0436\u0435 \u043f\u0438\u0441\u0430\u043b - \u043b\u043e\u0433\u0440\u0435\u0433\u0440\u0435\u0441\u0441\u0438\u044f \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0442 \u043f\u0440\u0438\u043b\u0438\u0447\u043d\u043e, \u0430  SVM \u0443\u0436\u0430\u0441\u043d\u043e","0c51be01":"\u0422\u0435\u043f\u0435\u0440\u044c **Undersampling**","0c58ad3e":"## \u0417\u0430\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u0435\n\n\u0411\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u043a\u0430 \u043a\u043b\u0430\u0441\u0441\u043e\u0432 \u043f\u043e\u043c\u043e\u0433\u043b\u0430 LogReg \u0443\u043b\u0443\u0447\u0448\u0438\u0442\u044c \u0440\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b, \u0441\u0443\u0434\u044f \u043f\u043e ROC-\u043a\u0440\u0438\u0432\u043e\u0439.\nSVM \u043d\u0438\u0447\u0435\u0433\u043e \u043d\u0435 \u043f\u043e\u043c\u043e\u0433\u043b\u043e.\n\u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043c\u043e\u0436\u043d\u043e \u0437\u0430\u043a\u043b\u044e\u0447\u0438\u0442\u044c, \u0447\u0442\u043e \u0432\u044b\u0431\u043e\u0440 \u043c\u043e\u0434\u0435\u043b\u0438 \u0432\u0430\u0436\u043d\u0435\u0435, \u0447\u0435\u043c \u0431\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u043a\u0430 \u043a\u043b\u0430\u0441\u0441\u043e\u0432.","3a129e72":"\u041d\u0430 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0439 \u0441\u0442\u0440\u043e\u0447\u043a\u0435 \u0434\u0435\u043b\u0430\u044e `oversampling` \u043c\u043e\u0434\u0435\u043b\u0438","03376946":"### \u0411\u0430\u043b\u0430\u043d\u0441\u0438\u0440\u043e\u0432\u043a\u0430 \u043a\u043b\u0430\u0441\u0441\u043e\u0432\n\n\u0422\u0435\u043f\u0435\u0440\u044c \u043f\u043e\u043f\u0440\u043e\u0431\u0443\u044e \u0441\u0434\u0435\u043b\u0430\u0442\u044c `oversampling`, \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u044e \u043a\u0430\u043a \u043e\u043d \u043f\u043e\u0432\u043b\u0438\u044f\u0435\u0442 \u043d\u0430 \u043c\u043e\u0434\u0435\u043b\u0438"}}