f\"{define_name}_{2018+int(x_count_range)}_{att_name}\", \" DONE\")\n        globals()[f\"{define_name}_{2018+int(x_count_range)}_LON\"] = x_def_name.variables[\"longitude\"][:]\n        print(f\"{define_name}_{2018+int(x_count_range)}_LON\", \" DONE\")\n        globals()[f\"{define_name}_{2018+int(x_count_range)}_LAT\"] = x_def_name.variables[\"latitude\"][:]\n        print(f\"{define_name}_{2018+int(x_count_range)}_LAT\", \" DONE\")\n        globals()[f\"{define_name}_{2018+int(x_count_range)}_TIME\"] = x_def_name.variables[\"time\"][:]\n        print(f\"{define_name}_{2018+int(x_count_range)}_TIME\", \" DONE\")\n        print(\"----\"*10)\n        print(\"\\n\")","9759bacd":"def general_checking(att_list=list,name_year=list):\n    for x_cocp_name, x_year_name in zip(att_list,name_year):\n        print(\"\\033[1;33m%s\\x1b[0m\" % (x_year_name))\n        print(repr(x_cocp_name))\n        print(\"\\n\")","e5250958":"def variables_checking(att_list=list,name_year=list):\n    for x_cocp_name, x_year_name in zip(att_list,name_year):\n        print(\"\\033[1;33m%s\\x1b[0m\" % (x_year_name))\n        print(repr(x_cocp_name.variables))\n        print(\"\\n\")","6aca7172":"def keys_checking(att_list=list,name_year=list):\n    for x_cocp_name, x_year_name in zip(att_list,name_year):\n        print(\"\\033[1;33m%s\\x1b[0m\" % (x_year_name))\n        print(repr(x_cocp_name.variables.keys()))\n        print(\"\\n\")","2066cb5f":"def dimension_checking(att_list=list,name_year=list,var\u0131ables_name=str):\n    print(f\"{var\u0131ables_name} DIMENSION\")\n    print(\"---\"*10)\n    for x_cocp_name, x_year_name in zip(att_list,name_year):\n        print(\"\\033[1;33m%s\\x1b[0m\" % (x_year_name))\n        print(repr(x_cocp_name.variables[var\u0131ables_name].dimensions))\n        print(\"\\n\")\n\n    print(\"latitude DIMENSION\")\n    print(\"---\"*10)\n    for x_cocp_name, x_year_name in zip(att_list,name_year):\n        print(\"\\033[1;33m%s\\x1b[0m\" % (x_year_name))\n        print(repr(x_cocp_name.variables[\"latitude\"].dimensions))\n        print(\"\\n\")\n\n    print(\"longitude DIMENSION\")\n    print(\"---\"*10)\n    for x_cocp_name, x_year_name in zip(att_list,name_year):\n        print(\"\\033[1;33m%s\\x1b[0m\" % (x_year_name))\n        print(repr(x_cocp_name.variables[\"longitude\"].dimensions))\n        print(\"\\n\")\n\n    print(\"time DIMENSION\")\n    print(\"---\"*10)\n    for x_cocp_name, x_year_name in zip(att_list,name_year):\n        print(\"\\033[1;33m%s\\x1b[0m\" % (x_year_name))\n        print(repr(x_cocp_name.variables[\"time\"].dimensions))\n        print(\"\\n\")","fb16928d":"plt.style.use(\"dark_background\")\nplt.rcParams.update({\"font.size\":22})","174cdde9":"def displaying_map(source):\n    \n    figure = plt.figure(figsize=(10,10))\n\n    Image_List = []\n    plt.style.use(\"dark_background\")\n    for indexing in source:\n        \n        Read_IMG = plt.imshow(indexing, animated=True,cmap=\"hot\")\n        plt.axis('off')\n        Image_List.append([Read_IMG])\n\n    Animation_Func = animation.ArtistAnimation(figure, Image_List, interval=350, repeat_delay=10200)\n    \n    plt.close()\n    \n    return Animation_Func","0a55572f":"c_longitude,c_latitude = 12,46\nextent_real = [39, 57, 11, 20]","79e4b567":"def mapping_all(lon_p,lat_p,z_p,data_params):\n    figure = plt.figure(figsize=(10,10))\n\n    axis_func = plt.axes(projection=ccrs.Orthographic(c_longitude,c_latitude))\n    axis_func.set_global()\n    axis_func.add_feature(cartopy.feature.LAND, edgecolor='black')\n    axis_func.add_feature(cartopy.feature.BORDERS, linestyle='-', alpha=1)\n    axis_func.add_feature(cartopy.feature.OCEAN,facecolor=(\"blue\"))\n    axis_func.coastlines(resolution=\"110m\",linewidth=3)\n    axis_func.gridlines(linestyle='--',color='green',linewidth=2)\n\n    axis_func.set_extent(extent_real)\n\n    plt.contourf(lon_p, lat_p, z_p[0,:,:], transform=ccrs.PlateCarree(),cmap=\"hot\")\n\n    color_bar_func = plt.colorbar(ax=axis_func, orientation=\"vertical\", pad=0.05, aspect=16, shrink=0.5).set_label(\"UNIT DENS\",size=15,weight='bold')\n    #color_bar_func.ax.tick_params(labelsize=7)\n\n    plt.title(data_params.long_name)\n    plt.tight_layout()\n    plt.show()","ca5f2235":"CPC_URL_2018 = \"..\/input\/yemen-climate-data-for-the-last-4-years\/YEMEN_CASE\/CO_chemical_production_non_biomass_burning\/2018\/tavg3_2d_tag_Nx-202112131248output.19962.webform.nc4\"\nCPC_URL_2019 = \"..\/input\/yemen-climate-data-for-the-last-4-years\/YEMEN_CASE\/CO_chemical_production_non_biomass_burning\/2019\/tavg3_2d_tag_Nx-202112131248output.20024.webform.nc4\"\nCPC_URL_2020 = \"..\/input\/yemen-climate-data-for-the-last-4-years\/YEMEN_CASE\/CO_chemical_production_non_biomass_burning\/2020\/tavg3_2d_tag_Nx-202112131249output.20278.webform.nc4\"\nCPC_URL_2021 = \"..\/input\/yemen-climate-data-for-the-last-4-years\/YEMEN_CASE\/CO_chemical_production_non_biomass_burning\/2021\/tavg3_2d_tag_Nx-202112131249output.20362.webform.nc4\"","a5a2ddbf":"CPC_Data_2018 = Dataset(CPC_URL_2018)\nCPC_Data_2019 = Dataset(CPC_URL_2019)\nCPC_Data_2020 = Dataset(CPC_URL_2020)\nCPC_Data_2021 = Dataset(CPC_URL_2021)","99a5e71c":"DATA_ALL_CO_CP = [CPC_Data_2018,CPC_Data_2019,CPC_Data_2020,CPC_Data_2021]","1df30228":"general_checking(DATA_ALL_CO_CP,NAME_YEAR_LIST)","20a3d008":"variables_checking(DATA_ALL_CO_CP,NAME_YEAR_LIST)","03e9ddf1":"keys_checking(DATA_ALL_CO_CP,NAME_YEAR_LIST)","91ad20c8":"dimension_checking(DATA_ALL_CO_CP,NAME_YEAR_LIST,\"copdnbgl\")","62f0f964":"global_define(DATA_ALL_CO_CP,\"copdnbgl\",\"DCP\")","0ba19ab7":"mapping_all(DCP_2018_LON,DCP_2018_LAT,DCP_2018_copdnbgl,CPC_Data_2018.variables[\"copdnbgl\"])","64466aec":"mapping_all(DCP_2019_LON,DCP_2019_LAT,DCP_2019_copdnbgl,CPC_Data_2019.variables[\"copdnbgl\"])","ce2422ef":"mapping_all(DCP_2020_LON,DCP_2020_LAT,DCP_2020_copdnbgl,CPC_Data_2020.variables[\"copdnbgl\"])","3a835270":"mapping_all(DCP_2021_LON,DCP_2021_LAT,DCP_2021_copdnbgl,CPC_Data_2021.variables[\"c