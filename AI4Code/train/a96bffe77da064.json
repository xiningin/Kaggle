\"{col}: {len(df_train[col].unique())}\")","766bc70c":"df_train['dataset_title'].value_counts()","b1c9f4dc":"df_test=pd.read_csv('\/kaggle\/input\/coleridgeinitiative-show-us-the-data\/sample_submission.csv')\ndf_test","7808846b":"# get a small subset of the df_train dataset\n\nsubset_nfl_data = df_train.loc[:, 'Id':'pub_title'].head()\nsubset_nfl_data\n","df715aba":"words =list( df_train['pub_title'].values)\nwords=[word.split() for word in words]\n#words","a5d21756":"allwords = []\nfor wordlist in words:\n    allwords += wordlist","f447b1a7":"mostcommon = FreqDist(allwords).most_common(100)\nwordcloud = WordCloud(width=1600, height=800, background_color='white').generate(str(mostcommon))\nfig = plt.figure(figsize=(30,10), facecolor='white')\nplt.imshow(wordcloud, interpolation=\"bilinear\")\nplt.axis('off')\nplt.title('Top 100 Most Common Words in Data Label', fontsize=50)\nplt.tight_layout(pad=0)\nplt.show()","cbad87c4":"mostcommon_small = FreqDist(allwords).most_common(25)\nx, y = zip(*mostcommon_small)\nplt.figure(figsize=(50,30))\nplt.margins(0.02)\nplt.bar(x, y)\nplt.xlabel('Words', fontsize=50)\nplt.ylabel('Frequency of Words', fontsize=50)\nplt.yticks(fontsize=40)\nplt.xticks(rotation=60, fontsize=40)\nplt.title('Freq of 25 Most Common Words in Data-Label', fontsize=60)\nplt.show()","211227cc":"df_test_input = pd.DataFrame(columns=['id','section_title','text'])\nids=df_test['Id'].values\nimport warnings\nwarnings.filterwarnings(\"ignore\", 'This pattern has match groups')\n#len(ids)\nfor id in ids:\n    df=pd.read_json ('..\/input\/coleridgeinitiative-show-us-the-data\/test\/{}.json'.format(id))\n    df.loc[:,['id']] = id\n    df.reset_index(inplace=True,drop=True)\n    df_test_input=pd.concat([df_test_input, df], ignore_index=True)\n    df_test_input.reset_index(inplace=True,drop=True) ","be6c8dfe":"def clean_text(txt):\n    return re.sub('[^A-Za-z0-9]+', ' ', str(txt).lower()).strip()\nsubmission_df = pd.read_csv('..\/input\/coleridgeinitiative-show-us-th