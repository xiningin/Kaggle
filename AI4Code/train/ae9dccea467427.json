","168e4c72":"df_december=df[df['Month']==12].sort_values('collision_time')\ndf_december['collision_time']=pd.to_datetime(df_december['collision_time'])\ndf['hour']=df_december['collision_time'].dt.hour","26380999":"px.bar(data_frame=df[df['Month']==12],x='hour',y='killed_victims')","0b4caaa4":"group=data.groupby('collision_severity').count()","0dd94b6f":"px.bar(data_frame=group,x=group.index,y='case_id',template='plotly_dark')","5dcbdd91":"# getting the day data form the collision date\ndf['day']= pd.DatetimeIndex(df['collision_date']).day","97476b9d":"group_day=df.groupby('day').mean()","a3d32c1e":"m=group_day['killed_victims'].mean()","fd4fe645":"fig=px.bar(data_frame=group_day,x=group_day.index,y='killed_victims')\nfig.add_shape(type='line',x0=0,x1=32,y1=m,y0=m,line=dict(        color=\"Red\",\n        width=4,\n        dash=\"dashdot\",))","376681ca":"# getting the year data from the collision date\ndf['year']=pd.DatetimeIndex(df['collision_date']).year","3dde35f2":"group_year=df.groupby('year').sum()","44fa1317":"x=np.asarray(list(group_year.index)).astype('float').reshape(-1,1)","634819b5":"y=group_year['killed_victims'].values","7ccf644b":"lr=LinearRegression()\nlr.fit(x,group_year['killed_victims'].values)\npred=lr.predict(x)\n","5c014f12":"fig=go.Figure()\nfig.add_trace(go.Bar(name='Year vs Killed_Victims', x=group_year.index, y=group_year['killed_victims'].values))\nfig.add_trace(go.Scatter(name='line of best fit', x=group_year.index, y=pred, mode='lines'))\n","990d765b":"df_2020=df[df['year']==2020]","dc3d0683":"grouped_data=df_2020.groupby('collision_date').sum()","cace153a":"px.line(data_frame=grouped_data,x=grouped_data.index,y='killed_victims',template='plotly_dark')","f3bd4883":"df.dropna(subset=['latitude','longitude'],inplace=True)\ngroup_loc=data.groupby(['latitude','longitude']).sum()","ed94563f":"lat=[]\nlong=[]\ngroup_loc.sort_values('killed_victims',ascending=True)\nfor i in group_loc.index:\n    lat.append(i[0])\n    long.append(i[1])","5155