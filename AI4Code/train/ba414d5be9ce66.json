ncol_difference(train, 'wards_difference', 'radiant_wards', 'dire_wards')\ncol_difference(train, 'towers_killed_difference', 'radiant_towers_killed', 'dire_towers_killed')\ncol_difference(train, 'stuns_difference', 'radiant_stuns', 'dire_stuns')\ntrain.head(5)","20308a5a":"corr = train.corr()\ncorr_target = abs(corr['radiant_win'])\ncorr_target = corr_target.sort_values(ascending=False)\nprint(corr_target.head(10))","c2617d37":"selected_col = corr_target.head(15).index \ntemp = train[selected_col]\nf, ax = plt.subplots(figsize=(12, 9))\ncorr2 = temp.corr()\nsns.heatmap(corr2, vmax=.8, annot=True, square=True, cmap=\"Greens\");\nbottom, top = ax.get_ylim()\nax.set_ylim(bottom + 0.5, top - 0.5 )\nplt.show()","ece4472b":"keeping_cols = {'gold_difference', 'xp_difference', 'towers_killed_difference', 'game_time_x'}\ntrain_X = train.filter(keeping_cols, axis=1)\ntrain_Y = train[['radiant_win']]\ntrain_X.head(10)","995b4491":"train_Y.head(10)","c9bef859":"sns.set()\nsns.pairplot(train[keeping_cols])\nplt.show()","94487d90":"lr = LogisticRegression(solver='lbfgs')\ncv = ShuffleSplit(n_splits=5, test_size=0.2, random_state=8)\ncv_score_mean = (cross_val_score(lr, train_X, train_Y.values.ravel(), cv=cv, scoring='roc_auc')).mean()\nprint(cv_score_mean)","658b5f05":"sum_candidates = {'r1_gold', 'r2_gold', 'r3_gold', 'r4_gold', 'r5_gold'}\ncol_sum(test, 'radiant_gold', sum_candidates)","07dc99d5":"sum_candidates = {'r1_xp', 'r2_xp', 'r3_xp', 'r4_xp', 'r5_xp'}\ncol_sum(test, 'radiant_xp', sum_candidates)","89f7a124":"sum_candidates = {'r1_towers_killed', 'r2_towers_killed', 'r3_towers_killed', 'r4_towers_killed','r5_towers_killed'}\ncol_sum(test, 'radiant_towers_killed', sum_candidates)","b4bbfdb3":"sum_candidates = {'d1_gold', 'd2_gold', 'd3_gold', 'd4_gold', 'd5_gold'}\ncol_sum(test, 'dire_gold', sum_candidates)","da97b8bd":"sum_candidates = {'d1_xp', 'd2_xp', 'd3_xp', 'd4_xp', 'd5_xp'}\ncol_sum(test, 'dire_xp', sum_candidates)","2ff60f4e":"sum_candidates = {'d1_towers_killed', 'd2_towers_killed', 'd3_towers_killed', 'd4_towers_killed','d5_towers_killed'}\ncol_sum(test, 'dire_towers_killed', sum_candidates)","3b15759a":"col_difference(test, 'gold_difference', 'radiant_gold', 'dire_gold')\ncol_difference(test, 'xp_difference', 'radiant_xp', 'dire_xp')\ncol_difference(test, 'towers_killed_difference', 'radiant_towers_killed', 'dire_towers_killed')","3359d46e":"keeping_cols = {'xp_difference','gold_difference','towers_killed_difference','game_time'}\ntest = test.filter(keeping_cols,axis=1)\ntest.head(10)","2853bf0d":"lr.fit(train_X, train_Y.values.ravel())\nsubmission = pd.DataFrame()\nsubmission['match_id_hash'] = test.index\nsubmission['radiant_win_prob']= lr.predict_proba(test)[:, 1]\nsubmission.to_csv('submission.csv', index=False)","fe41bd04":"\u0417\u0434\u0435\u0441\u044c \u043c\u044b \u043f\u0440\u0438\u043d\u044f\u043b\u0438 \u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u043e\u0431\u044a\u0435\u0434\u0438\u043d\u0438\u0442\u044c train_features \u0438 train_targets \u0434\u043b\u044f \u0431\u043e\u043b\u044c\u0448\u0435\u0433\u043e \u0443\u0434\u043e\u0431\u0441\u0442\u0432\u0430 \u043f\u0440\u0438 \u0440\u0430\u0431\u043e\u0442\u0435 \u0441 \u0434\u0430\u043d\u043d\u044b\u043c\u0438 \u0438 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u044e \u043c\u043e\u0434\u0435\u043b\u0438. ","c26e0840":"### \u041e\u0431\u044a\u0435\u0434\u0438\u043d\u0435\u043d\u0438\u0435 \u043a\u043e\u043b\u043e\u043d\u043e\u043a \u0441\u0438\u043b \u0442\u044c\u043c\u044b","af202ee2":"### \u041e\u0431\u044a\u0435\u0434\u0438\u043d\u0435\u043d\u0438\u0435 \u043a\u043e\u043b\u043e\u043d\u043e\u043a \u0441\u0438\u043b \u0441\u0432\u0435\u0442\u0430","9642dab5":"![image.png](attachment:a2717ea7-972f-4954-8886-e9e48eacc371.png)","5e23bfe9":"### \u041e\u0431\u0440\u0430\u0431\u043e\u0442\u043a\u0430 \u0434\u0430\u043d\u043d\u044b\u0445 \u0432 test \u0434\u0430\u0442\u0430\u0441\u0435\u0442\u0435","1924a51d":"\u0412\u043e \u0432\u0440\u0435\u043c\u044f \u0438\u0433\u0440\u044b \u0431\u043e\u043b\u044c\u0448\u0435\u0435 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0435 \u0438\u043c\u0435\u0435\u0442 \u043e\u0431\u0449\u0435\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0437\u043e\u043b\u043e\u0442\u0430\/\u043e\u043f\u044b\u0442\u0430 \u043a\u043e\u043c\u0430\u043d\u0434\u044b, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u044d\u0442\u043e \u043a\u043e\u043c\u0430\u043d\u0434\u043d\u0430\u044f \u0438\u0433\u0440\u0430. \u041a\u043e\u043d\u0435\u0447\u043d\u043e, \u0432\u0435\u0441 \u0438\u043c\u0435\u0435\u0442 \u0438 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0437\u043e\u043b\u043e\u0442\u0430 \u0438 \u043e\u043f\u044b\u0442\u0430 \u0443 \u043a\u0435\u0440\u0440\u0438, \u043f\u043e \u0441\u0440\u0430\u0432\u043d\u0435\u043d\u0438\u044e \u0441 \u043a\u0435\u0440\u0440\u0438 \u0434\u0440\u0443\u0433\u043e\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u044b, \u043d\u043e \u0432\u0441\u0435 \u0436\u0435 \u043e\u0431\u0449\u0435\u0435 \u043f\u0440\u0435\u0438\u043c\u0443\u0449\u0435\u0441\u0442\u0432\u043e \u043f\u043e \u0437\u043e\u043b\u043e\u0442\u0443 \u0438 \u043e\u043f\u044b\u0442\u0443 \u043a\u043e\u043c\u0430\u043d\u0434\u044b \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043f\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u0435\u0435, \u043d\u0435\u0436\u0435\u043b\u0438 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u0433\u0435\u0440\u043e\u044f. \u041f\u043e\u044d\u0442\u043e\u043c\u0443 \u043c\u044b \u0441\u0443\u043c\u043c\u0438\u0440\u0443\u0435\u043c \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0437\u043e\u043b\u043e\u0442\u0430 \u0438 \u043e\u043f\u044b\u0442\u0430 \u0433\u0435\u0440\u043e\u0435\u0432 \u043a\u0430\u0436\u0434\u043e\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u044b \u0434\u043b\u044f \u0431\u043e\u043b\u0435\u0435 \u0443\u0434\u043e\u0431\u043d\u043e\u0433\u043e \u043f\u0440\u0435\u0434\u0441\u043a\u0430\u0437\u0430\u043d\u0438\u044f. \n\n\u0422\u0435\u043f\u0435\u0440\u044c \u043f\u0430\u0440\u0443 \u0441\u043b\u043e\u0432 \u043e \u0440\u0430\u0437\u0440\u0443\u0448\u0435\u043d\u043d\u044b\u0445 \u0431\u0430\u0448\u043d\u044f\u0445. \u041e\u043f\u044f\u0442\u044c \u0436\u0435, \u0440\u0430\u0437\u0440\u0443\u0448\u0435\u043d\u043d\u0430\u044f \u0431\u0430\u0448\u043d\u044f \u0434\u0430\u0435\u0442 \u043f\u0440\u0435\u0438\u043c\u0443\u0449\u0435\u0441\u0442\u0432\u043e \u043a\u043e\u043c\u0430\u043d\u0434\u0435, \u0430 \u043d\u0435 \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u043e\u043c\u0443 \u0433\u0435\u0440\u043e\u044e, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u043a\u043e\u043b\u043e\u043d\u043a\u0438, \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0431\u044b\u043b\u0438 \u0434\u0430\u043d\u043d\u044b\u0435 \u043e \u0442\u043e\u043c, \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0431\u0430\u0448\u0435\u043d \u0441\u043b\u043e\u043c\u0430\u043b \u0433\u0435\u0440\u043e\u0439 \u043c\u044b \u0441\u0443\u043c\u043c\u0438\u0440\u0443\u0435\u043c \u0438 \u043c\u044b \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u043c \u043e\u0431\u0449\u0435\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0440\u0430\u0437\u0440\u0443\u0448\u0435\u043d\u043d\u044b\u0445 \u0431\u0430\u0448\u0435\u043d \u043a\u043e\u043c\u0430\u043d\u0434\u043e\u0439. \n\n\u0422\u0430\u043a\u0436\u0435 \u0441\u0435\u0440\u044c\u0435\u0437\u043d\u0443\u044e \u0440\u043e\u043b\u044c \u0438\u0433\u0440\u0430\u0435\u0442 \u043d\u0430\u043b\u0438\u0447\u0438\u0435 \"\u0432\u0430\u0440\u0434\u043e\u0432\" - \u0442\u043e\u0447\u0435\u043a \u0432\u0438\u0434\u0438\u043c\u043e\u0441\u0442\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0431\u044b\u0432\u0430\u044e\u0442 \u0434\u0432\u0443\u0445 \u0442\u0438\u043f\u043e\u0432: \u043e\u0431\u044b\u0447\u043d\u0430\u044f \u0432\u0438\u0434\u0438\u043c\u043e\u0441\u0442\u044c \u0432 \u043d\u0435\u043a\u043e\u0435\u043c \u0440\u0430\u0434\u0438\u0443\u0441\u0435, \u0438 \u0432\u0438\u0434\u0438\u043c\u043e\u0441\u0442\u044c \u043d\u0435\u0432\u0438\u0434\u0438\u043c\u044b\u0445 \u0433\u0435\u0440\u043e\u0435\u0432. \u041d\u0430\u043b\u0438\u0447\u0438\u0435 \u0432\u0430\u0440\u0434\u043e\u0432 \u0442\u043e\u0436\u0435 \u0434\u0430\u0435\u0442 \u043f\u0440\u0435\u0438\u043c\u0443\u0449\u0435\u0441\u0442\u0432\u043e \u043a\u043e\u043c\u0430\u043d\u0434\u0435, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u0431\u043e\u043b\u0435\u0435 \u0432\u0438\u0434\u043d\u043e \u043f\u0435\u0440\u0435\u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435 \u043f\u0440\u043e\u0442\u0438\u0432\u043d\u0438\u043a\u043e\u0432 \u043f\u043e \u043a\u0430\u0440\u0442\u0435 \u0432 \u0442\u0443\u043c\u0430\u043d\u0435 \u0432\u043e\u0439\u043d\u044b. \u0422\u0430\u043a \u0447\u0442\u043e \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043d\u044b\u0435 \u0432\u0430\u0440\u0434\u044b \u043a\u0430\u0436\u0434\u044b\u043c \u0433\u0435\u0440\u043e\u0435\u043c \u0442\u043e\u0436\u0435 \u0441\u0443\u043c\u043c\u0438\u0440\u0443\u044e\u0442\u0441\u044f, \u043f\u043e\u043b\u0443\u0447\u0430\u044f \u043e\u0431\u0449\u0435\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0432\u0430\u0440\u0434\u043e\u0432 \u043a\u043e\u043c\u0430\u043d\u0434\u044b. \n\n","3e473ee7":"# \u0410\u043d\u0430\u043b\u0438\u0437 \u0432\u0445\u043e\u0434\u043d\u044b\u0445 \u0434\u0430\u043d\u043d\u044b\u0445","6abe840a":"\u0422\u0435\u043f\u0435\u0440\u044c, \u043a\u043e\u0433\u0434\u0430 \u043c\u044b \u043f\u043e\u043b\u0443\u0447\u0438\u043b\u0438 \u0434\u0430\u043d\u043d\u044b\u0435 \u043e \u0437\u043e\u043b\u043e\u0442\u0435, \u043e\u043f\u044b\u0442\u0435, \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043d\u044b\u0445 \u0432\u0430\u0440\u0434\u0430\u0445 \u0438 \u0440\u0430\u0437\u0440\u0443\u0448\u0435\u043d\u043d\u044b\u0445 \u0431\u0430\u0448\u043d\u044f\u0445 \u043a\u0430\u0436\u0434\u043e\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u044b, \u043c\u044b \u043c\u043e\u0436\u0435\u043c \u043f\u043e\u0441\u0447\u0438\u0442\u0430\u0442\u044c \u043f\u0440\u0435\u0438\u043c\u0443\u0449\u0435\u0441\u0442\u0432\u043e \u043f\u043e \u0432\u0441\u0435\u043c \u044d\u0442\u0438\u043c \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u0430\u043c. \u041c\u044b \u043f\u0440\u0438\u043d\u044f\u043b\u0438 \u0443\u0441\u043b\u043e\u0432\u0438\u0435\u043c \u043f\u043e\u0434\u0441\u0447\u0435\u0442\u0430 \"\u043e\u0442\u043d\u043e\u0441\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0441\u0438\u043b \u0441\u0432\u0435\u0442\u0430\". ","7ff121fb":"# \u041a\u0440\u0430\u0442\u043a\u0438\u0439 \u044d\u043a\u0441\u043a\u0443\u0440\u0441 \u043f\u043e Dota2, \u0435\u0441\u043b\u0438 \u0432\u044b \u0432 \u043d\u0435\u0435 \u043d\u0438\u043a\u043e\u0433\u0434\u0430 \u043d\u0435 \u0438\u0433\u0440\u0430\u043b\u0438\n\u0412 \u0438\u0433\u0440\u0435 \u0443\u0447\u0430\u0441\u0442\u0432\u0443\u044e\u0442 \u0434\u0432\u0435 \u043a\u043e\u043c\u0430\u043d\u0434\u044b: \u0421\u0438\u043b\u044b \u0421\u0432\u0435\u0442\u0430(Radiant) \u0438 \u0421\u0438\u043b\u044b \u0422\u044c\u043c\u044b(Dire). \u0421\u043e\u0431\u044b\u0442\u0438\u044f \u0440\u0430\u0437\u0432\u0438\u0432\u0430\u044e\u0442\u0441\u044f \u043d\u0430 \u043a\u0430\u0440\u0442\u0435, \u043d\u0430 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u043d\u0430\u0445\u043e\u0434\u044f\u0442\u0441\u044f \u0441\u0442\u0440\u043e\u0435\u043d\u0438\u044f \u0441\u0438\u043b \u0441\u0432\u0435\u0442\u0430 \u0438 \u0441\u0442\u0440\u043e\u0435\u043d\u0438\u044f \u0441\u0438\u043b \u0442\u044c\u043c\u044b. \u0412\u043e\u0442 \u0442\u0430\u043a \u0432\u044b\u0433\u043b\u044f\u0434\u0438\u0442 \u043a\u0430\u0440\u0442\u0430: \n![image.png](attachment:8353a33e-afab-4e0c-bded-4f4d6eb2fe71.png)\n\n\n\u0423 \u043a\u0430\u0436\u0434\u043e\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u044b \u0438\u0437\u043d\u0430\u0447\u0430\u043b\u044c\u043d\u043e \u0435\u0441\u0442\u044c \u0441\u0432\u043e\u044f \u0431\u0430\u0437\u0430 \u043d\u0430 \u0441\u0432\u043e\u0435\u0439 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0435 \u043a\u0430\u0440\u0442\u044b, \u0438 \u043d\u0430 \u043a\u0430\u0440\u0442\u0435 \u0435\u0441\u0442\u044c \u0442\u0440\u0438 \u043b\u0438\u043d\u0438\u0438: \u0432\u0435\u0440\u0445\u043d\u044f\u044f(top), \u0446\u0435\u043d\u0442\u0440\u0430\u043b\u044c\u043d\u0430\u044f(mid), \u043d\u0438\u0436\u043d\u044f\u044f(bottom) \u0438 \u043d\u0430 \u043a\u0430\u0436\u0434\u043e\u0439 \u043b\u0438\u043d\u0438\u0438 \u043f\u043e 3 \u0437\u0430\u0449\u0438\u0442\u043d\u044b\u0435 \u0431\u0430\u0448\u043d\u0438. \u041e\u0441\u043d\u043e\u0432\u043d\u0430\u044f \u0437\u0430\u0434\u0430\u0447\u0430 - \u0443\u043d\u0438\u0447\u0442\u043e\u0436\u0438\u0442\u044c \u0431\u0430\u0437\u0443 \u043f\u0440\u043e\u0442\u0438\u0432\u043d\u0438\u043a\u0430 \u0438 \u043d\u0435 \u0434\u0430\u0442\u044c \u0443\u043d\u0438\u0447\u0442\u043e\u0436\u0438\u0442\u044c \u0441\u0432\u043e\u044e. \n\u0422\u0430\u043a\u0436\u0435 \u0435\u0441\u0442\u044c \u043f\u044f\u0442\u044c \u0440\u043e\u043b\u0435\u0439: \n* \u043f\u043e\u043b\u043d\u0430\u044f \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u043a\u0430 (\u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u043c \u0431\u043e\u0435\u0432\u044b\u043c \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u0436\u0430\u043c)\n* \u0447\u0430\u0441\u0442\u0438\u0447\u043d\u0430\u044f \u043f\u043e\u0434\u0434\u0435\u0440\u0436\u043a\u0430 (\u0442\u0430\u043a\u0436\u0435 \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442, \u043d\u043e \u0443\u0436\u0435 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0431\u043e\u043b\u0435\u0435 \u0431\u043e\u0435\u0432\u043e\u0439 \u0435\u0434\u0438\u043d\u0438\u0446\u0435\u0439)\n* \"\u0442\u0430\u043d\u043a\", \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0432 \u0434\u0440\u0430\u043a\u0430\u0445 \u0434\u043e\u043b\u0436\u0435\u043d \u0432\u043f\u0438\u0442\u044b\u0432\u0430\u0442\u044c \u0431\u043e\u043b\u044c\u0448\u0443\u044e \u0447\u0430\u0441\u0442\u044c \u0443\u0440\u043e\u043d\u0430 \u0438 \u0437\u0430\u0431\u0438\u0440\u0430\u0442\u044c \u0444\u043e\u043a\u0443\u0441 \u043d\u0430 \u0441\u0435\u0431\u044f\n* \u043c\u0438\u0434\u0435\u0440 - \u0438\u0433\u0440\u043e\u043a, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0441\u043d\u0430\u0447\u0430\u043b\u0430 \u043e\u0434\u0438\u043d \u0438\u0433\u0440\u0430\u0435\u0442 \u043d\u0430 \u0446\u0435\u043d\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0439 \u043b\u0438\u043d\u0438\u0438 \u0438 \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0442 \u0431\u043e\u043b\u044c\u0448\u0435 \u0434\u0435\u043d\u0435\u0433 \u0438 \u043e\u043f\u044b\u0442\u0430\n* \u043a\u0435\u0440\u0440\u0438 - \u043e\u0441\u043d\u043e\u0432\u043d\u0430\u044f \u0431\u043e\u0435\u0432\u0430\u044f \u0435\u0434\u0438\u043d\u0438\u0446\u0430, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u0432 \u043f\u043e\u0441\u043b\u0435\u0434\u0441\u0442\u0432\u0438\u0438 \u0444\u0430\u0440\u043c\u0430 (\u0437\u0430\u0440\u0430\u0431\u0430\u0442\u044b\u0432\u0430\u043d\u0438\u044f \u0434\u0435\u043d\u0435\u0433 \u0437\u0430 \u0443\u0431\u0438\u0439\u0441\u0442\u0432\u043e \u043a\u0440\u0438\u043f\u043e\u0432 \u0438 \u0443\u0431\u0438\u0439\u0441\u0442\u0432\u0430 \u0433\u0435\u0440\u043e\u0435\u0432), \u0431\u0443\u0434\u0435\u0442 \u043d\u0430\u043d\u043e\u0441\u0438\u0442\u044c \u043d\u0430\u0438\u0431\u043e\u043b\u044c\u0448\u0435\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0443\u0440\u043e\u043d\u0430 \u0441\u0440\u0435\u0434\u0438 \u0441\u0432\u043e\u0435\u0439 \u043a\u043e\u043c\u0430\u043d\u0434\u044b. \n\n\u0422\u0430\u043a \u0432\u043e\u0442 \u0432 \u0447\u0435\u043c \u0441\u0443\u0442\u044c - \u0447\u0435\u043c \u0431\u043e\u043b\u044c\u0448\u0435 \u0434\u0435\u043d\u0435\u0433 \u0438 \u043e\u043f\u044b\u0442\u0430 \u0443 \u0433\u0435\u0440\u043e\u0435\u0432 \u043a\u043e\u043c\u0430\u043d\u0434\u044b, \u0442\u0435\u043c \u0431\u043e\u043b\u044c\u0448\u0435 \u0443 \u043d\u0438\u0445 \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0445 \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u043e\u0432 \u0438 \u0441\u0438\u043b\u044b, \u0442\u0435\u043c \u0431\u043e\u043b\u044c\u0448\u0430\u044f \u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e\u0441\u0442\u044c \u043f\u043e\u0431\u0435\u0434\u044b. \u0411\u043e\u043b\u0435\u0435 \u043f\u043e\u0434\u0440\u043e\u0431\u043d\u043e \u043e \u0432\u0430\u0436\u043d\u043e\u0441\u0442\u0438 \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0434\u0430\u043d\u043d\u044b\u0445 \u043c\u044b \u0440\u0430\u0441\u0441\u043a\u0430\u0436\u0435\u043c \u0432 \u043f\u0440\u043e\u0446\u0435\u0441\u0441\u0435 \u0432\u044b\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u044f \u0440\u0430\u0431\u043e\u0442\u044b. ","dffaab16":"### \u0422\u0440\u0435\u043d\u0438\u0440\u043e\u0432\u043a\u0430 \u043c\u043e\u0434\u0435\u043b\u0438","50f31479":"### Label encoding \u0434\u043b\u044f radiant_win col\n\u0422\u0443\u0442 \u043c\u044b \u043f\u0440\u0435\u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043b\u0438 \u0431\u0443\u043b\u0435\u0432\u044b\u0435 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u0432 \u043a\u043e\u043b\u043e\u043d\u043a\u0435 radiant_win \u0432 \u0447\u0438\u0441\u043b\u043e\u0432\u044b\u0435, \u043e\u043f\u044f\u0442\u044c \u0436\u0435, \u0434\u043b\u044f \u0443\u0434\u043e\u0431\u0441\u0442\u0432\u0430 \u043e\u0431\u0443\u0447\u0435\u043d\u0438\u044f \u043c\u043e\u0434\u0435\u043b\u0438. ","e0a98354":"\u0418\u0441\u0445\u043e\u0434\u044f \u0438\u0437 \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043d\u043e\u0439 \u043c\u0430\u0442\u0440\u0438\u0446\u044b \u043a\u043e\u0440\u0435\u043b\u043b\u044f\u0446\u0438\u0438 \u043c\u043e\u0436\u043d\u043e \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u0432\u044b\u0432\u043e\u0434, \u0447\u0442\u043e \u043d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u043a\u043e\u0440\u0435\u043b\u043b\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u043c\u0438 \u044f\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u0440\u0430\u0437\u043d\u0438\u0446\u0430 \u0432 \u0437\u043e\u043b\u043e\u0442\u0435 \u0438 \u043e\u043f\u044b\u0442\u0435 \u043a\u043e\u043c\u0430\u043d\u0434, \u0440\u0430\u0437\u043d\u0438\u0446\u0430 \u0432 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0435 \u0440\u0430\u0437\u0440\u0443\u0448\u0435\u043d\u043d\u044b\u0445 \u0431\u0430\u0448\u0435\u043d. \u0418 \u0441 \u044d\u0442\u0438\u043c \u043c\u043e\u0433\u0443\u0442 \u0441\u043e\u0433\u043b\u0430\u0441\u0438\u0442\u044c\u0441\u044f \u0438\u0433\u0440\u043e\u043a\u0438. \u0422\u0430\u043a\u0436\u0435, \u0434\u0430\u0436\u0435 \u0432\u043e \u0432\u0440\u0435\u043c\u044f \u0441\u0430\u043c\u043e\u0439 \u0438\u0433\u0440\u044b, \u044d\u0442\u0438 \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u044b \u043d\u0430\u0438\u0431\u043e\u043b\u0435\u0435 \u043f\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0435. \n\n\u041d\u043e \u0432\u0441\u0435 \u0436\u0435, \u0435\u0441\u0442\u044c \u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e\u0441\u0442\u044c \u0442\u0430\u043a \u043d\u0430\u0437\u044b\u0432\u0430\u0435\u043c\u043e\u0433\u043e \"\u043a\u0430\u043c\u0431\u0435\u043a\u0430\" - \u043a\u043e\u0433\u0434\u0430 \u043f\u0440\u043e\u0438\u0433\u0440\u044b\u0432\u0430\u044e\u0449\u0430\u044f \u043a\u043e\u043c\u0430\u043d\u0434\u0430 \u0432\u043e \u0432\u0440\u0435\u043c\u044f \u0434\u043e\u043b\u0433\u043e\u0439 \u0438\u0433\u0440\u044b \u043c\u043e\u0436\u0435\u0442 \u0443\u0441\u043f\u0435\u0448\u043d\u043e \u0432\u044b\u0438\u0433\u0440\u0430\u0442\u044c \u0434\u0440\u0430\u043a\u0443 \u0438 \u0437\u0430\u043a\u0440\u0435\u043f\u0438\u0442\u044c \u043f\u0440\u0435\u0438\u043c\u0443\u0449\u0435\u0441\u0442\u0432\u043e \u0437\u0430 \u0441\u043e\u0431\u043e\u0439, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u043a\u043e\u043b\u043e\u043d\u043a\u0443 \u0441\u043e \u0432\u0440\u0435\u043c\u0435\u043d\u0435\u043c \u0438\u0433\u0440\u044b \u0442\u043e\u0436\u0435 \u0441\u043b\u0435\u0434\u0443\u0435\u0442 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043f\u0440\u0438 \u043f\u0440\u043e\u0433\u043d\u043e\u0437\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0438. ","564c06f9":"### \u041e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u043c \u0442\u043e\u043b\u044c\u043a\u043e \u0440\u0435\u043b\u0435\u0432\u0430\u043d\u0442\u043d\u044b\u0435 \u043a\u043e\u043b\u043e\u043d\u043a\u0438 "}}