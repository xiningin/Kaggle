u0438\u0441\u043b\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u0435\u0439 \u0443\u043c\u0435\u043d\u044c\u0448\u0430\u0435\u0442\u0441\u044f \u0441 \u0440\u043e\u0441\u0442\u043e\u043c \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043e\u043d\u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442. \u0422\u043e \u0435\u0441\u0442\u044c \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u0442 \u043c\u0430\u043b\u043e \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442, \u0430 \u043c\u0435\u043d\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e - \u043c\u043d\u043e\u0433\u043e. \u042d\u0442\u043e \u0438 \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0436\u0434\u0430\u0435\u0442 \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u0435\u043d\u0438\u0435 \u043f\u043e \u043a\u0432\u0430\u0440\u0442\u0435\u043b\u044f\u043c \u0432 \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u0430\u0445","fb90a37a":"\u041f\u043e\u0432\u0442\u043e\u0440\u044f\u0435\u043c \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u0438\u0435 \u043c\u0430\u0442\u0440\u0438\u0446\u044b \u043a\u043e\u0440\u0440\u0435\u043b\u044f\u0446\u0438\u0439 \u0421\u043f\u0438\u0440\u043c\u0435\u043d\u0430","816abf74":"\u0422\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c:\n\n\u041f\u0435\u0440\u0432\u0430\u044f \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u0430 \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0434\u0438\u043b\u0430\u0441\u044c \u0432 \u0442\u043e\u043c, \u0447\u0442\u043e \u0432\u0437\u0430\u0438\u043c\u043e\u0441\u0432\u044f\u0437\u044c \u0435\u0441\u0442\u044c, \u044d\u0442\u043e \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0436\u0434\u0430\u0435\u0442 \u043c\u0430\u043b\u043e\u0435 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0435 p-value.\n\u0410 \u0432\u043e\u0442 \u0442\u043e, \u0447\u0442\u043e \u0442\u0435, \u043a\u0442\u043e \u0431\u044b\u0432\u0430\u044e\u0442 \u0437\u0430 \u0433\u0440\u0430\u043d\u0438\u0446\u0435\u0439, \u043e\u0431\u043b\u0430\u044e\u0442 \u0431\u0435\u043b\u0435\u0435 \u0434\u043e\u0440\u043e\u0433\u0438\u043c\u0438 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430\u043c\u0438 - \u043d\u0435\u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0434\u0438\u043b\u043e\u0441\u044c, \u0430 \u0432\u044b\u044f\u0441\u043d\u0438\u043b\u043e\u0441\u044c, \u0447\u0442\u043e \u043d\u0430\u043e\u0431\u043e\u0440\u043e\u0442.\n\n\u0412\u0442\u043e\u0440\u0430\u044f \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u0430 \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0434\u0438\u043b\u0430\u0441\u044c \u0432 \u0442\u043e\u043c, \u0447\u0442\u043e \u0435\u0441\u0442\u044c \u043a\u043e\u0440\u0440\u0435\u043b\u044f\u0446\u0438\u044f \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0432 \u0431\u0443\u0434\u043d\u0438\u0435 \u0434\u043d\u0438 \u0432 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u0445 \u0438 \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0432 \u0432\u044b\u0445\u043e\u0434\u043d\u044b\u0435. \u0422\u0430\u043a\u0436\u0435 \u0431\u044b\u043b\u043e \u0432\u044b\u044f\u0432\u043b\u0435\u043d\u043e \u0447\u0442\u043e \u043f\u0435\u0440\u0432\u044b\u0435 \u0434\u0432\u0435 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043d\u044b\u0435 \u0433\u0440\u0443\u043f\u043f\u044b \u0437\u0430\u043d\u0438\u043c\u0430\u044e\u0442 \u043b\u0438\u0434\u0438\u0440\u0443\u044e\u0449\u0438\u0435 \u043f\u043e\u0437\u0438\u0446\u0438\u0438 \u0432 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u043c\u044b\u0445 \u043e\u0431\u044a\u0435\u043c\u0430\u0445 \u0442\u0440\u0430\u0444\u0438\u043a\u0430, \u0430 \u043d\u0430\u0447\u0438\u043d\u0430\u044f \u0441 3 \u0433\u0440\u0443\u043f\u043f\u044b \u043d\u0430\u0431\u043b\u044e\u0434\u0430\u0435\u0442\u0441\u044f \u0441\u043d\u0438\u0436\u0435\u043d\u0438\u0435 \u0442\u0440\u0430\u0444\u0438\u043a\u0430. \u041c\u043e\u0436\u043d\u043e \u0441\u0447\u0438\u0442\u0430\u0442\u044c, \u0447\u0442\u043e \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0434\u0438\u043b\u043e\u0441\u044c \u0442\u043e, \u0447\u0442\u043e \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u044b \u043f\u043e\u043c\u043b\u0430\u0434\u0448\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442 \u0431\u043e\u043b\u044c\u0448\u0435 \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0432 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u0445.","8908e3f4":"### \u041f\u0440\u043e\u0432\u0435\u0440\u0438\u043c \u043f\u0435\u0440\u0432\u0443\u044e \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u0443","690a001d":"\u0422\u0435\u043f\u0435\u0440\u044c \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u0438\u043c \u043a\u0430\u043a \u0441\u0432\u044f\u0437\u0430\u043d\u044b \u0444\u0430\u043a\u0442 \u043f\u043e\u0435\u0437\u0434\u043a\u0438 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433 \u0437\u0430 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0435 12 \u043c\u0435\u0441\u044f\u0446\u0435\u0432 (0 \u2013 \u043d\u0435\u0442, 1 \u2013 \u0435\u0441\u0442\u044c) \u0438 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u0438 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430 ( 1,2,3,4, \u0433\u0434\u0435 1 \u0441\u0430\u043c\u0430\u044f \u0434\u043e\u0440\u043e\u0433\u0430\u044f \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f).","9c53aeac":"\u0422\u0435\u043f\u0435\u0440\u044c \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u0438\u043c \u043a\u0430\u043a \u0440\u0430\u0441\u043f\u0440\u0435\u0434\u0435\u043b\u044f\u0435\u0442\u0441\u044f \u043e\u0431\u044a\u0435\u043c \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u043c\u044b\u0445 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442 \u0432 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043d\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u044b \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u043e\u0432.","57a897a0":"\u041f\u043e\u0441\u043c\u043e\u0442\u0440\u0438\u043c \u043d\u0430 \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u043f\u0430\u0440\u0430\u043c\u0435\u0442\u0440\u044b \u043d\u0430\u0448\u0438\u0445 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0445 \u0434\u0430\u043d\u043d\u044b\u0445","e16ef7e2":"\u0428\u0438\u0440\u0438\u043d\u0430 \"\u044f\u0449\u0438\u043a\u0430\" \u0443\u0437\u043a\u0430\u044f. \u0417\u043d\u0430\u0447\u0438\u0442 \u0438\u043d\u0442\u0435\u0440\u043a\u0432\u0430\u0440\u0442\u0438\u043b\u044c\u043d\u044b\u0439 \u0440\u0430\u0437\u043c\u0430\u0445 (\u0440\u0430\u0437\u043d\u043e\u0441\u0442\u044c \u043c\u0435\u0436\u0434\u0443 \u0442\u0440\u0435\u0442\u044c\u0438\u043c  Q3  \u0438 \u043f\u0435\u0440\u0432\u044b\u043c  Q1  \u043a\u0432\u0430\u0440\u0442\u0438\u043b\u044f\u043c\u0438) \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0430\u044f. \u0412\u0435\u0440\u0442\u0438\u043a\u0430\u043b\u044c\u043d\u0430\u044f \u043b\u0438\u043d\u0438\u044f \u0432\u043d\u0443\u0442\u0440\u0438 \u044f\u0449\u0438\u043a\u0430, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442 \u043c\u0435\u0434\u0438\u0430\u043d\u0443 (\u0432\u0442\u043e\u0440\u043e\u0439 \u043a\u0432\u0430\u0440\u0442\u0438\u043b\u044c), \u043d\u0435 \u0432\u0438\u0434\u043d\u0430, \u043e\u043d\u0430 \u0441\u043b\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u0441 \u043f\u0435\u0440\u0432\u044b\u043c \u043a\u0432\u0430\u0440\u0442\u0438\u043b\u0435\u043c. \"\u0423\u0441\u0438\u043a\", \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0438\u0432\u0430\u044e\u0442 \u0442\u043e\u0447\u043a\u0438, \u043f\u043e\u043f\u0430\u0434\u0430\u044e\u0449\u0438\u0435 \u0432 \u0438\u043d\u0442\u0435\u0440\u0432\u0430\u043b  [Q1\u22121.5\u2217IQR;Q3+1.5\u2217IQR] , \u0433\u0434\u0435  IQR  --- \u0438\u043d\u0442\u0435\u0440\u043a\u0432\u0430\u0440\u0442\u0438\u043b\u044c\u043d\u044b\u0439 \u0440\u0430\u0437\u043c\u0430\u0445, - \u0442\u043e\u043b\u044c\u043a\u043e \u043e\u0434\u0438\u043d, \u0441\u043c\u0435\u0449\u0435\u043d\u0438\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0432 \u043e\u0434\u043d\u0443 \u0441\u0442\u043e\u0440\u043e\u043d\u0443. \u0418 \u0432\u0438\u0434\u0438\u043c \u043e\u0442\u0434\u0435\u043b\u044c\u043d\u044b\u0435 \u0442\u043e\u0447\u043a\u0438 \u043d\u0430 \u0433\u0440\u0430\u0444\u0438\u043a\u0435, \u043e\u043d\u0438 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044e\u0442 \u0432\u044b\u0431\u0440\u043e\u0441\u0430\u043c --- \u043d\u0435\u0442\u0438\u043f\u0438\u0447\u043d\u044b\u043c \u0434\u043b\u044f \u0434\u0430\u043d\u043d\u043e\u0439 \u0432\u044b\u0431\u043e\u0440\u043a\u0438 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f\u043c. \u0418\u0445 \u043e\u043a\u0430\u0437\u0430\u043b\u043e\u0441\u044c \u0434\u043e\u0432\u043e\u043b\u044c\u043d\u043e \u043c\u043d\u043e\u0433\u043e.","58d47a3f":"## \u0413\u0438\u043f\u043e\u0442\u0435\u0437\u044b\n\n1. \u0421\u0442\u043e\u043b\u0431\u0446\u044b: **\u0424\u0430\u043a\u0442 \u043f\u043e\u0435\u0437\u0434\u043a\u0438 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u00a0 \u0437\u0430 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0435\u00a012 \u043c\u0435\u0441\u044f\u0446\u0435\u0432 (0 \u2013 \u043d\u0435\u0442, 1 \u2013 \u0435\u0441\u0442\u044c)\u00a0** \u0438 **\u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u0438 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430 ( 1,2,3,4, \u0433\u0434\u0435 1 \u0441\u0430\u043c\u0430\u044f \u0434\u043e\u0440\u043e\u0433\u0430\u044f \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f)**. \u041e\u0431\u0430 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0430\u043b\u044c\u043d\u044b\u0435. \u041f\u0440\u043e\u0432\u0435\u0440\u044f\u0435\u043c \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u0443, \u0441\u0432\u044f\u0437\u0430\u043d\u044b \u043b\u0438 \u043e\u043d\u0438. \u041f\u0440\u0430\u0432\u0434\u0430 \u043b\u0438, \u0447\u0442\u043e \u0442\u0435 \u043a\u0442\u043e \u0431\u044b\u0432\u0430\u044e\u0442 \u0437\u0430 \u0433\u0440\u0430\u043d\u0438\u0446\u0435\u0439, \u043e\u0431\u043b\u0430\u044e\u0442 \u0431\u0435\u043b\u0435\u0435 \u0434\u043e\u0440\u043e\u0433\u0438\u043c\u0438 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430\u043c\u0438?\n\n2. \u0421\u0442\u043e\u043b\u0431\u0446\u044b: **\u0414\u0430\u0442\u0430 \u0442\u0440\u0430\u0444\u0438\u043a \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u0430 \u0432 \u0431\u0443\u0434\u043d\u0438\u0435 \u0434\u043d\u0438 \u0432 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u0445** \u0438 **\u0414\u0430\u0442\u0430 \u0442\u0440\u0430\u0444\u0438\u043a \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u0430 \u0432 \u0432\u044b\u0445\u043e\u0434\u043d\u044b\u0435 \u0432 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u0445** + **\u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043d\u0430\u044f \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f** (\u0446\u0435\u043b\u0435\u0432\u043e\u0439). \u041f\u0440\u043e\u0432\u0435\u0440\u044f\u0435\u043c: \u0430) \u043a\u043e\u0440\u0440\u0435\u043b\u044f\u0446\u0438\u0438 \u043c\u0435\u0436\u0434\u0443 \u0431\u0443\u0434\u043d\u0438\u043c\u0438\/\u0432\u044b\u0445\u043e\u0434\u043d\u044b\u043c\u0438 \u0434\u043d\u044f\u043c\u0438; \u0431) \u043a\u0430\u043a \u0441\u0432\u044f\u0437\u0430\u043d\u043e \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0441 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043e\u043c. \u0412\u0435\u0440\u043d\u043e \u043b\u0438, \u0447\u0442\u043e \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u044b \u043f\u043e\u043c\u043b\u0430\u0434\u0448\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442 \u0431\u043e\u043b\u044c\u0448\u0435 \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0432 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u0445?\n\n","42d10769":"\u041c\u0435\u0434\u0438\u0430\u043d\u043d\u0430 \u043d\u0430\u043c\u043d\u043e\u0433\u043e \u043c\u0435\u043d\u044c\u0448\u0435 \u0441\u0440\u0435\u0434\u043d\u0435\u0433\u043e. \u0417\u043d\u0430\u0447\u0438\u0442 \u043c\u043e\u0433\u0443\u0442 \u0432\u044b\u0431\u0440\u043e\u0441\u044b \u0441 \u0431\u043e\u043b\u044c\u0448\u0438\u043c\u0438 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f\u043c\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0438 \u0432\u043b\u0438\u044f\u044e\u0442 \u043d\u0430 \u0441\u0440\u0435\u0434\u043d\u0435\u0435 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0435. \u041f\u043e\u043a\u0437\u0430\u0442\u0435\u043b\u0438 \u043f\u043e \u043e\u0431\u044a\u0435\u043c\u0443 \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0432 \u0431\u0443\u0434\u043d\u0438\u0435 \u0438 \u0432\u0432\u044b\u0445\u043e\u0434\u043d\u044b\u0435 \u043f\u043e\u0445\u043e\u0436\u0438. \n\n\u0418\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e, \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0437\u0434\u0435\u0441\u044c \u0438\u0437 \u0440\u0430\u0441\u0447\u0435\u0442\u0430 \u043d\u0430 5 \u0434\u043d\u0435\u0439 \u0431\u0443\u0434\u043d\u0438\u0445 \u0438 2 \u0434\u043d\u0435\u0439 \u0432\u044b\u0445\u043e\u0434\u043d\u044b\u0445? \u0415\u0441\u043b\u0438 \u0442\u0430\u043a, \u0442\u043e \u0432 \u0432\u044b\u0445\u043e\u0434\u043d\u043e\u0439 \u0434\u0435\u043d\u044c \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u0442\u0441\u044f \u0431\u043e\u043b\u044c\u0448\u0435.","4b0bfccf":"### \u041f\u0435\u0440\u0435\u0445\u043e\u0434\u0438\u043c \u043a\u043e \u0432\u0442\u043e\u0440\u043e\u0439 \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u0435.\n\n\u0421\u0442\u043e\u043b\u0431\u0446\u044b: \u0422\u0440\u0430\u0444\u0438\u043a \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u0430 \u0432 \u0431\u0443\u0434\u043d\u0438\u0435 \u0434\u043d\u0438 \u0432 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u0445 \u0438 \u0422\u0440\u0430\u0444\u0438\u043a \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u0430 \u0432 \u0432\u044b\u0445\u043e\u0434\u043d\u044b\u0435 \u0432 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u0445 + \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043d\u0430\u044f \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f (\u0446\u0435\u043b\u0435\u0432\u043e\u0439). \u041f\u0440\u043e\u0432\u0435\u0440\u044f\u0435\u043c: \u0430) \u043a\u043e\u0440\u0440\u0435\u043b\u044f\u0446\u0438\u0438 \u043c\u0435\u0436\u0434\u0443 \u0431\u0443\u0434\u043d\u0438\u043c\u0438\/\u0432\u044b\u0445\u043e\u0434\u043d\u044b\u043c\u0438 \u0434\u043d\u044f\u043c\u0438; \u0431) \u043a\u0430\u043a \u0441\u0432\u044f\u0437\u0430\u043d\u043e \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0441 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043e\u043c. \u0412\u0435\u0440\u043d\u043e \u043b\u0438, \u0447\u0442\u043e \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u044b \u043f\u043e\u043c\u043b\u0430\u0434\u0448\u0435 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u044e\u0442 \u0431\u043e\u043b\u044c\u0448\u0435 \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0432 \u043c\u0435\u0433\u0430\u0431\u0430\u0439\u0442\u0430\u0445?","ab525d36":"\u041f\u043e\u0441\u043c\u043e\u0442\u0440\u0438\u043c \u043d\u0430 \u043f\u0440\u043e\u0446\u0435\u043d\u0442\u043d\u043e\u0435 \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0435 \u043a\u0430\u0436\u0434\u043e\u0439 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430 \u043f\u043e \u0433\u0440\u0443\u043f\u043f\u0430\u043c \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u0430.","b4774706":"\u041f\u043e\u0441\u0442\u0440\u043e\u0438\u043c \u043a\u0440\u043e\u0441\u0441-\u0442\u0430\u0431\u043b\u0438\u0446\u0443","07985f3e":"\u041d\u0435\u043b\u044c\u0437\u044f \u0441\u043a\u0430\u0437\u0430\u0442\u044c, \u0447\u0442\u043e \u0442\u0435 \u043a\u0442\u043e \u0431\u044b\u0432\u0430\u044e\u0442 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u0435 \u043e\u0431\u043b\u0430\u0434\u0430\u044e\u0442 \u0431\u043e\u043b\u0435\u0435 \u0434\u043e\u0440\u043e\u0433\u0438\u043c\u0438 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430\u043c\u0438, \u0430 \u043a\u0442\u043e \u043d\u0435 \u0431\u044b\u0432\u0430\u0435\u0442 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u0435 - \u0431\u043e\u043b\u0435\u0435 \u0434\u0435\u0448\u0435\u0432\u044b\u043c\u0438. \u0421\u0440\u0435\u0434\u0438 \u0442\u0435\u0445 \u043a\u0442\u043e \u0431\u044b\u0432\u0430\u0435\u0442 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u0435 \u0438 \u043d\u0435\u0442, \u043d\u0430\u0438\u0431\u043e\u043b\u044c\u0448\u0435 \u043e\u0431\u043b\u0430\u0434\u0430\u0442\u0435\u043b\u0435\u0439 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 3, \u0438 \u0442\u0430\u043a\u0436\u0435 \u0432 \u043e\u0431\u043e\u0438\u0445 \u0433\u0440\u0443\u043f\u043f\u0430\u0445 \u0435\u0441\u0442\u044c \u043e\u0431\u043b\u0430\u0434\u0430\u0442\u0435\u043b\u0438 \u0433\u0440\u0430\u043d\u0438\u0447\u043d\u044b\u0445 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0439 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u0445 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u043e\u0432 \u043f\u043e \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u0438.","f76d3961":"\u0412 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u044b\u0445 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u043e\u0432 2 \u043f\u0440\u0435\u043e\u0431\u043b\u0430\u0434\u0430\u044e\u0442 \u0442\u0435, \u043a\u0442\u043e \u0431\u044b\u0432\u0430\u044e\u0442 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u0435. \u041f\u043e \u043e\u0441\u0442\u0430\u043b\u044c\u043d\u044b\u043c \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f\u043c \u0438\u0445 \u043c\u0435\u043d\u044c\u0448\u0435 \u0442\u0435\u0445, \u043a\u0442\u043e \u043d\u0435 \u0431\u044b\u0432\u0430\u0435\u0442 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u0435.","aac51c58":"\u0422\u0435\u043f\u0435\u0440\u044c \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u0438\u043c \u043a\u0430\u043a \u043a\u043e\u0440\u0440\u0435\u043b\u0438\u0440\u0443\u044e\u0442 \u043d\u0430\u0448\u0438 \u0434\u0430\u043d\u043d\u044b\u0435 \u043f\u043e\u0441\u043b\u0435 \u0443\u0434\u0430\u043b\u0435\u043d\u0438\u044f \u0432\u044b\u0431\u0440\u0430\u0441\u043e\u0432.","e1e37278":"\u041f\u043e\u0441\u0442\u0440\u043e\u0438\u043c \u043c\u0430\u0442\u0440\u0438\u0446\u0443 \u043a\u043e\u0440\u0440\u0435\u043b\u044f\u0446\u0438\u0439 \u0421\u043f\u0438\u0440\u043c\u0435\u043d\u0430 \u0434\u043b\u044f \u0447\u0438\u0441\u043b\u043e\u0432\u044b\u0445 \u043f\u0440\u0438\u0437\u043d\u0430\u043a\u043e\u0432","1e4a38b9":"\u041a\u043e\u044d\u0444\u0444\u0438\u0446\u0438\u0435\u043d\u0442\u044b \u041a\u0435\u043d\u0434\u0430\u043b\u043b\u0430 \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u044e\u0442 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u043d\u0438\u0436\u0435, \u043d\u043e \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0436\u0434\u0430\u044e\u0442 \u0441\u0432\u044f\u0437\u044c \u043c\u0435\u0436\u0434\u0443 \u043e\u0431\u044a\u0435\u043c\u0430\u043c\u0438 \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0432 \u0431\u0443\u0434\u043d\u0438\u0435 \u0438 \u0432\u044b\u0445\u043e\u0434\u043d\u044b\u0435.","19af4080":"# \u0427\u0430\u0441\u0442\u044c 1\n\n\u041f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u043c, \u0447\u0442\u043e \u043c\u044b \u0440\u0435\u0448\u0430\u0435\u043c \u0437\u0430\u0434\u0430\u0447\u0443 \u043f\u043e\u0441\u0442\u0440\u043e\u0435\u043d\u0438\u044f \u0445\u043e\u0440\u043e\u0448\u0435\u0439 \u043c\u043e\u0434\u0435\u043b\u0438 \u0434\u043b\u044f \u043f\u0440\u0435\u0434\u0441\u043a\u0430\u0437\u0430\u043d\u0438\u044f \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043d\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u044b \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u0430 \u043f\u043e \u044d\u0442\u0438\u043c \u0434\u0430\u043d\u043d\u044b\u043c. \u041f\u0440\u0435\u0436\u0434\u0435 \u0447\u0435\u043c \u0441\u0442\u0440\u043e\u0438\u0442\u044c \u0441\u0430\u043c\u0443 \u043c\u043e\u0434\u0435\u043b\u044c, \u043d\u0430\u043c \u043f\u0440\u0438\u0434\u0451\u0442\u0441\u044f \u043f\u0440\u043e\u0430\u043d\u0430\u043b\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0434\u0430\u043d\u043d\u044b\u0435 \u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0430\u043c\u0438 \u0432\u0438\u0437\u0443\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u0438 \u0438 \u043a\u043e\u0440\u0440\u0435\u043b\u044f\u0446\u0438\u043e\u043d\u043d\u043e\u0433\u043e \u0430\u043d\u0430\u043b\u0438\u0437\u0430 (\u043f\u0440\u043e\u0432\u0435\u0440\u043a\u0438 \u0440\u0430\u0437\u043d\u044b\u0445 \u0433\u0438\u043f\u043e\u0442\u0435\u0437).\n\n## \u0417\u0430\u0434\u0430\u043d\u0438\u0435:\n- \u0437\u0430\u0433\u0440\u0443\u0437\u0438\u0442\u044c \u043d\u0430\u0431\u043e\u0440 \u0434\u0430\u043d\u043d\u044b\u0445 \u0441\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u043d\u043e\u043c\u0435\u0440\u0443 \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u0430;\n- \u0432\u044b\u0431\u0440\u0430\u0442\u044c \u043f\u043e\u0434\u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u043e \u0441\u0442\u043e\u043b\u0431\u0446\u043e\u0432, \u043a\u043e\u0442\u043e\u0440\u044b\u043c \u0431\u0443\u0434\u0435\u0442\u0435 \u0437\u0430\u043d\u0438\u043c\u0430\u0442\u044c\u0441\u044f;\n- \u0441\u0444\u043e\u0440\u043c\u0443\u043b\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0431\u044b\u043b\u043e \u0431\u044b \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e \u043f\u0440\u043e\u0432\u0435\u0440\u0438\u0442\u044c \u043f\u043e \u044d\u0442\u043e\u043c\u0443 \u043f\u043e\u0434\u043c\u043d\u043e\u0436\u0435\u0441\u0442\u0432\u0443 \u0441\u0442\u043e\u043b\u0431\u0446\u043e\u0432.\n\n### \u041f\u0440\u0438\u043c\u0435\u0440. \n\n1. \u0421\u0442\u043e\u043b\u0431\u0446\u044b: **\u0443\u0440\u043e\u0432\u0435\u043d\u044c \u0434\u043e\u0445\u043e\u0434\u0430 \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u0430** \u0438 **\u043d\u0430\u043b\u0438\u0447\u0438\u0435 \u0430\u0432\u0442\u043e\u043c\u043e\u0431\u0438\u043b\u044f**. \u041e\u0431\u0430 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0430\u043b\u044c\u043d\u044b\u0435. \u041f\u0440\u043e\u0432\u0435\u0440\u044f\u0435\u043c \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u0443, \u0441\u0432\u044f\u0437\u0430\u043d\u044b \u043b\u0438 \u043e\u043d\u0438.\n\n2. \u0421\u0442\u043e\u043b\u0431\u0446\u044b: **\u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0432\u0445\u043e\u0434\u044f\u0449\u0438\u0445** \u0438 **\u0438\u0441\u0445\u043e\u0434\u044f\u0449\u0438\u0445 \u0437\u0432\u043e\u043d\u043a\u043e\u0432** (4 \u0441\u0442\u043e\u043b\u0431\u0446\u0430) + **\u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043d\u0430\u044f \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f** (\u0446\u0435\u043b\u0435\u0432\u043e\u0439). \u041f\u0440\u043e\u0432\u0435\u0440\u044f\u0435\u043c: \u0430) \u043a\u043e\u0440\u0440\u0435\u043b\u044f\u0446\u0438\u0438 \u043c\u0435\u0436\u0434\u0443 \u0432\u0445\u043e\u0434\u044f\u0449\u0438\u043c\u0438\/\u0438\u0441\u0445\u043e\u0434\u044f\u0449\u0438\u043c\u0438, \u0440\u0430\u0431\u043e\u0447\u0438\u043c\u0438\/\u0432\u044b\u0445\u043e\u0434\u043d\u044b\u043c\u0438 \u0434\u043d\u044f\u043c\u0438; \u0431) \u043a\u0430\u043a \u0441\u0432\u044f\u0437\u0430\u043d\u043e \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u0437\u0432\u043e\u043d\u043a\u043e\u0432 \u0441 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043e\u043c. \u0412\u0435\u0440\u043d\u043e \u043b\u0438, \u0447\u0442\u043e \u0430\u0431\u043e\u043d\u0435\u043d\u0442\u044b \u043f\u043e\u0441\u0442\u0430\u0440\u0448\u0435 \u0447\u0430\u0449\u0435 \u0440\u0430\u0437\u0433\u043e\u0432\u0430\u0440\u0438\u0432\u0430\u044e\u0442 \u043f\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0443?","d7919d27":"\u041f\u0440\u043e\u0446\u0435\u043d\u0442\u043d\u043e\u0435 \u043e\u0442\u043d\u043e\u0448\u0435\u043d\u0438\u0435 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0439 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430 \u043f\u043e \u043e\u0431\u043e\u0438\u043c \u0444\u0430\u043a\u0442\u0430\u043c \u043f\u043e\u0435\u0437\u0434\u043e\u043a \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433, \u0442\u0430\u043a\u0436\u0435 \u043f\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442, \u0447\u0442\u043e \u0431\u043e\u043b\u0438\u043d\u0441\u0442\u0432\u043e \u043e\u0431\u043b\u0430\u0434\u0430\u0435\u0442 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430\u043c\u0438 3 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438. \u041e\u0434\u043d\u0430\u043a\u043e \u0442\u0430\u043a\u0436\u0435 \u043c\u043e\u0436\u043d\u043e \u043e\u0442\u043c\u0435\u0442\u0438\u0442\u044c, \u0447\u0442\u043e \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0435 \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e \u0441\u0440\u0435\u0434\u0438 \u0442\u0435\u0445, \u043a\u0442\u043e \u0431\u044b\u0432\u0430\u0435\u0442 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u0435, 21,8% \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0442 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u044b 2 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438. \u0410 \u0441\u0440\u0435\u0434\u0438 \u0442\u0435\u0445, \u043a\u0442\u043e \u043d\u0435 \u0431\u044b\u0432\u0430\u0435\u0442 \u0432 \u0440\u043e\u0443\u043c\u0438\u043d\u0433\u0435, 28,3% \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0442 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u044b 4 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438.\n\n\u0422\u0443\u0442 \u043d\u0430\u0448\u0430 \u0433\u0438\u043f\u043e\u0442\u0435\u0437\u0430 \u0442\u0430\u043a\u0436\u0435 \u043e\u043f\u0440\u0435\u0434\u0435\u043b\u0438\u043b\u0430\u0441\u044c \u043d\u0430\u043e\u0431\u043e\u0440\u043e\u0442. \n\u0422\u043e \u0435\u0441\u0442\u044c \u043f\u043e\u0441\u043b\u0435 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u0439 \u0442\u0440\u0435\u0442\u0435\u0439 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u043e\u0432, \u0442\u0435 \u043a\u0442\u043e \u0435\u0437\u0434\u044f\u0442 \u0437\u0430 \u0433\u0440\u0430\u043d\u0438\u0446\u0443 \u0432\u044b\u0431\u0438\u0440\u0430\u044e\u0442 \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u044b \u043f\u043e\u043f\u0440\u043e\u0449\u0435, \u0430 \u0442\u0435 \u043a\u0442\u043e \u043d\u0435 \u0435\u0437\u0434\u044f\u0442 - \u043f\u043e\u0434\u043e\u0440\u043e\u0436\u0435.","f94ff926":"\u041f\u043e\u0441\u043c\u043e\u0442\u0440\u0438\u043c \u043d\u0430 \u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u0441\u0442\u043e\u043b\u0431\u0446\u0430 \u0441 \u0434\u0430\u043d\u043d\u044b\u043c\u0438 \u043f\u043e \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f\u043c \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u0438 \u043c\u043e\u0431\u0438\u043b\u044c\u043d\u043e\u0433\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0430","efd11b4c":"P-\u0437\u043d\u0430\u0447\u0435\u043d\u0438\u044f \u0432 \u043e\u0431\u043e\u0438\u0445 \u0441\u043b\u0443\u0447\u0430\u044f\u0445 \u0434\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u043c\u0430\u043b\u044b, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0443 \u043d\u0430\u0441 \u043d\u0435\u0442 \u043e\u0441\u043d\u043e\u0432\u0430\u043d\u0438\u0439 \u0443\u0442\u0432\u0435\u0440\u0436\u0434\u0430\u0442\u044c, \u0447\u0442\u043e \u043c\u0435\u0436\u0434\u0443 \u043e\u0431\u0457\u0435\u043c\u043e\u043c \u0442\u0440\u0430\u0444\u0438\u043a\u0430 \u0438 \u0432\u043e\u0437\u0440\u0430\u0441\u0442\u043e\u043c \u043d\u0435\u0442 \u0441\u0442\u0430\u0442\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0436\u0434\u0451\u043d\u043d\u043e\u0439 \u0432\u0437\u0430\u0438\u043c\u043e\u0441\u0432\u044f\u0437\u0438.","7840be45":"\u041f\u043e\u043f\u0440\u043e\u0431\u0443\u0435\u043c \u0435\u0449\u0435 \u0442\u0430\u043a \u0432\u0438\u0437\u0443\u0430\u043b\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c:"}}