{"cell_type":{"a48dcb5e":"code","06bb59a3":"code","dc1c78d2":"code","119ef148":"code","bbe59b1b":"code","6d935bb7":"code","b91ba23e":"code","dff707f3":"code","95b4ae59":"code","daac72b6":"code","dae93b75":"code","4e166d68":"code","2bd387f3":"code","fbda4b2e":"code","869c98f1":"code","38051402":"code","8a67af2d":"code","c58df430":"code","65d2138c":"code","45161b23":"code","161166b6":"code","5240121c":"code","764acba2":"code","21933d9d":"code","2e571e77":"code","309f6e8d":"code","12e0e0ed":"code","6b183c74":"code","8a942e84":"markdown","812d9a6e":"markdown","45e269ae":"markdown","8e9ce563":"markdown","a359f457":"markdown","f1d42386":"markdown"},"source":{"a48dcb5e":"import pandas as pd\nimport numpy as np\nimport matplotlib.pyplot as plt\nimport seaborn as sns\n\n%matplotlib inline","06bb59a3":"df = pd.read_csv('..\/input\/titanic-train\/titanic_train.csv')","dc1c78d2":"df.head()","119ef148":"df.info()","bbe59b1b":"plt.figure(figsize=(12,6))\nsns.heatmap(df.isnull(), yticklabels=False, cbar=False, cmap=\"viridis\")","6d935bb7":"sns.set_style('whitegrid')","b91ba23e":"sns.countplot(x='Survived', data=df, hue='Sex')","dff707f3":"sns.countplot(x='Survived', data=df, hue='Pclass')","95b4ae59":"plt.figure(figsize=(12,6))\nsns.boxplot(x='Pclass', y='Age', data=df)","daac72b6":"c1, c2, c3 = df.groupby('Pclass')['Age'].mean()","dae93b75":"def inputAge(df):\n    age = df['Age']\n    Pclass = df['Pclass']\n    \n    mean = {\n        1: int(c1),\n        2: int(c2),\n        3: int(c3)\n    }\n    \n    if pd.isnull(age):\n        return mean[Pclass]\n    else:\n        return age","4e166d68":"df['Age'] = df[['Age', 'Pclass']].apply(inputAge, axis=1)","2bd387f3":"df.drop('Cabin', axis=1, inplace=True)","fbda4b2e":"plt.figure(figsize=(12,6))\nsns.heatmap(df.isnull(), yticklabels=False, cbar=False, cmap=\"viridis\")","869c98f1":"sex = pd.get_dummies(df['Sex'], drop_first=True)","38051402":"embark = pd.get_dummies(df['Embarked'], drop_first=True)","8a67af2d":"df.drop(['PassengerId', 'Name', 'Sex', 'Ticket', 'Embarked'], axis=1, inplace=True)","c58df430":"df = pd.concat([df, sex, embark], axis=1)","65d2138c":"df.head()","45161b23":"from sklearn.linear_model import LogisticRegression","161166b6":"from sklearn.model_selection import train_test_split","5240121c":"x_train, x_test, y_train, y_test = train_test_split(\n    df.drop('Survived', axis=1), \n    df['Survived'], \n    test_size=0.3\n)","764acba2":"lm = LogisticRegression(solver='liblinear')","21933d9d":"x_train.dtypes","2e571e77":"lm.fit(x_train, y_train)","309f6e8d":"predicts = lm.predict(x_test)","12e0e0ed":"from sklearn.metrics import classification_report","6b183c74":"print(classification_report(y_test, predicts))","8a942e84":"## Treinando a m\u00e1quina","812d9a6e":"### Calculando a m\u00e9dia de idade com base na classe","45e269ae":"## Removendo colunas irrelevantes para a an\u00e1lise","8e9ce563":"### Criando fun\u00e7\u00e3o para substituir os dados nulos","a359f457":"## Visualiza\u00e7\u00e3o de dados faltantes","f1d42386":"## Substituindo informa\u00e7\u00f5es qualitativas"}}