{"cell_type":{"3bb9ef63":"code","def558d9":"code","fb5f1e76":"code","6c4b5d1d":"code","7022a485":"code","efc1ce9d":"code","3494520a":"code","59fc5fc5":"code","73a8b64b":"code","b932fc79":"code","feac2e46":"code","6a84c11a":"code","f5378535":"code","9aa6d9e7":"code","fee4285d":"markdown","78966eef":"markdown","185bf496":"markdown"},"source":{"3bb9ef63":"import numpy as np\nimport pandas as pd\nimport matplotlib.pyplot as plt \n%matplotlib inline","def558d9":"raw_data = pd.read_csv(\"..\/input\/train.csv\")\nraw_data.head(5)","fb5f1e76":"data = raw_data.drop(\"label\",axis=1)\nlabel = raw_data[\"label\"]","6c4b5d1d":"image_data = data.iloc[5]\nplt.imshow(image_data.values.reshape(28,28), cmap=\"Greens\")","7022a485":"temp = data.iloc[5]\ntemp = temp\/255\nplt.imshow(temp.values.reshape(28,28), cmap=\"Greens\")","efc1ce9d":"temp1 = data.iloc[5]\ntemp1[temp1>1]=1\nplt.imshow(temp1.values.reshape(28,28), cmap=\"Greens\")","3494520a":"from sklearn.svm import SVC","59fc5fc5":"model = SVC(gamma='scale')","73a8b64b":"data[data>0]=1","b932fc79":"model.fit(data, label)","feac2e46":"test_data = pd.read_csv(\"..\/input\/test.csv\")","6a84c11a":"test_data[test_data>0]=1","f5378535":"prediction = model.predict(test_data)","9aa6d9e7":"results = pd.Series(prediction,name=\"Label\")\nsubmission = pd.concat([pd.Series(range(1,28001),name = \"ImageId\"),results],axis = 1)\n\nsubmission.to_csv(\"results.csv\",index=False)","fee4285d":"# Original Image","78966eef":"# Modified Image 1","185bf496":"# Modified Image 2"}}