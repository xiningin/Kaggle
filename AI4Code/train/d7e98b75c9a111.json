'].unique()","83c5f6f6":"encoder = LabelEncoder()\n\ndata['Inactive_flag'] = encoder.fit_transform(data['Inactive_flag'])","33d3582a":"data","46906f8b":"data['Federation'].unique()","da060bdc":"data.drop('Federation', axis=1, inplace=True)","aab0fd73":"data","1058ca05":"y = data['GM']\nX = data.drop('GM', axis=1)","6d8f6d97":"X","8bb4d6df":"scaler = MinMaxScaler()\n\nX = pd.DataFrame(scaler.fit_transform(X), columns=X.columns)","329fe3bf":"X","6c4e2535":"X_train, X_test, y_train, y_test = train_test_split(X, y, train_size=0.8)","c50c682b":"model = LogisticRegression()\nmodel.fit(X_train, y_train)","77a9f075":"print(f\"Model Accuracy: {model.score(X_test, y_test)}\")","0da65be3":"y_pred = model.predict(X_test)","343cafbc":"print(f\"Model F1 Score: {f1_score(y_test, y_pred)}\")","c2ec8057":"print(f\"Percent Grandmaster: {y_test.sum() \/ len(y)}\")","68b583fa":"## Missing Values","7e958c5e":"## Encoding","400c27e5":"# Preprocessing","00acc89f":"# Training","c1fbee26":"# Getting Started"}}