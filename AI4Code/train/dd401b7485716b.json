n())\ntest_df['Fare'] = test_df['Fare'].fillna(test_df['Fare'].median())\ntest_df['Embarked'] = test_df['Embarked'].fillna(test_df['Embarked'].mode()[0])\ntest_df.isnull().sum()","7dbf5952":"test_df.head()","3ae14261":"plt.subplot(1,2,1)\ntrain_df['Embarked'].value_counts().plot(kind = 'bar', color = 'green')\nplt.subplot(1,2,2)\ntest_df['Embarked'].value_counts().plot(kind = 'bar', color = 'green')","bcd5f0b0":"plt.figure(figsize = (10,5))\nplt.subplot(1,2,1)\nsns.countplot(train_df.Pclass)\nplt.subplot(1,2,2)\nsns.countplot(test_df.Pclass)","0966e2f4":"plt.boxplot(train_df['Age'])","7dc2cc9a":"train_df.info()","79ad17b2":"test_df.info()","5eecd66c":"en = LabelEncoder()\ncat_cols = ['Sex','Embarked']\nfor i in cat_cols:\n    train_df[i] = en.fit_transform(train_df[i])\n    test_df[i] = en.transform(test_df[i])","3aafda98":"train_df.head()","a0f7235e":"test_df.head()","a8e44c11":"x = train_df.drop('Survived', axis = 1)\ny = train_df.Survived","524ee5d5":"x_train, x_test, y_train, y_test = train_test_split(x, y, test_size = 0.2, random_state = 40)","9ee12e01":"model = LogisticRegression(random_state = 0, max_iter = 1000).fit(x_train, y_train)","2a880869":"y_pred = model.predict(x_test)","216900b3":"accuracy_score(y_test, y_pred)","463da261":"cm = confusion_matrix(y_test, y_pred)","78c005fb":"cm","fe78e4c4":"cr = classification_report(y_test, y_pred)\nprint(cr)","0234369c":"submission_preds = model.predict(test_df)","3636bd90":"df = pd.DataFrame({'PassengerId' : test_df_ids.values, \n                  'Survived' : submission_preds})","57fe417e":"df.to_csv('TitanicSubmission.csv', index = False)","f26ac42c":"# TITANIC - MACHINE LEARNING FROM DISASTER","c6c2b6e2":"# Data Cleaning\n","e41110b0":"# Model Building","72c14daa":"# Train Test Split ","31711950":"# Handling Categorical Values","2d2c1c6b":"# Data Visualization","e5c3c856":"# Model Evaluation"}}