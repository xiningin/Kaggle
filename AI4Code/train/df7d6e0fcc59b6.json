{"cell_type":{"09ff3c3a":"code","f823172c":"code","9093eea0":"code","daadf79a":"code","d63e0f8c":"code","9d3b1a54":"code","e37d12ce":"code","1714685b":"code","8265c0e1":"code","24a31e3e":"code","a2c3e5ec":"code","cf315874":"code","3d64369a":"code","afdef9a2":"code","daf81516":"code","1e11cfaf":"code","6f2edb8d":"code","4eea8f48":"code","af0c2310":"code","ed994568":"code","5469795e":"code","d6dae3b8":"code","2aa26ef9":"code","b68d7d1c":"code","f7fdd056":"code","1fee5a4f":"code","03ba5c85":"code","97607b9f":"code","8f0261c5":"code","8b2e77d2":"code","e7800913":"code","27994ddd":"code","e8893f59":"code","031683bd":"code","e3312e8d":"code","962b1703":"code","18d1990f":"code","ed299bc0":"code","510db777":"code","85da1876":"code","0ee4ba3e":"code","d4dd545a":"code","27bfe7da":"code","c5df2b4d":"code","12f1dbe7":"markdown","0b00176d":"markdown","c30e7cc3":"markdown","068163bc":"markdown","6a6d9d0f":"markdown","d0fc795b":"markdown","84a87b0d":"markdown","90b01af5":"markdown"},"source":{"09ff3c3a":"import pandas as pd\nfrom sklearn.preprocessing import LabelEncoder\nimport numpy as np\nimport matplotlib.pyplot as plt\nimport seaborn as sns\nfrom sklearn.model_selection import train_test_split\nfrom sklearn.linear_model import LogisticRegression\nfrom sklearn.neighbors import KNeighborsClassifier\nfrom sklearn.svm import SVC\nfrom sklearn.metrics import accuracy_score","f823172c":"df = pd.read_csv(\"..\/input\/weather-dataset-rattle-package\/weatherAUS.csv\", encoding=\"ISO-8859-1\")","9093eea0":"df","daadf79a":"df.head()","d63e0f8c":"df.tail()","9d3b1a54":"df.shape","e37d12ce":"df.columns","1714685b":"df.nunique()","8265c0e1":"df.info()","24a31e3e":"df.isna().sum() ","a2c3e5ec":"df.columns","cf315874":"df = df[['Location', 'MinTemp', 'MaxTemp', 'Rainfall', 'Evaporation',\n       'Sunshine', 'WindGustDir', 'WindGustSpeed', 'WindDir9am', 'WindDir3pm',\n       'WindSpeed9am', 'WindSpeed3pm', 'Humidity9am', 'Humidity3pm',\n       'Pressure9am', 'Pressure3pm', 'Cloud9am', 'Cloud3pm', 'Temp9am',\n       'Temp3pm', 'RainToday', 'RainTomorrow']]\n","3d64369a":"df = df.fillna(df.mean())\ndf = df.dropna()\ndf.drop_duplicates(inplace = True)","afdef9a2":"print(df.isna().sum())\nprint(df.shape)","daf81516":"df.describe()","1e11cfaf":"n1 = df[(df[\"Location\"]==\"Cairns\")]\nprint(\"Average rainfall in Cairns: {:.2f}\".format(np.mean(n1[\"Rainfall\"])))","6f2edb8d":"n2 = df.sort_values(\"Rainfall\")\nn3 = n2[n2[\"Rainfall\"]>200]\nprint(n3[[\"Location\",\"Rainfall\"]].to_string(index=False))","4eea8f48":"lae=LabelEncoder()\ndf[\"Location\"] = lae.fit_transform(df[\"Location\"].astype(str))\ndf[\"WindGustDir\"] = lae.fit_transform(df[\"WindGustDir\"].astype(str))\ndf[\"WindDir9am\"] = lae.fit_transform(df[\"WindDir9am\"].astype(str))\ndf[\"WindDir3pm\"] = lae.fit_transform(df[\"WindDir3pm\"].astype(str))\ndf[\"RainToday\"] = lae.fit_transform(df[\"RainToday\"].astype(str))\ndf[\"RainTomorrow\"] = lae.fit_transform(df[\"RainTomorrow\"].astype(str))","af0c2310":"df.describe().columns","ed994568":"_ = df.boxplot(column=['MaxTemp'])","5469795e":"df = df[(df[\"MaxTemp\"] > 2) & (df[\"MaxTemp\"]  < 43)]\ndf.shape","d6dae3b8":"_ = df.boxplot(column=['Rainfall'])","2aa26ef9":"df = df[df[\"Rainfall\"]<5]\ndf.shape","b68d7d1c":"_ = df.boxplot(column=['Evaporation'])","f7fdd056":"df = df[df[\"Evaporation\"]<8]\ndf.shape","1fee5a4f":"_ = df.boxplot(column=['WindGustSpeed'])","03ba5c85":"df = df[df[\"WindGustSpeed\"]<78]\ndf.shape","97607b9f":"_ = df.boxplot(column=['WindSpeed9am'])","8f0261c5":"df = df[df[\"WindSpeed9am\"]<41]\ndf.shape","8b2e77d2":"_ = df.boxplot(column=['WindSpeed3pm'])","e7800913":"df = df[df[\"WindSpeed3pm\"]<41]\ndf.shape","27994ddd":"_ = df.boxplot(column=['Pressure9am'])","e8893f59":"df = df[(df[\"Pressure9am\"]>1003) & (df[\"Pressure9am\"]<1035)]\ndf.shape","031683bd":"_ = df.boxplot(column=['Temp9am'])","e3312e8d":"df = df[df[\"Temp9am\"]<34]\ndf.shape","962b1703":"df.describe()","18d1990f":"corelation = df.corr()\nplt.subplots(figsize=(20,15))\n_ = sns.heatmap(corelation,xticklabels=corelation.columns, yticklabels=corelation.columns,annot=True)","ed299bc0":"_ = sns.relplot(x=\"MinTemp\",y=\"MaxTemp\",hue=\"Rainfall\",palette=\"viridis\",data=df,s=100)","510db777":"_ = sns.relplot(x=\"Sunshine\",y=\"Evaporation\",hue=\"Rainfall\",palette=\"viridis\",data=df,s=200)","85da1876":"X=df[['Location', 'MinTemp', 'MaxTemp', 'Rainfall', 'Evaporation', 'Sunshine',\n       'WindGustDir', 'WindGustSpeed', 'WindDir9am', 'WindDir3pm',\n       'WindSpeed9am', 'WindSpeed3pm', 'Humidity9am', 'Humidity3pm',\n       'Pressure9am', 'Pressure3pm', 'Cloud9am', 'Cloud3pm', 'Temp9am',\n       'Temp3pm', 'RainToday']].values\nY=df[['RainTomorrow']].values","0ee4ba3e":"from sklearn.model_selection import train_test_split\nX_train,X_test,Y_train,Y_test=train_test_split(X,Y)","d4dd545a":"lr=LogisticRegression()\nlr.fit(X_train,Y_train)\nY_pred1=lr.predict(X_test)\naccuracy_score(Y_test,Y_pred1)","27bfe7da":"knn=KNeighborsClassifier(n_neighbors=8)\nknn.fit(X_train,Y_train)\nY_pred2=knn.predict(X_test)\naccuracy_score(Y_test,Y_pred2)","c5df2b4d":"svc=SVC()\nsvc.fit(X_train,Y_train)\nY_pred3=svc.predict(X_test)\naccuracy_score(Y_test,Y_pred3)","12f1dbe7":"# Logistic Regression","0b00176d":"# Which place has the rainfall above 200 cm?","c30e7cc3":"# Feature Selection:","068163bc":"# What is the average rainfall in Cairns ?","6a6d9d0f":"# EDA","d0fc795b":"# K-Nearest Neighbors (KNN)","84a87b0d":"# Data Cleaning","90b01af5":"# Support Vector Machine (SVM)"}}