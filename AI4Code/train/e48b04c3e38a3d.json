{"cell_type":{"40795979":"code","e8cc37b0":"code","2b3dcb89":"code","b37fd763":"code","a3d77ab4":"code","1af46359":"code","85f01c3d":"code","529dd6bd":"code","3888ed0b":"code","2ba1c66a":"code","3a405b9d":"code","8d2a4ef0":"code","cf5f80e8":"code","4a2f4117":"markdown","f4e0436c":"markdown"},"source":{"40795979":"import numpy as np # linear algebra\nimport pandas as pd # data processing, CSV file I\/O (e.g. pd.read_csv)\nimport os\nimport seaborn as sns\nimport matplotlib.pyplot as plt\n%matplotlib inline\nimport warnings \nwarnings.filterwarnings('ignore')\n\nfor dirname, _, filenames in os.walk('\/kaggle\/input'):\n    for filename in filenames:\n        print(os.path.join(dirname, filename))","e8cc37b0":"data = pd.read_csv('..\/input\/honey-production\/honeyproduction.csv')\ndata.head()","2b3dcb89":"data[['state', 'totalprod']].groupby('state').mean().round()","b37fd763":"data['year'].nunique()","a3d77ab4":"data[data['totalprod'] == data['totalprod'].max()]","1af46359":"data[['year','yieldpercol']].groupby('year').mean().round()","85f01c3d":"cor = data[['numcol',\n           'yieldpercol',\n           'totalprod',\n           'stocks',\n           'priceperlb',\n           'prodvalue']].corr()\ncor","529dd6bd":"sns.heatmap(cor,annot = True,cmap='viridis',vmin=-1,vmax=1,linewidth =1)","3888ed0b":"sns.pairplot(data.drop('year',axis=1))","2ba1c66a":"g= sns.FacetGrid(data,col='state',col_wrap=10,size=3)\ng = g.map(plt.plot,'year','totalprod')","3a405b9d":"sns.lmplot(x='numcol',y='prodvalue',data=data,hue='year')","8d2a4ef0":"plt.figure(figsize=(15,5))\nsns.pointplot('year','prodvalue',data = data)\n# plt.show()","cf5f80e8":"plt.figure(figsize=(15,7))\nsns.boxplot('year','totalprod',data = data)\nplt.show()","4a2f4117":"**Which state has highest volume in production and in which year?**","f4e0436c":"production value per colony increases every year"}}