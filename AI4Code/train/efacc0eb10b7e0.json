{"cell_type":{"9ee3f4e3":"code","c399f508":"code","e5761c20":"code","6e16461e":"code","6e52ed51":"code","a1e94a39":"code","d14d638a":"code","69118ac9":"code","9a8214e6":"code","9a86cbba":"markdown","3ca70eeb":"markdown","a16df43d":"markdown","30f85ac8":"markdown","dac4628e":"markdown","2076fd4b":"markdown"},"source":{"9ee3f4e3":"from IPython.display import clear_output\n!pip3 install pycaret --user\nclear_output()\n\nimport numpy as np\nimport pandas as pd \nfrom pycaret.datasets import get_data\nfrom pycaret.anomaly import *","c399f508":"TRAIN_PATH = \"..\/input\/titanic\/train.csv\"\nTEST_PATH = \"..\/input\/titanic\/test.csv\"\nTARGET = \"Survived\"","e5761c20":"train = pd.read_csv(TRAIN_PATH)\ntest = pd.read_csv(TEST_PATH)","6e16461e":"setup(\n    data=train,        \n    silent=True\n)","6e52ed51":"knn = create_model('knn')\nknn_df = assign_model(knn)\nknn_df","a1e94a39":"abnormalData = knn_df[knn_df.Anomaly == 1]\nprint(\"abnormalData len = \",len(abnormalData))\nabnormalData","d14d638a":"knn = create_model('knn')\n# plot a model\nplot_model(knn)","69118ac9":"tuned_knn = tune_model(model = 'knn', supervised_target = TARGET)","9a8214e6":"plot_model(tuned_knn)","9a86cbba":"# init pycaret ","3ca70eeb":"# catch outliers","a16df43d":"# optimize outlier ","30f85ac8":"# build model and detect outlier","dac4628e":"# plot outliers","2076fd4b":"# installs and imports"}}