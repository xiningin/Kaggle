['current ver'].mode()[0],inplace=True)","31b17cea":"apps['android ver'].unique()","cc7c7a48":"apps['android ver'].mode()[0]","1f29a175":"apps['android ver'].fillna(apps['android ver'].mode()[0],inplace=True)","abe3a729":"apps.isnull().sum()","ecdbe78a":"apps['rating']=apps['rating'].fillna(apps['rating'].median())","b2a076a1":"apps.isnull().sum()","5382b9fa":"apps.describe()","4d635f7f":"apps.info()","b48e2203":"apps.isnull().sum().sum()","b9c618df":"apps[apps['app'].duplicated()].sort_values('app')","9427139e":"apps[apps['app']=='textPlus: Free Text & Calls']","2fcc2b0b":"apps[apps['app']=='Facebook']","4a01aedd":"clean_apps=apps.drop_duplicates(subset =[\"app\"])","7c5769e8":"clean_apps","f58fd85f":"clean_apps.info()","febf2202":"clean_apps.describe()","f6d762a1":"clean_apps.head()","53dc13f4":"clean_apps['category'].value_counts()","4bfe3bc5":"#Visualize it\nplt.figure(figsize=(15,10))\nsns.countplot(clean_apps['category'],order=clean_apps['category'].value_counts().index)\nplt.xticks(rotation=90)\nplt.show()","980ea32f":"category_installs=clean_apps.groupby('category')['installs'].sum().sort_values(ascending=False)\ncategory_installs","4cca01ff":"plt.figure(figsize=(15,10))\nsns.barplot(category_installs.index,category_installs)\nplt.xticks(rotation=90)\nplt.show()","6cfcf626":"size_installs=clean_apps[clean_apps!= 'Varies with device']","97641ade":"sns.boxplot(size_installs['size'])","8e95d0f3":"plt.figure(figsize=(15,10))\nplt.scatter(size_installs['size'],size_installs['installs'])\nplt.show()","7d98b2b6":"clean_apps.loc[clean_apps[\"rating\"].idxmax()]","327d4d19":"high_ratings=clean_apps[(clean_apps['rating']>clean_apps['rating'].mean())]","d4001409":"high_ratings","80789bba":"plt.figure(figsize=(15,8))\nsns.countplot(high_ratings['category'],order = (high_ratings['category'].value_counts().index))\nplt.xticks(rotation=90)","d050cb48":"clean_apps.loc[clean_apps[\"rating\"].idxmin()]","f74c4d05":"low_ratings=clean_apps[(clean_apps['rating']<=clean_apps['rat