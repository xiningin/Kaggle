{"cell_type":{"dfeb2a1d":"code","4168a107":"code","06edec15":"code","d0912592":"code","8e55b33e":"code","707c54b5":"code","9b0a0faa":"code","31eae87f":"code","4e4be2fd":"code","787dffac":"code","13b13f61":"code","f25d3ea4":"code","c7f6ecd8":"code","93e5b353":"code","a3cd713d":"code","77d2e803":"code","39c60f9f":"markdown","1bb8bc50":"markdown","d1ee4548":"markdown","7ed77253":"markdown","7e49038e":"markdown","c6e35140":"markdown","8fd3f1d7":"markdown","2343f937":"markdown","33f0f606":"markdown","9590f499":"markdown","a3fc17db":"markdown"},"source":{"dfeb2a1d":"from PIL import Image\nimport numpy as np\nfrom IPython.display import Image as imm\nfrom IPython.display import clear_output\nimport time","4168a107":"im2=Image.open('..\/input\/horse-image-part-2\/horse part 2.jfif')","06edec15":"im=Image.open('..\/input\/horse-picture\/horse part1.jfif')","d0912592":"im2","8e55b33e":"im","707c54b5":"im.size","9b0a0faa":"im=im.resize((round(im.size[0]*0.7758620689655172), round(im.size[1]*1.293103448275862)))","31eae87f":"t=np.array(im)","4e4be2fd":"tt=np.array(im2)","787dffac":"t.shape","13b13f61":"x=[]\nfor i in range(225):\n    p=[]\n    for j in range(225):\n        p.append(list((t[i][j]+tt[i][j])\/225))\n    x.append(list(p))","f25d3ea4":"# Create a numpy array of floats to store the average (assume RGB images)\narr=numpy.zeros((225,225,3),numpy.float)\nimlist=[im,im2]\n# Build up average pixel intensities, casting each image as an array of floats\nfor i in imlist:\n    arrs=np.array(i)\n    imarr=np.array(arrs,dtype=numpy.float)\n    arr=arr+imarr\/2\n\n# Round values in array and cast as 8-bit integer\narr=numpy.array(numpy.round(arr),dtype=numpy.uint8)","c7f6ecd8":"out=Image.fromarray(arr,mode=\"RGB\")\n","93e5b353":"out","a3cd713d":"def create_video(im,im2,count=1):\n    x=[np.array(im)]\n    arr=numpy.zeros((225,225,3),numpy.float)\n    imlist=[im,im2]\n    # Build up average pixel intensities, casting each image as an array of floats\n    for i in imlist:\n        arrs=np.array(i)\n        imarr=np.array(arrs,dtype=numpy.float)\n        arr=arr+imarr\/2\n\n    # Round values in array and cast as 8-bit integer\n    arr=numpy.array(numpy.round(arr),dtype=numpy.uint8)\n    out=Image.fromarray(arr,mode=\"RGB\")\n    pm=out.copy()\n    t=arr.copy()\n    if count>1:\n        for i in range(count-1):\n                arr=numpy.zeros((225,225,3),numpy.float)\n                imlist=[im,out]\n                # Build up average pixel intensities, casting each image as an array of floats\n                for i in imlist:\n                    arrs=np.array(i)\n                    imarr=np.array(arrs,dtype=numpy.float)\n                    arr=arr+imarr\/2\n\n                # Round values in array and cast as 8-bit integer\n                arr=numpy.array(numpy.round(arr),dtype=numpy.uint8)\n                x.append(arr)\n                out2=Image.fromarray(arr,mode=\"RGB\")\n                im=out2.copy()\n        x.append(t)\n        for i in range(count-1):\n                arr=numpy.zeros((225,225,3),numpy.float)\n                imlist=[out,im2]\n                # Build up average pixel intensities, casting each image as an array of floats\n                for i in imlist:\n                    arrs=np.array(i)\n                    imarr=np.array(arrs,dtype=numpy.float)\n                    arr=arr+imarr\/2\n\n                # Round values in array and cast as 8-bit integer\n                arr=numpy.array(numpy.round(arr),dtype=numpy.uint8)\n                x.append(arr)\n                out=Image.fromarray(arr,mode=\"RGB\")\n        x.append(np.array(im2))\n    else:\n        x.append(arr)\n        x.append(np.array(im2))\n    \n    return x\n            \n            \n            \n","77d2e803":"x=create_video(im,im2,4)\nfor i in x:\n    \n    clear_output(wait=True)\n    out=Image.fromarray(i,mode=\"RGB\")\n    display(out)\n    time.sleep(0.00000000000000000005)\n","39c60f9f":"# Creating an intermediate image","1bb8bc50":"# Resizing the images","d1ee4548":"![](https:\/\/media3.giphy.com\/media\/2A3DG83yvN8uaBiaNR\/200.gif)","7ed77253":"# Looks so cool :) \n# Well i'm new so would really appreciate some pointers to the topic","7e49038e":"# Creating N Intermediate Images to make it look continous","c6e35140":"# Having a look at the images","8fd3f1d7":"# Fade Away Animation","2343f937":"![](data:image\/jpeg;base64,\/9j\/4AAQSkZJRgABAQAAAQABAAD\/2wCEAAkGBxMTEhUTExMWFhUXFxcXFRcXFxgXFxcXFxcXFxUYFxUYHSggGBolHRUXITEhJSkrLi4uFx8zODMtNygtLisBCgoKDg0OGxAQGi0mHyUvLS0tLS0tLS0tLS8tLS0tLS0tLS0tLS0tLS0tLy0tLS0tLS0tLS0tLS0tLS0tLS0tLf\/AABEIAKgBLAMBIgACEQEDEQH\/xAAcAAABBQEBAQAAAAAAAAAAAAACAQMEBQYABwj\/xABCEAABAwIDBAcFBQYGAgMAAAABAAIDBBEFEiEGMUFREyJhcYGRoQdyscHwMkJSYtEUI4KSovEVM0NTwuGDshZjc\/\/EABoBAAIDAQEAAAAAAAAAAAAAAAECAAMEBQb\/xAAuEQACAgEEAQEGBQUAAAAAAAAAAQIRAwQSITFBUQUTIkJhcTKBkbHRFCNSofD\/2gAMAwEAAhEDEQA\/APQHOTYC4x35pCDz7\/7reZmF0d0nRLmhx3Jwb0bBSYPQ8lw3J9qbc1CyVXQDB9fqnWMQNYVIYxBsKQIauLbo3hK1LYROjuibTpyFvYpGRYtbq\/cx47fRZix73z0QpIeSEN5qxDE1LFbhok0uvjm+GXEv3+38Ay4nDnwRmssi6NJUSBupIHeQFFdjMbRvLvdF\/U2C1ZM0Ifikl+ZXCMp8JMmFi50appdpB92I\/wARt6C6hTbQzHcGt7hc+qyS9pYI9O\/sjTHR5X4o1AYm3vYz7Tmt7yB8Vi5cSmdvkdbsOUeQURwuVln7XXyx\/UvjoH8zNhPjlO37+bsaCfXcoE20zPuxuPeQPhdZ\/o0hjWaftLO+ml9l\/Nl0dHiXfJcnaZx\/02jxJRR7Qu4sb4EhUJjSAKr+t1H+Q\/8ATYfQ11Ji7XkNIyk7tdD2KblusO2QrX4fU54g69juPePq\/iuv7O1cstwn2c\/WYFjqUeh97dEOQFE92ijZ11DJuQ70IRtiHemHzaeKJxI+vkpTJuiOujSZFFdM7fuXCU9qbawe8iOujKodraroKWaW+rWOt7xFm+pCvOlK889rta4UzIRq6WQCw3kN13e9kUbcYtkVSaR55i37rDqSLjM+WpeONh+5i8LMefFNCK1HBGN88zpD7rLRM9TIUe3rwKswtN208cdO3\/xMAf8A1l6shTXrooOFPFHGeWYNu\/8Are7yWCKtmxvg9E2dpLNaANwC0sUYsqrChZuqtWuXSiqRilJWWrL2BCMhOxDRIRyCpsvobY0cd6IAX0Rhg5JLIWShdULmIgnFCDbWp0MSxsRPna3Q3J5AKrJljBXJ0FRb6AypGt1QSVZ+623fqo7nP4uPhp8Fiye0sUerZYtPN\/QtWZWi7iB3m3omZcTjbuu49g+Zsq4Ackluxef1Gonlm5M3Y8MYqiRJizz9hgHvEn0FlCmnndoXkdjbN+Gqfv2JNVR8Rakl4K11KeK4UysHApGsVsfqBtkD9m7EhogrTo12RWpFe9lQcPCD\/DVddGuEaG1MPvGUrsPTT6A8loOjXdGjsRPesy76Qpl1OVqpKcFQ30aXZQyyWZ5lOVe4CMuZp