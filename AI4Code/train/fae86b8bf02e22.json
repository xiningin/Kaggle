{"cell_type":{"2e897a5b":"code","7b523460":"code","0b84846e":"code","181c6327":"code","9e46588c":"code","6ba935dd":"code","4cd30ef8":"code","9fdb65a1":"code","88236b4a":"code","8edd3d15":"code","eb12f1ee":"code","e4f13656":"code","459f9894":"code","23060f9b":"code","cd1d54ce":"code","2a1fcad1":"code","8523cce8":"code","23b78818":"code","c9f85518":"code","9c5aa5a2":"code","2dbdc25b":"markdown","3bc82385":"markdown","50839856":"markdown","0b8ceb1b":"markdown","7b80f2f5":"markdown","5d4869b7":"markdown","5130a069":"markdown","6fc054b3":"markdown","1cf610aa":"markdown","b238f4ed":"markdown","ccd71d49":"markdown","b76fba88":"markdown","3f306092":"markdown","f47440ef":"markdown"},"source":{"2e897a5b":"# This Python 3 environment comes with many helpful analytics libraries installed\n# It is defined by the kaggle\/python docker image: https:\/\/github.com\/kaggle\/docker-python\n# For example, here's several helpful packages to load in \n\nimport numpy as np # linear algebra\nimport pandas as pd # data processing, CSV file I\/O (e.g. pd.read_csv)\nfrom pandas_profiling import ProfileReport \nimport plotly.express as px\n\nimport plotly.graph_objects as go\nfrom plotly.offline import iplot                    # Used for interactive plots\nimport matplotlib.pyplot as plt   \n\n# Input data files are available in the \"..\/input\/\" directory.\n# For example, running this (by clicking run or pressing Shift+Enter) will list the files in the input directory\n\nimport os\nprint(os.listdir(\"..\/input\"))\n\n## Any results you write to the current directory are saved as output.","7b523460":"zomato_data = pd.read_csv(\"..\/input\/zomato-restaurants-data\/zomato.csv\", encoding = \"ISO-8859-1\")","0b84846e":"zomato_data.head()","181c6327":"zomato_data.shape","9e46588c":"zomato_data.info()","6ba935dd":"zomato_data.columns","4cd30ef8":"zomatoPandasProfile = ProfileReport(zomato_data, title = 'Zomato Data: Pandas Profiling Report', html = {'style':{'full_width':True}})","9fdb65a1":"zomatoPandasProfile\n","88236b4a":"zomato_data.head(2)","8edd3d15":"zomato_data['Cuisines'].mode()","eb12f1ee":"zomato_data[zomato_data['Cuisines']==\"North Indian\"]","e4f13656":"zomato_data['Cuisines'].mode()[0]","459f9894":"zomato_data['Cuisines'].fillna(zomato_data['Cuisines'].mode()[0], inplace=True)","23060f9b":"zomato_data.info()","cd1d54ce":"# Plotting thecoordinates of the restaurants on an interactive map \n\nzomato = zomato_data.copy(deep=True)\n\ncolorscale = [\n[0, 'rgb(255, 255, 255)'], \n[0.1, 'rgb(255, 255, 255)'],\n[0.2, 'rgb(212, 28, 28)'], \n[0.3, 'rgb(212, 28, 28)'],\n[0.4, 'rgb(255, 186, 59)'], \n[0.5, 'rgb(255, 186, 59)'],\n[0.6, 'rgb(248, 255, 59)'], \n[0.7, 'rgb(248, 255, 59)'], \n[0.8, 'rgb(0, 227, 30)'], \n[0.9, 'rgb(0, 227, 30)'],\n[1, 'rgb(0, 117, 16)']\n]\n\nfig1 = [dict(\n    type='scattergeo',\n    lon = zomato['Longitude'],\n    lat = zomato['Latitude'],\n    text = zomato['Restaurant Name'], \n    mode = 'markers',\n    marker = dict(\n    color = zomato['Rating color'],\n    cmin = zomato['Aggregate rating'].min(),\n    cmax = zomato['Aggregate rating'].max(),\n    colorscale=colorscale,\n    colorbar = dict(\n                title = 'Rating'\n            )\n        )\n    )]\n\nfig_layout = dict(\n    title = 'Rating of zomato restaurants on the world map'\n)\nfig = go.Figure(data=fig1, layout=fig_layout)\niplot(fig)","2a1fcad1":"zomato_data.groupby(['Cuisines'], as_index=False)['Restaurant ID'].count()","8523cce8":"cuisine_data = zomato_data.groupby(['Cuisines'], as_index=False)['Restaurant ID'].count()\ncuisine_data.columns = ['Cuisines','Number of Resturants']\nTop15= (cuisine_data.sort_values(['Number of Resturants'],ascending=False)).head(15)\nTop15","23b78818":"import seaborn as sns\nimport matplotlib.pyplot as plt","c9f85518":"cuisine_data = zomato_data.groupby(['Cuisines'], as_index=False)['Restaurant ID'].count()\ncuisine_data.columns = ['Cuisines','Number of Resturants']\nTop15= (cuisine_data.sort_values(['Number of Resturants'],ascending=False)).head(15)\nsns.set(rc={'figure.figsize':(11.7,8.27)})\nsns.barplot(Top15['Cuisines'], Top15['Number of Resturants'])\nplt.xlabel('Cuisines', fontsize=20)\nplt.ylabel('Number of Resturants', fontsize=20)\nplt.title('Top 15 cuisines on Zomato', fontsize=20)\nplt.xticks(rotation = 90)\nplt.show()","9c5aa5a2":"# Plotting pie chart with title and float with 1 decimal points\nzomato_data['Restaurant Name'].value_counts().head(20).plot(kind='pie',figsize=(13,13), title=\"Top 15 Restaurants with maximum outlets\", autopct='%1.1f%%')","2dbdc25b":"![](data:image\/jpeg;base64,\/9j\/4AAQSkZJRgABAQAAAQABAAD\/2wCEAAoGBxQUExYUFBMXFxYYGSEZGRkZGSEfGBkcHB8gGSIhIBgfHyoiHRwnHyIcIzQjJysuMTExGCE2OzYwOiowMS4BCwsLDw4PHRERHTMoIigwMDIuLjAwMDAxMDEyMjA1MDIwMDIyMDAwMy4wMDAuMDAwMDAwMDAwMDIwMDAwMDAwMP\/AABEIAKgBLAMBIgACEQEDEQH\/xAAcAAACAgMBAQAAAAAAAAAAAAAFBgQHAAIDAQj\/xABEEAACAQIEAgcFBQUHBAIDAAABAhEDIQAEEjEFQQYTIlFhcYEHMpGhsUJicsHwFCNS0eEzU4KSorLCJGNz8TTSFRaT\/8QAGgEAAgMBAQAAAAAAAAAAAAAAAwQBAgUABv\/EAC8RAAICAQMCBAQGAwEAAAAAAAECABEDEiExBEEyUWFxBRMikSOBobHB8DPR4fH\/2gAMAwEAAhEDEQA\/AGYwoNKaRLPpYiZUkRElrHeCP4dpthZ6Q5mrlVpV3p0zUhqUCJZKsVC\/WAbkoVmN2YwJxPVgnEi1OHVamg7wtVlOkkmQQP3vcS3hfCnxTMM6sj1TUAdiJ2Em8LyBgH4YXRCzfvIPEI8DqJSoVK1U6WqqWRWggGoGCkxc2AvAHbN7mFHP5t6hMmfqe6Y8z8ThspM2ZpijllUk0wlXUNlEqNbESNMBgB9ptjgPxHolWoqXlXUe9pmR4wRcYsj41emNHyl1xOy6gNpnBuBitSDLV\/eFo0RIHcDeRO87csOPRvNUetNMU1RhVhjT7IdkUkgrHLtGO4yIwi8GzdShUDU927JWTBnbyIOx\/rh56M5T9ohtRL0j1kyFJbTAYcp3F7R3RgHV6hsTseI30oUgnipOrK4zDLRp6n6qH7QRQCbB30kkC7QIj1jDNlskqUUCgVDTU0xqjmRJMiREHbceeE7NuaGaq1SOzVdNambiwVbSNWsg6ZGobThy4jCqhsDKljaeze7ecj1I54zwdAvtXMZygkqIAyvSgE\/2bPUpq7H3QiFCVKe6DqK6j8PEjjmOkwNRwrap0gUhTdnBaIBhSpEEnVq+EYJcPoUy1QVeqZazsYUgF2YiBHfYySQScRczXXLViAgCgLoESxWSAsg6mOrVyvIFziDRGwkiga7\/AKe0VMvxbMVqmZoNqSnFfU++kAHVKg7wpUXjkN8d+N8RyqUaVKgwdqCytV9js0okbzbULzbEvi70tNbSNA61KdQFwQ9Ooj+5aQpBDDnY40q8KyvUkrl6pSmyhhqJuwlWUM8MjbW2JgxBhnUPL7Sgu7MmZbjeVPDDl6lYpUZSEX7cntBQBuTsRzk9+FarmWzWaBR5WqRTWZABkKbHbtEk+eC2dbLU3dwaYJMkTqc