p_first=True)\nPc.head()","d301fe8f":"df = pd.concat([df,sex,embark,Pc],axis=1)\ndf.head()","a6edaa19":"df.drop(['Sex','PassengerId','Name','Ticket'],axis=1,inplace=True)","99572096":"df.head()","df357051":"df.drop('Pclass',axis=1,inplace=True)\ndf.head()","a0d0014f":"train=['Age','Q','male','Fare','S','SibSp','Parch',2,3]\nX=df[train]\nY=df['Survived']","69076914":"from sklearn.model_selection import train_test_split","4068ef3e":"X_train,X_test,Y_train,Y_test=train_test_split(X,Y,test_size=0.25,random_state=0)","82fee6ca":"from sklearn.linear_model import LogisticRegression","daebf52a":"lmodel=LogisticRegression()","aced29f8":"lmodel.fit(X_train,Y_train)","a27d2bd8":"prediction=lmodel.predict(X_test)","93d5e4eb":"from sklearn.metrics import classification_report\nfrom sklearn.metrics import confusion_matrix\n\nclassification_report(Y_test,prediction)","1beecad3":"confusion_matrix(Y_test,prediction)","f80e7be4":"from sklearn.metrics import accuracy_score","2915e2cb":"accuracy_score(Y_test,prediction)","b0f0726b":"# Cleaning data\n","c74467f0":"# Train data","acef6754":"# Please upvotes if you find it useful","3de9949d":"# counting surviver","a9728886":"# Arranging data","4b717bb5":"# Check for null value","c449e02f":"# Add column","a5c9e5b9":"# Plot surviver"}}